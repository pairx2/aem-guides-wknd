<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Story Grid"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Grid Common Configs"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <header
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Header"
                                        name="./header"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Description"
                                        name="./description"/>
                                    <agerange
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Age Range Label"
                                        name="./ageRange"/>
                                    <producttype
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Product Type Label"
                                        name="./productType"/>
                                    <storytype
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Story Type Label"
                                        name="./storyType"/>
                                    <storytopic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Story Topic Label"
                                        name="./storyTopic"/>
                                    <seemore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="See More Stories Label"
                                        name="./seeMoreText"/>
                                </items>
                            </columns>
                        </items>
                    </tab1>
                    <tab2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Grid1 Configs"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <block1image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 1 Default Image"
                                        name="./block11Image"/>
                                    <block2image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 2 Default Image"
                                        name="./block12Image"/>
                                    <block3image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 3 Default Image"
                                        name="./block13Image"/>
                                    <block4image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 4 Default Image"
                                        name="./block14Image"/>
                                    <ctaimagev1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image Variant 1"
                                        name="./cta14ImageV1"/>
                                    <ctaquotev1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA Quote Variant 1"
                                        name="./cta14QuoteV1"/>
                                    <ctatextv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA Text Variant 1"
                                        name="./cta14TextV1"/>
                                    <ctalinkv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Target Link Variant 1"
                                        name="./cta14LinkV1"/>
                                </items>
                            </columns>
                        </items>
                    </tab2>
                    <tab3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Grid2 Configs"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <block1image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 1 Default Image"
                                        name="./block21Image"/>
                                    <block2image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 2 Default Image"
                                        name="./block22Image"/>
                                    <block3image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 3 Default Image"
                                        name="./block23Image"/>
                                </items>
                            </columns>
                        </items>
                    </tab3>
                    <tab4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Grid3 Configs"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <block1image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Block 1 Default Image"
                                        name="./block33Image"/>
                                    <ctaimage1v1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image 1 Variant 1"
                                        name="./cta31Image1V1"/>
                                    <cta1quotev1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Quote Variant 1"
                                        name="./cta31QuoteV1"/>
                                    <cta1textv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Text Variant 1"
                                        name="./cta31TextV1"/>
                                    <cta1linkv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA 1 Target Link Variant 1"
                                        name="./cta31LinkV1"/>
                                    <ctaimage1v2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image 1 Variant 2"
                                        name="./cta31Image1V2"/>
                                    <cta1quotev2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Quote Variant 2"
                                        name="./cta31QuoteV2"/>
                                    <cta1textv2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Text Variant 2"
                                        name="./cta31TextV2"/>
                                    <cta1linkv2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA 1 Target Link Variant 2"
                                        name="./cta31LinkV2"/>
                                    <ctaimage1v3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image 1 Variant 2"
                                        name="./cta31Image1V3"/>
                                    <cta1quotev3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Quote Variant 3"
                                        name="./cta31QuoteV3"/>
                                    <cta1textv3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 1 Text Variant 3"
                                        name="./cta31TextV3"/>
                                    <cta1linkv3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA 1 Target Link Variant 3"
                                        name="./cta31LinkV3"/>
                                    <ctaimage2v1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image 2 Variant 1"
                                        name="./cta32Image2V1"/>
                                    <cta2quotev1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 2 Quote Variant 1"
                                        name="./cta32QuoteV1"/>
                                    <cta2textv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 2 Text Variant 1"
                                        name="./cta32TextV1"/>
                                    <cta2linkv1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA 2 Target Link Variant 1"
                                        name="./cta32LinkV1"/>
                                    <ctaimage2v2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA Default Image 2 Variant 2"
                                        name="./cta32Image2V2"/>
                                    <cta2quotev2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 2 Quote Variant 2"
                                        name="./cta32QuoteV2"/>
                                    <cta2textv2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="CTA 2 Text Variant 2"
                                        name="./cta32TextV2"/>
                                    <cta2linkv2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="CTA 2 Target Link Variant 2"
                                        name="./cta32LinkV2"/>
                                </items>
                            </columns>
                        </items>
                    </tab4>
                    <tab5
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Story Popup Configs"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <leftarrowimage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Left Arrow Image"
                                        name="./leftArrowImage"
                                        rootPath="/content/dam/adc/freestyle"/>
                                    <rightarrowimage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Right Arrow Image"
                                        name="./rightArrowImage"
                                        rootPath="/content/dam/adc/freestyle"/>
                                    <learnmore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Learn More Text"
                                        name="./learnMore"/>
                                    <learnmorelink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Learn More Target Link"
                                        name="./targetLink"/>
                                    <agerange
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Age Range Label"
                                        name="./storyPopupAgeRange"/>
                                    <producttype
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Product Type Label"
                                        name="./storyPopupProductType"/>
                                    <storytype
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Story Type Label"
                                        name="./storyPopupStoryType"/>
                                    <storytopic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Story Topic Label"
                                        name="./storyPopuStoryTopic"/>
                                    <sharestorytext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Share Story Footer Label"
                                        name="./storyPopupShareStoryText"/>
                                    <disclaimer
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Disclaimer Text"
                                        name="./disclaimer"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/uiSettings"/>
                                        <htmlRules
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/htmlRules"/>
                                    </disclaimer>
                                    <fbsharelabe
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Facebook Social Media Share Label"
                                        name="./fbShareLabel"/>
                                    <twittersharelabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Twitter Social Media Label"
                                        name="./twitterShareLabel"/>
                                </items>
                            </columns>
                        </items>
                    </tab5>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
