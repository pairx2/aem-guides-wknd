<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <policy_adc_faq_question_answer_page
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ADC Faq Question &amp; Answer Page"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[group:ADC - Container,group:ADC - Content,group:ADC - Form,group:ADC - Search,group:ADC - Commerce]">
                        <jcr:content
                            jcr:mixinTypes="[cq:ReplicationStatus]"
                            jcr:primaryType="nt:unstructured"/>
                    </policy_adc_faq_question_answer_page>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
    <adc jcr:primaryType="nt:unstructured">
        <globals jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <structure jcr:primaryType="nt:unstructured">
                    <genericpage jcr:primaryType="nt:unstructured">
                        <policy_adc_faq_question_answer_page_template
                            jcr:primaryType="nt:unstructured"
                            jcr:title="ADC Faq Question &amp; Answer Page"
                            sling:resourceType="wcm/core/components/policy/policy"
                            appResourcesClientlib="abbott.favicon.butterfly">
                            <jcr:content
                                jcr:mixinTypes="[cq:ReplicationStatus]"
                                jcr:primaryType="nt:unstructured"/>
                        </policy_adc_faq_question_answer_page_template>
                    </genericpage>
                </structure>
            </components>
        </globals>
    </adc>
</jcr:root>
