<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" 
	jcr:title="Account Contact Details"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs">
				<items jcr:primaryType="nt:unstructured">
					<column jcr:primaryType="nt:unstructured"
						jcr:title="Properties"
						sling:resourceType="granite/ui/components/coral/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<heading 
							    jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Heading Text"
								name="./heading"
								maxlength="{Long}50"
								required="{Boolean}true" />
							<communication-type
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Communication Type" 
								maxlength="{Long}50"
								name="./communicationType" 
								required="{Boolean}true" />
							<communication-Pref-link
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
								fieldLabel="Edit Communication Preferences link"
								name="./communicationPrefLink"
								rootPath="/content/adc/freestylelibrede/de/de/" />
							<abort-cta-style 
							    jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/select"
								fieldLabel="Abort CTA Style" 
								name="./abortCtaStyle">
								<datasource jcr:primaryType="nt:unstructured"
									sling:resourceType="abbott-platform/datasource/generic-list"
									path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
							</abort-cta-style >
							<save-cta-style
							    jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/select"
								fieldLabel="Save CTA Style"
								name="./saveCtaStyle">
								<datasource jcr:primaryType="nt:unstructured"
									sling:resourceType="abbott-platform/datasource/generic-list"
									path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
							</save-cta-style>
						</items>
					</column>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>
