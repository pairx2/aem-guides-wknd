<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="My Account Document Upload"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Account Document Upload"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <fixedcolums
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <document-upload
                                jcr:primaryType="nt:unstructured"
                                jcr:title="My Account Document Upload"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
									<heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Heading"
                                        name="./heading"/>
                                    <healthHelpText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Health Help Text"
                                        name="./healthHelpText"/>
									<health-cta-style 
									    jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Health Cta Style" 
										name="./healthStyle">
										<datasource jcr:primaryType="nt:unstructured"
											sling:resourceType="abbott-platform/datasource/generic-list"
											path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
									</health-cta-style>
									<health-cta-text
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Health Cta Text"
	                                    name="./healthCtaText">
	                                </health-cta-text>
									<health-cta-link
		                                    jcr:primaryType="nt:unstructured"
		                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
		                                    fieldLabel="Health CTA Link"
		                                    rootPath="/content/adc/freestylelibrede"
		                                    name="./healthCtaLink">		                               
		                            </health-cta-link>
									<reader-information
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Reader Information"
                                        name="./readerInformation"/>
									<dnd-label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="DND Label"
                                        name="./dndLabel"/>
									<upload-label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Upload Label"
                                        name="./uploadLabel"/>	
									<exemption-help-text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Exemption Help Text"
                                        name="./exemptionHelpText"/>
									<exemption-style 
									    jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Exemption Style" 
										name="./exemptionStyle">
										<datasource jcr:primaryType="nt:unstructured"
											sling:resourceType="abbott-platform/datasource/generic-list"
											path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
									</exemption-style>
									<exemption-cta-text
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Exemption Cta Text"
	                                    name="./exemptionCtaText">
	                                </exemption-cta-text>
									<exemption-cta-link
		                                    jcr:primaryType="nt:unstructured"
		                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
		                                    fieldLabel="Exemption CTA Link"
		                                    rootPath="/content/adc/freestylelibrede"
		                                    name="./exemptionCtaLink">		                               
		                            </exemption-cta-link>	
									<upload-style 
									    jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Upload Style" 
										name="./uploadStyle">
										<datasource jcr:primaryType="nt:unstructured"
											sling:resourceType="abbott-platform/datasource/generic-list"
											path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
									</upload-style>
									<upload-cta-text
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Upload Cta Text"
	                                    name="./uploadCtaText">
	                                </upload-cta-text>
									<upload-cta-link
		                                    jcr:primaryType="nt:unstructured"
		                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
		                                    fieldLabel="Upload CTA Link"
		                                    rootPath="/content/adc/freestylelibrede"
		                                    name="./uploadCtaLink">		                               
		                            </upload-cta-link>	
									<upload-more-style 
									    jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Upload More Style" 
										name="./uploadMoreStyle">
										<datasource jcr:primaryType="nt:unstructured"
											sling:resourceType="abbott-platform/datasource/generic-list"
											path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
									</upload-more-style>
									<upload-more-cta-text
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Upload More Cta Text"
	                                    name="./uploadMoreCtaText">
	                                </upload-more-cta-text>	
									<upload-more-cta-link
		                                    jcr:primaryType="nt:unstructured"
		                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
		                                    fieldLabel="Upload More CTA Link"
		                                    rootPath="/content/adc/freestylelibrede"
		                                    name="./uploadMoreCtaLink">		                               
		                            </upload-more-cta-link>	
									<submit-style 
									    jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Submit Style" 
										name="./submitStyle">
										<datasource jcr:primaryType="nt:unstructured"
											sling:resourceType="abbott-platform/datasource/generic-list"
											path="/apps/adc/freestylelibrede/authoring/datasources/cta-types" />
									</submit-style>	
									<submit-style-cta-text
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Submit Style Cta Text"
	                                    name="./submitStyleCtaText">
	                                </submit-style-cta-text>
									<submit-cta-link
		                                    jcr:primaryType="nt:unstructured"
		                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
		                                    fieldLabel="Submit CTA Link"
		                                    rootPath="/content/adc/freestylelibrede"
		                                    name="./submitCtaLink">		                               
		                            </submit-cta-link>		
                                </items>
                            </document-upload>
                        </items>
                    </properties>
                </items>
            </fixedcolums>
        </items>
    </content>
</jcr:root>
