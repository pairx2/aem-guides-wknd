<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Account_Address Display/Edit"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Heading"
                                name="./heading"/>
                            <termsandconditions
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Return Order T&amp;C"
                                name="./termsAndConditions"/>
                            <returninstructions
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Return Order Instructions"
                                name="./returnInstructions"
                                removeSingleParagraphContainer="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <returntimeline
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="How will your return be processed, how long will it take for a replacement to arrive?"
                                fieldLabel="Return Order Timeline"
                                name="./returnTimeline"/>
                            <returnform
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Return Order Form"
                                name="./returnForm"
                                rootPath="/content/dam"/>
							<returnorderreadertext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Return Order Reader Text"
                                name="./returnOrderReaderText"
                                removeSingleParagraphContainer="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"/>
                        </items>
                    </general>
                    <ctastyles
                        jcr:primaryType="nt:unstructured"
                        jcr:title="CTA Styles"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <continuestyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="&quot;Continue&quot; CTA Style"
                                name="./continueStyle">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                    path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </continuestyle>
                            <downloadstyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="&quot;Download the returns form&quot; CTA style"
                                name="./downloadStyle">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                    path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </downloadstyle>
                            <sendmailstyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="&quot;Send returns slip by e-mail&quot; CTA style"
                                name="./sendMailStyle">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                    path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </sendmailstyle>
                            <cancelstyle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="&quot;Back to the order overview&quot; CTA style"
                                name="./cancelStyle">
                                <datasource
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                    path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </cancelstyle>
                        </items>
                    </ctastyles>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
