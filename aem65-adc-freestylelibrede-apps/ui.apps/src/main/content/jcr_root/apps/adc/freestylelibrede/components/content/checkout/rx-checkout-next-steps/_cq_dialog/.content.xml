<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Rx Checkout Next Steps"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <assistance-wizard
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Assistant Wizard"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
											<enable-assistance-wizard
									            jcr:primaryType="nt:unstructured"
									            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
									            fieldDescription="Enable Assistance Wizard"
									            name="./enableAssistanceWizard"
									            text="Enable Assistance Wizard"
									            uncheckedValue="{Boolean}false"
												value="{Boolean}true"/>
											<enable-assistance-wizard-typehint
	                                            jcr:primaryType="nt:unstructured"
	                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
	                                            name="./enableAssistanceWizard@TypeHint"
												value="Boolean"/>
                                            <important-heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Important Heading"
                                                fieldLabel="Important Heading"
                                                name="./importantHeading"/>
                                            <instruction-message
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Instruction Message"
                                                fieldLabel="Instruction Message"
                                                name="./message"/>
                                            <icon-path
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Icon"
                                                name="./icon">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                                    path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                                            </icon-path>
                                            <download-cta-style
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Download CTA Style"
                                                name="./downloadCtaStyle"
                                                text="Download CTA Style">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="abbott-platform/datasource/generic-list"
                                                    path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                                            </download-cta-style>
											<download-cta-text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Download CTA Text"
                                                fieldLabel="Download CTA Text"
                                                name="./downloadCtaText"/>											
											<download-asset-path
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Download Asset Path"
                                                name="./downloadAssetPath"
                                                rootPath="/content/dam"/>	
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </assistance-wizard>
                    <manual-wizard
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Manual Wizard"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <next-steps-heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Next Steps Heading"
                                                fieldLabel="Next Steps Heading"
                                                name="./nextStepsHeading"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Next Steps Description"
                                                fieldLabel="Next Steps Description"
                                                name="./description"/>												
											<step-list
							                                jcr:primaryType="nt:unstructured"
							                                jcr:title="Step List"
							                                sling:resourceType="granite/ui/components/coral/foundation/container">
							                                <items jcr:primaryType="nt:unstructured">
							                                    <step-item
							                                        jcr:primaryType="nt:unstructured"
							                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
							                                        composite="{Boolean}true">
							                                        <field
							                                            jcr:primaryType="nt:unstructured"
							                                            sling:resourceType="/libs/granite/ui/components/coral/foundation/container"
							                                            name="./nextSteps">
							                                            <items
							                                                jcr:primaryType="nt:unstructured"
							                                                name="./items">
																		<step-heading
						                                                    jcr:primaryType="nt:unstructured"
						                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						                                                    fieldLabel="Step Heading"
						                                                    name="./stepHeading"/>
																		<step-description
						                                                    jcr:primaryType="nt:unstructured"
						                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						                                                    fieldLabel="Step Description"
						                                                    name="./stepDescription"/>
																		<icon-path
								                                            jcr:primaryType="nt:unstructured"
								                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
								                                            fieldLabel="Step Icon"
								                                            name="./stepIcon">
								                                            <datasource
								                                                jcr:primaryType="nt:unstructured"
								                                                sling:resourceType="abbott-platform/datasource/generic-list"
								                                                path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
																		</icon-path>	
						                                            </items>
						                                        </field>
						                                    </step-item>
						                                </items>
						                    </step-list>	
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </manual-wizard>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
