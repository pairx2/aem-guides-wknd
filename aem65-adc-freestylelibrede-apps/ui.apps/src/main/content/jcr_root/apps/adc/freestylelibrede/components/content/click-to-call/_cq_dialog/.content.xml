<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Click To Call"
    jcr:primaryType="nt:unstructured"
    jcr:title="Click To Call"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <fixedcolums
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
						    <click-to-call-header
									            jcr:primaryType="nt:unstructured"
									            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
									            fieldDescription="Enable Click To Call Header"
									            name="./clickToCallHeader"
									            text="Enable Click To Call Header"
									            uncheckedValue="{Boolean}false"
												value="{Boolean}true"/>
					       <click-to-call-header-typehint
	                                            jcr:primaryType="nt:unstructured"
	                                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
	                                            name="./clickToCallHeader@TypeHint"
												value="Boolean"/>
                           <icon-path
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Click To Call Image Icon"
                                            name="./icon">
                                            <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="abbott-platform/datasource/generic-list"
                                                path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </icon-path>
							<online-icon-path
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Click To Call Online Icon"
                                            name="./onlineIcon">
                                            <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="abbott-platform/datasource/generic-list"
                                                path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </online-icon-path>
							<offline-icon-path
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Click To Call Offline Icon"
                                            name="./offlineIcon">
                                            <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="abbott-platform/datasource/generic-list"
                                                path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </offline-icon-path>
							<icon-label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Click To Call Label"
                                                    name="./label"/>
							<serviceText
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
									fieldLabel="Service Text"
									name="./serviceText"/>
							<serviceNumber
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Service Number"
                                                    name="./serviceNumber"/>								
							<servicenoopeninghours
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                displayValue="hh:mm"
								fieldLabel="Service Number Opening Hour"
                                name="./serviceOpeningHour"
                                type="time"/>
                            <servicenoclosinghours
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                displayValue="hh:mm"
                                fieldLabel="Service Number Closing Hour"
                                name="./serviceClosingHour"
                                type="time"/>						
							<help-text
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Help Text"
                                                    name="./helpText"/>
							<cta-styling
					                                jcr:primaryType="nt:unstructured"
					                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
					                                fieldDescription="Choose CTA Styling"
					                                fieldLabel="CTA Styling"
					                                name="./ctaStyling">
					                                <datasource
							                            jcr:primaryType="nt:unstructured"
							                            sling:resourceType="abbott-platform/datasource/generic-list"
							                            path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                           </cta-styling>							
						   <cta-text
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="CTA Text"
                                                    name="./ctaText"/>
						   <cta-email
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="CTA Email"
                                                    name="./ctaEmail"/>								
                        </items>
                    </properties>
                </items>
            </fixedcolums>
        </items>
    </content>
</jcr:root>
