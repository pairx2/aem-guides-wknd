<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Mini Cart Popup"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
							<cartmenuitemlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Cart Menu Item Label"
                                name="./label"/>
							<icon-path
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Cart Menu Item Icon"
                                            name="./icon">
                                            <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="abbott-platform/datasource/generic-list"
                                                path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </icon-path>	
							<cartmenuitemalt
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Cart Menu Item Alt"
                                name="./alt"/>
                            <subtotallabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Sub Total Label"
                                name="./subTotalLabel"/>
                            <hideShowAppDetails
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}true"
                                name="./hideShowAppDetails"
                                text="Check for Hide App Details"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <downloadappimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Download App Image"
                                name="./downloadAppImage"
                                rootPath="/content/dam"
                                validation="cq-dialog-link-validation"/>
                            <downloadapptext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Download App Text;"
                                name="./downloadAppText"/>
                            <learnmorelinktext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Learn More Link Text"
                                name="./learnMoreLinkText"/>
                            <learnmorelinkdestination
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Learn More Link Destination;"
                                name="./learnMoreLinkDestination"
                                rootPath="/content"
                                validation="cq-dialog-link-validation"/>
                            <cartheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Cart Heading"
                                name="./cartHeading"/>
                            <opentab
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Open In Tab"
                                name="./openTab">
                                <items jcr:primaryType="nt:unstructured">
                                    <new-tab
                                        jcr:primaryType="nt:unstructured"
                                        selected="{Boolean}true"
                                        text="New Tab"
                                        value="_blank"/>
                                    <same-tab
                                        jcr:primaryType="nt:unstructured"
                                        text="Same Tab"
                                        value="_self"/>
                                    <modal-tab
                                        jcr:primaryType="nt:unstructured"
                                        text="Modal"
                                        value="modal"/>
                                </items>
                            </opentab>
                            <emptycartmessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Empty Cart Message"
                                name="./emptyCartMessage"/>
						   <shopping-cart-page
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Shopping Cart Page Path;"
                                name="./shoppingcartPage"
                                rootPath="/content"
                                validation="cq-dialog-link-validation"/>
							<checkout-page
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Checkout Page Path"
                                name="./checkoutPage"
                                rootPath="/content/adc/freestylelibrede"
                                validation="cq-dialog-link-validation"/>
                            <login-page
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Login Page Path"
                                    name="./loginPage"
                                    rootPath="/content/adc/freestylelibrede"
                                    validation="cq-dialog-link-validation"/>
                            <miniCartEvent
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Mini cart Event"
                                name="./miniCartEvent"/>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
