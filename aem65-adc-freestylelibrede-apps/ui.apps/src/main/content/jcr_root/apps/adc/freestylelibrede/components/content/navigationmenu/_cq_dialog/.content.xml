<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Navigation Menu"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.commons.editor.dialog.childreneditor.v1,core.wcm.components.tabs.v1.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_tab_v1"
    trackingFeature="core-components:tabs:v1">
    <content
        granite:class="cmp-tabs__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
											<mobileMenuLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Mobile Menu Label"
                                                fieldLabel="Mobile Menu Label"
                                                name="./mobileMenuLabel"/>
                                            <backButtonImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Back Button Image"
                                                fieldLabel="Back Button Image"
                                                name="./backButtonImage"
                                                rootPath="/content/dam"/>
                                            <mobileNavCloseImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Mobile Nav Close Image"
                                                fieldLabel="Mobile Nav Close Image"
                                                name="./mobileNavCloseImage"
                                                rootPath="/content/dam"/>
                                            <mobileHeaderMenuBar
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Mobile Header Menu Bar Image"
                                                fieldLabel="Mobile Header Menu Bar Image"
                                                name="./mobileHeaderMenuBarImage"
                                                rootPath="/content/dam"/>
                                            <arrowUpImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Arrow Up Image"
                                                fieldLabel="Arrow Up Image"
                                                name="./arrowUpImage"
                                                rootPath="/content/dam"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>					
					<propertiesContact
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Side Nav Mobile Contact Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
											<menu-mobile-list
		                                        granite:class="menu-mobile-list-multifield-validation"
		                                        jcr:primaryType="nt:unstructured"
		                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/multifield"
		                                        composite="{Boolean}true"
		                                        fieldDescription="Click On Add Button">
				                                        <field
				                                            jcr:primaryType="nt:unstructured"
				                                            sling:resourceType="/libs/granite/ui/components/coral/foundation/container"
				                                            name="./mobileNavList">
				                                            <items
				                                                jcr:primaryType="nt:unstructured"
				                                                name="./items">
				                                                <side-nav-icon
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
				                                                    fieldLabel="Mobile Side Icon"
				                                                    name="./sideNavIcon"/>
																<side-nav-label
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				                                                    fieldLabel="Mobile Side Nav Label"
				                                                    name="./sideNavLabel"/>	
				                                                <side-nav-alt
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				                                                    fieldLabel="Mobile Side Nav Alt"
				                                                    name="./sideNavImageAlt"/>
																<side-nav-link
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
				                                                    fieldLabel="Side Nav Link"
				                                                    name="./sideNavLink"
																	rootPath="/content/adc"
				                                                    validation="cq-dialog-link-validation"/>       
																<side-nav-link-action
									                                jcr:primaryType="nt:unstructured"
									                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
									                                fieldDescription="Link Action New Tab or Same Tab"
									                                fieldLabel="Link Action"
									                                name="./linkAction">
									                                <items jcr:primaryType="nt:unstructured">
									                                 <left
									                                     jcr:primaryType="nt:unstructured"
									                                     text="Open link in New Tab"
									                                     value="_blank"/>
									                                 <right
									                                    jcr:primaryType="nt:unstructured"
									                                    text="Open link in Same Tab"
									                                    value="_self"/>	
									                                </items>
									                            </side-nav-link-action>																
																<country-selector-xf
																		            jcr:primaryType="nt:unstructured"
																		            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
																		            fieldDescription="Country Selector for Mobile"
																		            name="./countrySelectorXF"
																		            text="Country Selector for Mobile"
																		            uncheckedValue="{Boolean}false"
																					value="{Boolean}true"/>														       
																<side-nav-button-label
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
				                                                    fieldLabel="Mobile Side Nav Button Label"
				                                                    name="./sideNavButtonLabel"/>		
				                                                <button-type
									                                jcr:primaryType="nt:unstructured"
									                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
									                                fieldDescription="Button Type"
									                                fieldLabel="Button Type"
									                                name="./buttonType">
									                                <items jcr:primaryType="nt:unstructured">
									                                 <online-help-button
									                                     jcr:primaryType="nt:unstructured"
									                                     text="Online Help Button"
									                                     value="online-help-btn"/>
									                                 <contact-button
									                                    jcr:primaryType="nt:unstructured"
									                                    text="Contact Button"
									                                    value="contact-btn"/>
																	<glob-button
									                                    jcr:primaryType="nt:unstructured"
									                                    text="Globe Button"
									                                    value="globe-btn"/>																		
									                                </items>
									                            </button-type>
																<button-nav-link
				                                                    jcr:primaryType="nt:unstructured"
				                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
				                                                    fieldLabel="Button Side Nav Link"
				                                                    name="./buttonNavLink"
																	rootPath="/content/adc"
				                                                    validation="cq-dialog-link-validation"/>       
																<button-nav-link-action
									                                jcr:primaryType="nt:unstructured"
									                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
									                                fieldDescription="Button Link Action New Tab or Same Tab"
									                                fieldLabel="Button Link Action"
									                                name="./buttonLinkAction">
									                                <items jcr:primaryType="nt:unstructured">
									                                 <left
									                                     jcr:primaryType="nt:unstructured"
									                                     text="Open link in New Tab"
									                                     value="_blank"/>
									                                 <right
									                                    jcr:primaryType="nt:unstructured"
									                                    text="Open link in Same Tab"
									                                    value="_self"/>	
									                                </items>
									                            </button-nav-link-action>	
																	
				                                            </items>
				                                        </field>
												</menu-mobile-list>														
                                            </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </propertiesContact>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
