<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Panel List"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
						    <panel-type
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Panel Type"
                                fieldLabel="Panel Type"
                                name="./panelType">
                                <items jcr:primaryType="nt:unstructured">
                                 <vertical
                                     jcr:primaryType="nt:unstructured"
                                     text="Vertical Layout Panel"
                                     value="vertical"/>
                                 <horizontal
                                    jcr:primaryType="nt:unstructured"
                                    text="Horizontal Layout Panel"
                                    value="horizontal"/>	
                                </items>
                            </panel-type>
                            <image
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Background Image"
                                    rootPath="/content/dam"
                                    name="./image"/>
							<background-text-color
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Background Color"
                                                name="./bgColor">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
                                                    <blue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Blue"
                                                        value="blue"/>
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Orange"
                                                        value="orange"/>
													<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>
                                                    <grey
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Grey"
                                                        value="#f8f8f8"/>	
                                                </items>
                            </background-text-color>		
							<title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        name="./title"/>
							<heading-defaulttypes
                                jcr:primaryType="nt:unstructured"
					            sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Title Type / Size"
                                name="./headingType">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </heading-defaulttypes>
							<heading-text-color
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Heading Text Color"
                                                name="./headingTextColor">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
                                                    <blue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Blue"
                                                        value="blue"/>
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Orange"
                                                        value="orange"/>
													<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>		
                                                </items>
                            </heading-text-color>
							<subHeading
	                                    jcr:primaryType="nt:unstructured"
	                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                    fieldLabel="Subheading"
	                                    name="./subHeading">                                
	                        </subHeading>
							<sub-heading-defaulttypes
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Sub Heading Type / Size"
                                name="./subHeadingType">
                                <items jcr:primaryType="nt:unstructured">
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="H1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="H2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="H3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="H4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="H5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="H6"
                                        value="h6"/>
                                </items>
                            </sub-heading-defaulttypes>
							<subheading-text-color
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="SubHeading Text Color"
                                                name="./subHeadingTextColor">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
                                                    <blue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Blue"
                                                        value="blue"/>
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Orange"
                                                        value="orange"/>
													<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>		
                                                </items>
                            </subheading-text-color>
							<deep-link-text
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Banner Deep Link Label"
								fieldDescription="Deep Link Label"
								name="./deepLinkText"/>
							<banner-deep-link-target
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
								fieldLabel="Deep Link Target ID"
								fieldDescription="Deep Link Target ID"
								name="./deepLinkTarget"/>
							<banner-deep-link-target-page
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Deep Link Page"
                                    rootPath="/content/adc/freestylelibrede"
                                    name="./deepLink"/>		
                            	
							<items
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Panel List"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <items
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            composite="{Boolean}true"
                                            fieldLabel="Panel List">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                name="./panelList">
                                            <items jcr:primaryType="nt:unstructured">
											    <icon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldLabel="Section Icon"
                                                        name="./icon">
                                                    <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="abbott-platform/datasource/generic-list"
                                                            path="adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                                                </icon>
                                                <section-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Section Title"
                                                        name="./sectionTitle"/>
												<section-text-color
	                                                jcr:primaryType="nt:unstructured"
	                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
	                                                fieldLabel="Title Text Color"
	                                                name="./sectionTitleColor">
	                                                <items jcr:primaryType="nt:unstructured">
	                                                    <black
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Black"
	                                                        value="black"/>
	                                                    <white
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="White"
	                                                        value="white"/>
	                                                    <blue
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Blue"
	                                                        value="blue"/>
	                                                    <orange
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Orange"
	                                                        value="orange"/>
														<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>	
															
	                                                </items>
												</section-text-color>		
												<section-description
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Section Description"
                                                        required="{Boolean}true"
                                                        name="./sectionDescription"/>
												<section-description-color
	                                                jcr:primaryType="nt:unstructured"
	                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
	                                                fieldLabel="Description Text Color"
	                                                name="./sectionDescriptionColor">
	                                                <items jcr:primaryType="nt:unstructured">
	                                                    <black
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Black"
	                                                        value="black"/>
	                                                    <white
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="White"
	                                                        value="white"/>
	                                                    <blue
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Blue"
	                                                        value="blue"/>
	                                                    <orange
	                                                        jcr:primaryType="nt:unstructured"
	                                                        text="Orange"
	                                                        value="orange"/>
														<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>		
	                                                </items>
												</section-description-color>		
												<click-to-call
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Phone Number"
                                                        name="./phoneNumber"/>
												<call-text-color
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Phone Number Text Color"
                                                name="./phoneTextColor">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
                                                    <blue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Blue"
                                                        value="blue"/>
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Orange"
                                                        value="orange"/>
													<red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Red"
                                                        value="red"/>	
                                                </items>
										</call-text-color>		
									
												<ctaLabel
						                                jcr:primaryType="nt:unstructured"
						                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						                                fieldDescription="CTA Label Text"
						                                fieldLabel="CTA Label"
						                                name="./ctaLabel"/>
					                            <ctaLink
					                                jcr:primaryType="nt:unstructured"
					                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
					                                emptyText="Path to target page"
					                                fieldDescription="CTA Link"
					                                fieldLabel="CTA Link"
					                                name="./ctaLink"
					                                rootPath="/content/"/>
												<ctaAction
					                                jcr:primaryType="nt:unstructured"
					                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
					                                fieldDescription="CTA Action New Tab or Same Tab"
					                                fieldLabel="CTA Action"
					                                name="./ctaAction">
					                                <items jcr:primaryType="nt:unstructured">
					                                 <left
					                                     jcr:primaryType="nt:unstructured"
					                                     text="Open link in New Tab"
					                                     value="_blank"/>
					                                 <right
					                                    jcr:primaryType="nt:unstructured"
					                                    text="Open link in Same Tab"
					                                    value="_self"/>	
					                                </items>
					                            </ctaAction>	
												<cta-styling
					                                jcr:primaryType="nt:unstructured"
					                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
					                                fieldDescription="Choose CTA Styling"
					                                fieldLabel="CTA Styling"
					                                name="./ctaStyling">
					                                <datasource
							                            jcr:primaryType="nt:unstructured"
							                            sling:resourceType="abbott-platform/datasource/generic-list"
							                            path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
												</cta-styling>                                               
                                            </items>
                                        </field>
                                    </items>
                                </items>
                            </items>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
