<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Registration"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[contact,insurance,registerSuccess]">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General Info"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items
                            jcr:primaryType="nt:unstructured"
                            sling:hideChildren="[eApply,disable-registration,continueCtastyle,login]">
                            <registrationSubheadingDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Registration Subheading Description"
                                name="./registrationSubheadingDescription"/>
                            <informationText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Information text"
                                name="./informationText"/>
                            <customerNumberRegex
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Customer Number regex"
                                name="./customerNumberRegex"/>
                            <kvnrRegex
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="KVNR Regex"
                                name="./kvnrRegex"/>
                            <enableCaptcha
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                fieldLabel="Enable Captcha (V3)"
                                name="./enableCaptcha"
                                text="Enable Captcha (V3)"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <offlineRegistrationSuccessHeading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Offline Registration Success Heading"
                                name="./offlineRegistrationSuccessHeading"/>
                            <offlineRegistrationSuccessSubheading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Offline Registration Success Subheading"
                                name="./offlineRegistrationSuccessSubheading"/>
                            <accountSubHeadingDescription
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Account SubHeading Description"
                                name="./accountSubHeadingDescription"/>                            
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
