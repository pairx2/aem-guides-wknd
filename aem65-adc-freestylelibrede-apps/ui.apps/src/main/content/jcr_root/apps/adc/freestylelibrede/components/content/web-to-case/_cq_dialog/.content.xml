<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Web To Case "
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Heading"
                                name="./heading"/>
                            <privacyPolicy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Privacy Policy Link"
                                fieldLabel="Privacy Policy Link"
                                name="./privacyPolicy"
                                rootPath="/content/adc/freestylelibrede"/>
                            <salutationID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Salutation ID"
                                name="./salutationID"
                                required="{Boolean}true"/>
                            <firstNameID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="First Name ID"
                                name="./firstNameID"
                                required="{Boolean}true"/>
                            <lastNameID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Last Name ID"
                                name="./lastNameID"
                                required="{Boolean}true"/>
                            <emailID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Email ID"
                                    name="./emailID"
                                    required="{Boolean}true"/>
                            <kvnrID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="KVNR ID"
                                    name="./kvnrID"
                                    required="{Boolean}true"/>
                            <customerID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Customer ID"
                                    name="./customerID"
                                    required="{Boolean}true"/>
                            <zipcodeID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Zip/Postal Code ID"
                                    name="./zipcodeID"
                                    required="{Boolean}true"/>
                            <streetID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Street ID"
                                    name="./streetID"
                                    required="{Boolean}true"/>
                            <cityID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="City ID"
                                    name="./cityID"
                                    required="{Boolean}true"/>
                            <productCategoryID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Product Category ID"
                                name="./productCategoryID"
                                required="{Boolean}true"/>
                            <contactCategoryID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Contact Category ID"
                                name="./contactCategoryID"
                                required="{Boolean}true"/>
                            <contactReasonID
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Contact Reason ID"
                                name="./contactReasonID"
                                required="{Boolean}true"/>
                            <callBackCaseID
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Call back case ID"
                                    name="./callBackCaseID"
                                    required="{Boolean}true"/>
                            <retURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Return URL"
                                name="./retURL"
                                required="{Boolean}true"/>
                            <rootTag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Product Category Root Tag"
                                name="./rootTag"
                                rootPath="/content/cq:tags"
                                required="{Boolean}true"/>
                        </items>
                    </general>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
