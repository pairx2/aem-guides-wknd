<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Wizard Insurance Display/Edit"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <general
                    jcr:primaryType="nt:unstructured"
                    jcr:title="General"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Heading"
                                    name="./heading"/>
                            <info-icon
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Info Icon"
                                    name="./infoIcon">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </info-icon>
                        </items>
                    </column>
                </items>
            </general>
            <no-insurance
                    jcr:primaryType="nt:unstructured"
                    jcr:title="No Insurance Saved"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <no-insurance-heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="No Insurance Heading"
                                    name="./noInsuranceHeading"/>
                            <no-insurance-description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="No Insurance Description"
                                    name="./noInsuranceDescription"/>
                            <no-insurance-icon
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="No Insurance Icon"
                                    name="./noInsuranceIcon">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </no-insurance-icon>
                            <secure-data-message
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Secure Data Message"
                                    name="./secureDataMessage"/>
                            <secure-icon
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Secure Icon"
                                    name="./secureIcon">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/abbott-icons"/>
                            </secure-icon>
							<pdfRootPath
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Sick Fund PDFs"
                                    name="./pdfRootPath"
                                    rootPath="/content/dam/adc"/>
							<contentFragmentRootPath
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Sick Fund Content Fragment Path"
                                    name="./contentFragmentRootPath"
                                    rootPath="/content/dam/adc/fsl"/>	
                        </items>
                    </column>
                </items>
            </no-insurance>
            <redirection
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Redirection"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <redirectPath jcr:primaryType="nt:unstructured"
                                   sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                   fieldLabel="Page the user should be redirected to"
                                   fieldDescription="Page the user should be redirected to when his insurance company does not support RX orders."
                                   name="./redirectPath"
                                   rootPath="/content/adc/fsl"/>
                        </items>
                    </column>
                </items>
            </redirection>
        </items>
    </content>
</jcr:root>
