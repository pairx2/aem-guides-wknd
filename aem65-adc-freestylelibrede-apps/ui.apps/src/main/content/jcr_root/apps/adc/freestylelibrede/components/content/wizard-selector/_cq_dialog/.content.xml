<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Wizard Selector"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <general
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Heading"
                                    name="./heading"/>
                            <publicPrescriptionImage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Public Prescription Image"
                                    name="./publicPrescriptionImage"
                                    rootPath="/content/dam"
                                    validation="cq-dialog-link-validation"/>
                            <publicPrescriptionButtonText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Public Prescription Button Text"
                                    name="./publicPrescriptionButtonText"/>
                            <publicPrescriptionButtonCta
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Public Prescription Button CTA Style"
                                    name="./publicPrescriptionButtonCta"
                                    text="Public Prescription Button CTA Style">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </publicPrescriptionButtonCta>
                            <publicPrescriptionButtonLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Select the checkout page for a public prescription"
                                    fieldLabel="Public Prescription Button Link"
                                    rootPath="/content/adc/freestylelibrede/de/de"
                                    name="./publicPrescriptionButtonLink"/>
                            <privatePrescriptionImage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Private Prescription Image"
                                    name="./privatePrescriptionImage"
                                    rootPath="/content/dam"
                                    validation="cq-dialog-link-validation"/>
                            <privatePrescriptionButtonText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Private Prescription Button Text"
                                    name="./privatePrescriptionButtonText"/>
                            <privatePrescriptionButtonCta
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="Private Prescription Button CTA Style"
                                    name="./privatePrescriptionButtonCta"
                                    text="Private Prescription Button CTA Style">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </privatePrescriptionButtonCta>
                            <privatePrescriptionButtonLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Select the checkout page for a private prescription"
                                    fieldLabel="Private Prescription Button Link"
                                    rootPath="/content/adc/freestylelibrede/de/de"
                                    name="./privatePrescriptionButtonLink"/>
                            <noPrescriptionButtonText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="No Prescription Button Text"
                                    name="./noPrescriptionButtonText"/>
                            <noPrescriptionButtonCta
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldLabel="No Prescription Button CTA Style"
                                    name="./noPrescriptionButtonCta"
                                    text="No Prescription Button CTA Style">
                                <datasource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="abbott-platform/datasource/generic-list"
                                        path="/apps/adc/freestylelibrede/authoring/datasources/cta-types"/>
                            </noPrescriptionButtonCta>
                            <noPrescriptionButtonLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Select the checkout page for no prescription"
                                    fieldLabel="No Prescription Button Link"
                                    rootPath="/content/adc/freestylelibrede/de/de"
                                    name="./noPrescriptionButtonLink"/>
                            <goBackText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Go Back Text"
                                    name="./goBackText"/>
                            <goBackLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Select the page the go back button should go to"
                                    fieldLabel="Go back link"
                                    rootPath="/content/adc/freestylelibrede/de/de"
                                    name="./goBackLink"/>
                            <loginPagePath
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Login page path"
                                    rootPath="/content/adc/freestylelibrede/de/de"
                                    name="./loginPagePath"/>
                        </items>
                    </column>
                </items>
            </general>
        </items>
    </content>
</jcr:root>
