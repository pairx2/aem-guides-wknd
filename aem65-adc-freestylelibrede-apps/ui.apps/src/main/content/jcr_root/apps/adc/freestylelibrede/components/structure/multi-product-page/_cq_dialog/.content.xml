<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <product
                        cq:showOnCreate="{Boolean}true"
                        granite:class="nexgen-multiproduct"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Product"
                        sling:orderBefore="basic"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items
                                    jcr:primaryType="nt:unstructured"
                                    sling:hideChildren="[quantityOrder,common-image]">
                                    <templateType
                                        granite:class="cq-dialog-dropdown-showhide-v2"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:orderBefore="productDescription"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Template type"
                                        name="./templateType">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            cq-dialog-dropdown-showhide-target-v2=".template-type-showhide-target"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <multiproductv1
                                                jcr:primaryType="nt:unstructured"
                                                text="V1"
                                                value="multiproductv1"/>
                                            <multiproductv2
                                                jcr:primaryType="nt:unstructured"
                                                text="V2"
                                                value="multiproductv2"/>
                                        </items>
                                    </templateType>
                                    <layoutType
                                        granite:class="hide template-type-showhide-target"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Layout type"
                                        name="./layoutType">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <reader
                                                jcr:primaryType="nt:unstructured"
                                                text="Reader"
                                                value="reader"/>
                                            <sensor
                                                jcr:primaryType="nt:unstructured"
                                                text="Sensor"
                                                value="sensor"/>
                                        </items>
                                    </layoutType>
                                    <productDescriptionv2
                                        cq:showOnCreate="{Boolean}true"
                                        granite:class="hide template-type-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Product description(v2)"
                                        name="./productDescriptionv2"
                                        required="{Boolean}true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                    </productDescriptionv2>
                                    <productBadgev2
                                        cq:showOnCreate="{Boolean}true"
                                        granite:class="hide template-type-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Product Badge (v2)"
                                        name="./productBadgev2">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                    </productBadgev2>
                                    <productHeadlinev2
                                        cq:showOnCreate="{Boolean}true"
                                        granite:class="hide template-type-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Product Headline (v2)"
                                        name="./productHeadlinev2"
                                        required="{Boolean}true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                    </productHeadlinev2>
                                    <productDescriptionv2
                                        cq:showOnCreate="{Boolean}true"
                                        granite:class="hide template-type-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Product description(v2)"
                                        name="./productDescriptionv2"
                                        required="{Boolean}true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                    </productDescriptionv2>
                                    <selectionSublinev2
                                        cq:showOnCreate="{Boolean}true"
                                        granite:class="hide template-type-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Selection Subline (v2)"
                                        name="./selectionSublinev2"
                                        required="{Boolean}true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="multiproductv2"/>
                                    </selectionSublinev2>
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Product Label"
                                        name="./label"
                                        required="{Boolean}true"
                                        validation="cq-dialog-link-validation"/>
                                    <productList
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Product List">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./productList">
                                            <items
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideChildren="[label]">
                                                <quantityOrder
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Quantity per order descripton"
                                                    maxlength="{Long}500"
                                                    name="./quantityOrder"/>
                                                <variantHeading
                                                    granite:class="hide template-type-showhide-target"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Variant heading"
                                                        name="./variantHeading"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv1"/>
                                                </variantHeading>
                                                <variantDescription
                                                    granite:class="hide template-type-showhide-target"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Variant description"
                                                        name="./variantDescription"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv1"/>
                                                </variantDescription>
                                                <common-image
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Order image"
                                                    name="./commonImage"
                                                    rootPath="/content/dam/adc">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv1"/>
                                                </common-image>
                                                <variantHeadlinev2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Variant Headline (v2)"
                                                    name="./variantHeadlinev2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantHeadlinev2>
                                                <variantDescriptionv2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Variant Description(v2)"
                                                    name="./variantDescriptionv2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantDescriptionv2>
                                                <variantBadgev2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Variant Badge (v2)"
                                                    name="./variantBadgev2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantBadgev2>
                                                <variantPriceSublinev2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Variant Price Subline (v2)"
                                                    name="./variantPriceSublinev2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantPriceSublinev2>
                                                <variantPriceDescriptionv2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                    fieldLabel="Variant Price Description(v2)"
                                                    name="./variantPriceDescriptionv2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantPriceDescriptionv2>
                                                <variantButtonLabelv2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Variant Button Label(v2)"
                                                    name="./variantButtonLabelv2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantButtonLabelv2>
                                                <variantButtonUrlv2
                                                    cq:showOnCreate="{Boolean}true"
                                                    granite:class="hide template-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Variant Button URL (v2)"
                                                    name="./variantButtonUrlv2"
                                                    required="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="multiproductv2"/>
                                                </variantButtonUrlv2>
                                            </items>
                                        </field>
                                    </productList>
                                </items>
                            </column>
                        </items>
                    </product>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
