<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <meta-tags
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Meta Tags"
                        sling:orderBefore="seoProperties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <items
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Meta Tags">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./metaTagList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <meta-name
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Meta Tag name"
                                                    name="./metaName"/>
                                                <meta-value
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Meta Tag Value"
                                                    name="./metaValue"/>
                                            </items>
                                        </field>
                                    </items>
                                </items>
                            </column>
                        </items>
                    </meta-tags>
                    <pageProperties
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                    <siteProperties
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Site Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <experienceFragment
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"/>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </siteProperties>
                    <monitoring
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Monitoring"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <section
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="New Relic"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <newRelicScript
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        emptyOption="{Boolean}true"
                                                        name="./newRelicScript">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <nonprod
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Non-Production"
                                                                value="newrelic-monitoring-nonprod"/>
                                                            <prod
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Production"
                                                                value="newrelic-monitoring-prod"/>
                                                        </items>
                                                    </newRelicScript>
                                                </items>
                                            </section>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </monitoring>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
