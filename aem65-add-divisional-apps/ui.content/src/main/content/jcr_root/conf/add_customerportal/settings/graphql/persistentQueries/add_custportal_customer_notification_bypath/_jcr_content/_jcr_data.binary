query getCustomerNotificationsByPath($notificationPath: String!) {
    customerNotifications: customerNotificationsByPath (_path: $notificationPath){ 
        item{
          _path
          notificationName
          notificationTitle
          notificationType
          notificationPublishDate
          subject 
          relevantProducts 
          notificationDetails {
              html
          }
        } 
    } 
}

