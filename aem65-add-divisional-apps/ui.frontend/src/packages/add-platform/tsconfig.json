{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "es6",
    "allowJs": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./bin",
    "jsx": "react",
    "composite": true
  }
}