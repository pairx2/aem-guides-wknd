<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Banner Image"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[bannerimage.authoring.dialog]"
    helpPath="/content/documentation/abbott-documenation/banner-image.html?wcmmode=disabled">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <variation
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variation"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <banner-type
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select banner type"
                                                fieldLabel="Banner Type"
                                                name="./bannerType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <as-banner-image
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott/Similac Banner Image"
                                                        value="as-banner-img"/>
                                                    <gs-banner-image
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Glucerna Banner Image"
                                                        value="gs-banner-img"/>
                                                </items>
                                            </banner-type>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </variation>
                    <image
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Image"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        id="as-banner-img"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <deskTopImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure desktop image"
                                                fieldLabel="Desktop Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./deskTopImageRef"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./deskTopImg"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure tab image"
                                                fieldLabel="Tab Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./tabImageRef"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./tabImg"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure mobile image"
                                                fieldLabel="Mobile Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./mobileImageRef"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./mobileImg"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </image>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        id="as-banner-img">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <altText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                fieldLabel="Alt Text"
                                                name="./imgAltText"/>
                                            <imageLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="A link to navigate on click of Image"
                                                fieldLabel="Image Link"
                                                name="./imageLink"
                                                rootPath="/content"/>
                                            <target
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Please check to open the link in new tab"
                                                name="./imgLinkTarget"
                                                text="Open in New Tab"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <mobile
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Mobile"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Mobile Text"
                                                name="./mobileText"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/abbott/components/plugins/rte/rtePlugins"/>
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/abbott/components/plugins/rte/uiSettings"/>
                                            </text>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </mobile>
                    <gs-properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        id="gs-banner-img">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <image-heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Image Group"/>
                                            <image-group
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <image
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                        allowUpload="{Boolean}false"
                                                        autoStart="{Boolean}false"
                                                        class="cq-droptarget"
                                                        fieldDescription="Configure desktop image"
                                                        fieldLabel="Image"
                                                        fileNameParameter="./gsfileName"
                                                        fileReferenceParameter="./gsImageRef"
                                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                                                        multiple="{Boolean}false"
                                                        name="./gsImg"
                                                        title="Upload Image Asset"
                                                        uploadUrl="${suffix.path}"
                                                        useHTML5="{Boolean}true"/>
                                                    <altText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                        fieldLabel="Alt Text"
                                                        name="./gsImgAltText"/>
                                                </items>
                                            </image-group>
                                            <title-heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Title Group"/>
                                            <title-group
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <titleText1
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldDescription="Configure title text1"
                                                        fieldLabel="Title Text1"
                                                        name="./gsTitleText1"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/uiSettings"/>
                                                    </titleText1>
                                                    <titleText2
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldDescription="Configure title text2"
                                                        fieldLabel="Title Text2"
                                                        name="./gsTitleText2"
														useFixedInlineToolbar="{Boolean}true">
														 <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/uiSettings"/>
                                                    </titleText2>
                                                    <color
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                        fieldDescription="Configure color"
                                                        fieldLabel="Color"
                                                        name="./gsTitleColor"/>
                                                </items>
                                            </title-group>
                                            <text-heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Text Group"/>
                                            <text-group
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <text
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldDescription="Configure text"
                                                        fieldLabel="Text"
                                                        name="./gsText"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/abbott/components/plugins/rte/uiSettings"/>
                                                    </text>
                                                </items>
                                            </text-group>
                                            <button-heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Button Group"/>
                                            <button-group
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <Annonymous-Label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Configure Annonymous user button label"
                                                        fieldLabel="Annonymous user Label"
                                                        name="./annonymousButtonLabel"/>
                                                    <Loggedin-Label
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Configure Logged in user button label"
                                                        fieldLabel="Logged in user Label"
                                                        name="./loggedinButtonLabel"/>
                                                    <Link
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Configure button link"
                                                        fieldLabel="Link"
                                                        name="./buttonLink"
                                                        rootPath="/content"/>
                                                </items>
                                            </button-group>
                                            <note-heading
                                                granite:class="coral-Heading coral-Heading--4"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                level="{Long}4"
                                                text="Note Group"/>
                                            <note-group
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <noteText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                        fieldDescription="Configure note text"
                                                        fieldLabel="Text"
                                                        name="./gsNoteText"/>
                                                    <noteTextColor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                        fieldDescription="Configure note text color"
                                                        fieldLabel="Color"
                                                        name="./gsNoteTextColor"/>
                                                </items>
                                            </note-group>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </gs-properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
