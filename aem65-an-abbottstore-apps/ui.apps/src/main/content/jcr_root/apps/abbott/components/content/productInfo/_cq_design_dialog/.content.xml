<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Product Info"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <flavors
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure flavors label"
                        fieldLabel="Flavors Label"
                        name="./flavors"/>
                    <size
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure size label"
                        fieldLabel="Size Label"
                        name="./sizeLabel"/>
                    <schedule
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure schedule label"
                        fieldLabel="Schedule Label"
                        name="./schedule"/>
                    <quantity
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure quantity label"
                        fieldLabel="Quantity Label"
                        name="./quantity"/>
                    <deliveryweeks
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure delivery weeks label"
                        fieldLabel="Delivery Weeks Label"
                        name="./deliveryWeeks"/>
                    <schedulenow
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure schedule now label"
                        fieldLabel="Schedule Now"
                        name="./scheduleNow"/>
                    <onetimepurchase
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure one time purchase label"
                        fieldLabel="One Time Purchase Label"
                        name="./oneTimePurchase"/>
                    <addtocart
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure add to cart label"
                        fieldLabel="Add To Cart Label"
                        name="./addToCart"/>
                    <confirm
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure confirm label"
                        fieldLabel="Confirm Label"
                        name="./confirm"/>
                    <errormessage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure error message label"
                        fieldLabel="Error Message Label"
                        name="./errorMessage"/>
					<backOrder
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Configure back order label"
						fieldLabel="Back Order Label"
						name="./backOrder"/>
					<instock
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
						fieldDescription="Configure instock label"
						fieldLabel="Instock Label"
						name="./inStock"/>
					<outofstock
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure outofstock label"
                        fieldLabel="out of stock Label"
                        name="./outOfStock"/>
					<regularPrice
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure regular price label"
                        fieldLabel="Regular Price Label"
                        name="./regularPrice"/>
					<subscribeNowText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure subscribe button label"
                        fieldLabel="Subscribe Now Label"
                        name="./subscribeLabel"/>
                    <loggedOutText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Configure introductory button label"
                        fieldLabel="Introductory Button Label"
                        name="./loggedOutText"/>
                    <glucernaCtaUrl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Configure  glucerna Cta  Url"
                        fieldLabel="Glucerna CTA URL"
                        name="./glucernanCtaUrl"
                        rootPath="/content"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
