<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <server
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
        emptyText="http://magento-server/path"
        fieldLabel="Server"
        name="server"
        readonly="true"
        value="http://www.magento-tadigital.com"/>
    <username
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
        fieldLabel="Username"
        name="username"
        readonly="true"/>
    <password
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/form/password"
        fieldLabel="Password"
        name="password"
        readonly="true"/>
    <store
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/form/select"
        emptyText="Select"
        fieldDescription="Select  the store "
        fieldLabel="Store"
        name="store"
        readonly="true"
		required="{Boolean}true">
        <datasource
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/bin/abbott/magento/storemasterdata"/>
    </store>
</jcr:root>
