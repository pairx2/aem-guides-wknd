<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration,cq.siteadmin.admin.properties,abbott.authoring.validation]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v2"
    mode="edit"
    trackingFeature="core-components:page:v2">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <seoProperties
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="SEO Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <general
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="General Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <section
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <keywords
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                        fieldLabel="SEO Keywords"
                                                                        name="./keywords"/>
                                                                </items>
                                                            </section>
                                                        </items>
                                                    </column>
                                                </items>
                                                <parentConfig
                                                    jcr:primaryType="nt:unstructured"
                                                    active="{Boolean}true"/>
                                            </general>
                                            <metadata
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Metadata Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <section
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <robots
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        fieldDescription="Refer https://moz.com/learn/seo/robots-meta-directives"
                                                                        fieldLabel="Robots"
                                                                        multiple="{Boolean}true"
                                                                        name="./robots">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <noindex
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Index"
                                                                                value="noindex"/>
                                                                            <nofollow
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Follow"
                                                                                value="nofollow"/>
                                                                            <noarchive
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Archive"
                                                                                value="noarchive"/>
                                                                            <nosnippet
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Snippet"
                                                                                value="nosnippet"/>
                                                                            <noimageindex
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Image Index"
                                                                                value="noimageindex"/>
                                                                            <notranslate
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="No Translate"
                                                                                value="notranslate"/>
                                                                            <index
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Index"
                                                                                value="index"/>
                                                                            <follow
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Follow"
                                                                                value="follow"/>
                                                                        </items>
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="robots"/>
                                                                    </robots>
                                                                    <canonicalUrl
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                        fieldDescription="Helps prevent duplicate content issues"
                                                                        fieldLabel="Canonical URL"
                                                                        name="./canonicalUrl"
                                                                        rootPath="/content">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="canonicalUrl"/>
                                                                    </canonicalUrl>
                                                                    <ogTitle
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Open Graph Title"
                                                                        name="./ogTitle">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="ogTitle"/>
                                                                    </ogTitle>
                                                                    <ogDescription
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Open Graph Description"
                                                                        name="./ogDescription">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="ogDescription"/>
                                                                    </ogDescription>
                                                                    <ogType
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        fieldLabel="Open Graph Type"
                                                                        name="./ogType">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <website
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="website"
                                                                                value="website"/>
                                                                        </items>
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="ogType"/>
                                                                    </ogType>
                                                                    <ogImage
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldLabel="Open Graph Image"
                                                                        name="./ogImage"
                                                                        rootPath="/content/dam">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="ogImage"/>
                                                                    </ogImage>
                                                                </items>
                                                            </section>
                                                        </items>
                                                    </column>
                                                </items>
                                                <parentConfig
                                                    jcr:primaryType="nt:unstructured"
                                                    active="{Boolean}true"/>
                                            </metadata>
                                            <sitemap
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Sitemap"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <field
                                                                cq:showOnCreate="{Boolean}true"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <hideInSitemap
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./hideInSitemap"
                                                                        renderReadOnly="{Boolean}true"
                                                                        text="Hide in Sitemap"
                                                                        value="true">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="hideInSitemap"/>
                                                                    </hideInSitemap>
                                                                    <changeFrequency
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        fieldLabel="Change Frequency"
                                                                        name="./changeFrequency">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <always
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Always"
                                                                                value="always"/>
                                                                            <hourly
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Hourly"
                                                                                value="hourly"/>
                                                                            <daily
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Daily"
                                                                                value="daily"/>
                                                                            <weekly
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Weekly"
                                                                                value="weekly"/>
                                                                            <monthly
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Monthly"
                                                                                value="monthly"/>
                                                                            <yearly
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Yearly"
                                                                                value="yearly"/>
                                                                            <never
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Never"
                                                                                value="never"/>
                                                                        </items>
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="changeFrequency"/>
                                                                    </changeFrequency>
                                                                    <priority
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                        fieldLabel="Priority"
                                                                        max="1"
                                                                        min="0"
                                                                        name="./priority"
                                                                        step="{Double}0.1">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="priority"/>
                                                                    </priority>
                                                                    <rootSitePath
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                        fieldLabel="Root Site Path"
                                                                        name="./rootSitePath"
                                                                        rootPath="/content">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="rootSitePath"/>
                                                                    </rootSitePath>
                                                                    <includeInHrefLang
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        name="./includeInHrefLang"
                                                                        renderReadOnly="{Boolean}true"
                                                                        text="Include in Href Lang"
                                                                        value="true">
                                                                        <granite:data
                                                                            jcr:primaryType="nt:unstructured"
                                                                            cq-msm-lockable="includeInHrefLang"/>
                                                                    </includeInHrefLang>
                                                                    <assetsPath
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        composite="{Boolean}true"
                                                                        fieldDescription="Add DAM asset folder path"
                                                                        fieldLabel="DAM Asset Path">
                                                                        <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./assetsPath">
                                                                            <layout
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                                                margin="{Boolean}false"/>
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <symbol
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldDescription="Alias name for folder"
                                                                                    fieldLabel="Folder Name"
                                                                                    name="./symbol">
                                                                                    <granite:data
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        cq-msm-lockable="symbol"/>
                                                                                </symbol>
                                                                                <assetSectionPath
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="dam/cfm/components/cfpicker"
                                                                                    emptyText="Enter or select content fragment"
                                                                                    fieldDescription="Please choose a fragment"
                                                                                    fieldLabel="Please choose a DAM path"
                                                                                    name="./assetSectionPath"
                                                                                    pickerTitle="Select Content Fragment"
                                                                                    rootPath="/content/dam">
                                                                                    <granite:data
                                                                                        jcr:primaryType="nt:unstructured"
                                                                                        cq-msm-lockable="assetSectionPath"/>
                                                                                </assetSectionPath>
                                                                            </items>
                                                                        </field>
                                                                    </assetsPath>
                                                                </items>
                                                            </field>
                                                        </items>
                                                    </column>
                                                </items>
                                                <parentConfig
                                                    jcr:primaryType="nt:unstructured"
                                                    active="{Boolean}true"/>
                                            </sitemap>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </seoProperties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
