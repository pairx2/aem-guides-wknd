<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Cart"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <cart
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Cart"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <carttitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Title"
                                                fieldLabel="Cart Title"
                                                name="./cartTitle"/>
                                            <closelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Close Label"
                                                fieldLabel="Cart Close Label"
                                                name="./closeLabel"/>
                                            <emptylabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Empty Label"
                                                fieldLabel="Cart Empty Label"
                                                name="./emptyLabel"/>
                                            <emptybuttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter empty cart Button Label"
                                                fieldLabel="Empty Cart Button Label"
                                                name="./emptyButtonLabel"/>
                                            <emptybuttonURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Enter empty cart Button URL"
                                                fieldLabel="Empty Cart Button URL"
                                                name="./emptyButtonURL"
                                                rootPath="/content/an"/>
                                            <quantitylabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Quantity Label"
                                                fieldLabel="Cart Quantiity Label"
                                                name="./quantityLabel"/>
                                            <totallabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Total Label"
                                                fieldLabel="Cart Total Label"
                                                name="./totalLabel"/>
                                            <editLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Edit Label"
                                                fieldLabel="View/Edit Cart Label"
                                                name="./editLabel"/>
                                            <cartURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the view/edit url"
                                                fieldLabel="Cart URL"
                                                name="./cartURL"/>
                                            <buttonlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the checkout Label"
                                                fieldLabel="Checkout Label"
                                                name="./buttonLabel"/>
                                            <checkoutURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the checkout url"
                                                fieldLabel="Checkout URL"
                                                name="./checkoutURL"/>
                                            <subscriptionLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Cart Subscription Label"
                                                fieldLabel="Cart Subscription Label"
                                                name="./subscriptionLabel"/>
                                            <subtotalLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the sub total Label"
                                                fieldLabel="SubTotal Label"
                                                name="./subTotalLabel"/>
                                            <discountLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the discount Label"
                                                fieldLabel="Discount Label"
                                                name="./discountLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </cart>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
