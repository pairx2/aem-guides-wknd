<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Similac Footer"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.list.v2.editor]"
    helpPath="/content/documentation/abbott-documenation/abbott-list.html?wcmmode=disabled">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <list
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Footer Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <abbottLogo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the Abbott Logo URL"
                                                fieldLabel="Abbott Logo URL"
                                                name="./abbottLogo"
                                                rootPath="/content/dam"/>
                                            <abbottLogoURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the Abbott Logo URL"
                                                fieldLabel="Abbott Logo URL"
                                                name="./abbottLogoURL"/>
                                            <abbottLogoAlttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the alt text for Abbott Logo"
                                                fieldLabel="Abbott Logo Alt text"
                                                name="./abbottLogoAlttext"/>
                                            <copyrightsText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the copyrights text"
                                                fieldLabel="Copyrights Text"
                                                name="./copyrightsText"/>
											<hidefooter
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
												fieldDescription="Hide Footer"
												name="./hidefooter"
												text="Hide Footer"
												value="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </list>
                    <socialIcons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Icons"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <socialiconsHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the social icons text"
                                                fieldLabel="Social Icons Text"
                                                name="./socialiconsHeader"/>
                                            <instagramIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the Instagram Icon URL"
                                                fieldLabel="Instagram Icon"
                                                name="./instagramIcon"
                                                rootPath="/content/dam"/>
                                            <instagramAlttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the instagram alt text"
                                                fieldLabel="Instagram Alt Text"
                                                name="./instagramAlttext"/>
                                            <instagramURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Instagram URL"
                                                fieldLabel="Instagram URL"
                                                name="./instagramURL"/>
                                            <youtubeIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the Youtube Icon URL"
                                                fieldLabel="Youtube Icon"
                                                name="./youtubeIcon"
                                                rootPath="/content/dam"/>
                                            <youtubeAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Youtube alt text"
                                                fieldLabel="Youtube Alt Text"
                                                name="./youtubeAltText"/>
                                            <youtubeURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Youtube URL"
                                                fieldLabel="Youtube URL"
                                                name="./youtubeURL"/>
                                            <facebookIcon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the Facebook Icon URL"
                                                fieldLabel="Facebook Icon"
                                                name="./facebookIcon"
                                                rootPath="/content/dam"/>
                                            <facebookAltText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Facebook alt text"
                                                fieldLabel="Facebook Alt Text"
                                                name="./facebookAltText"/>
                                            <facebookURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Facebook URL"
                                                fieldLabel="Facebook URL"
                                                name="./facebookURL"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </socialIcons>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/an/similac/components/common/dialogs/analytics"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
