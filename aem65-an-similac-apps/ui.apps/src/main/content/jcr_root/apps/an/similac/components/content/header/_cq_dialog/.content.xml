<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="/content/documentation/abbott-documenation/navigation-links.html?wcmmode=disabled">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <searchModal
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Search Modal"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <searchPlaceholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Search Placeholder Text"
                                        name="./searchPlaceholderText"/>
                                    <searchLandingURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Search Landing URL"
                                        fieldLabel="Search Landing URL"
                                        name="./searchLandingURL"/>
                                    <checkoutheader
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check this for Checkout Header"
                                        name="./checkoutHeader"
                                        text="Checkout Header"
                                        value="{Boolean}true"/>
                                    <disruptor
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check this to enable disruptor"
                                        name="./enableDisruptor"
                                        text="Enable Disruptor"
                                        value="{Boolean}true"/>
                                    <pwaheader
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check this for PWA Header"
                                        name="./pwaHeader"
                                        text="PWA Header"
                                        value="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </searchModal>
                    <logoutConfiguration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Logout Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <logoutURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout url"
                                        fieldLabel="Logout URL"
                                        name="./logoutURL"/>
                                    <renewURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the renew url"
                                        fieldLabel="Renew URL"
                                        name="./renewURL"/>
                                    <idleTimeout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the idle timeout"
                                        fieldLabel="Idle Timeout"
                                        name="./idleTimeout"/>
                                    <logoutPopupDuration
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout popup duration"
                                        fieldLabel="Logout Popup Duration"
                                        name="./logoutPopupDuration"/>
                                    <autoRenewal
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the auto renewal duration"
                                        fieldLabel="Auto Renewal"
                                        name="./autoRenewal"/>
                                    <logoutPopupLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="{Long}4"
                                        text="Logout Popup Label"/>
                                    <popupHeading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout popup heading"
                                        fieldLabel="Heading"
                                        name="./heading"/>
                                    <popupDesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout popup desceription"
                                        fieldLabel="Description"
                                        name="./desc"/>
                                    <popupConfirm
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout popup confirm label"
                                        fieldLabel="Confirm"
                                        name="./confirm"/>
                                    <popupCancel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the logout popup cancel label"
                                        fieldLabel="Cancel"
                                        name="./cancel"/>
                                </items>
                            </column>
                        </items>
                    </logoutConfiguration>
                    <exitPopup
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Exit Popup"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <popupTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the text for the popup title bar"
                                        fieldLabel="Popup Title"
                                        name="./popupTitle"/>
                                    <popupText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Popup Text"
                                        name="./popupText"/>
                                    <okButton
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the OK Button text"
                                        fieldLabel="OK Button Text"
                                        name="./okButtonText"/>
                                    <cancelButtonText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Cancel Button Text"
                                        fieldLabel="Cancel Button Text"
                                        name="./cancelButtonText"/>
                                    <safeList
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Enter the domains for which the exit popup will not be shown"
                                        fieldLabel="Safe Domains List"
                                        name="./safeList"/>
                                </items>
                            </column>
                        </items>
                    </exitPopup>
                    <socialIcons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Icons"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <socialIconsHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the social icons text"
                                                fieldLabel="Social Icons Text"
                                                name="./socialIconsHeader"/>
                                            <instagramURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Instagram URL"
                                                fieldLabel="Instagram URL"
                                                name="./instagramURL"/>
                                            <youtubeURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Youtube URL"
                                                fieldLabel="Youtube URL"
                                                name="./youtubeURL"/>
                                            <facebookURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Configure the Facebook URL"
                                                fieldLabel="Facebook URL"
                                                name="./facebookURL"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </socialIcons>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
