<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Offers Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_form_container_v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="container-showhide-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <myOffers
                        jcr:primaryType="nt:unstructured"
                        jcr:title="My Offers"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <variations
                                        granite:class="offers-container-variation-event"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Variation"
                                        name="./variation">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            offers-container-variation-event-target=".offers-showhide-target"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <myOffers
                                                jcr:primaryType="nt:unstructured"
                                                text="My Offers"
                                                value="myOffers"/>
											<selectRetailer
                                                jcr:primaryType="nt:unstructured"
                                                text="Select Retailer"
                                                value="selectRetailer"/>
                                            <retailerCouponPwa
                                                jcr:primaryType="nt:unstructured"
                                                text="Retailer Coupon PWA"
                                                value="retailerCouponPwa"/>
											<redeemCouponPwa
                                                jcr:primaryType="nt:unstructured"
                                                text="Redeem Coupon PWA"
                                                value="redeemCouponPwa"/>
                                        </items>
                                    </variations>
                                    <tile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the title"
                                        fieldLabel="Title"
                                        name="./title"/>
                                    <actionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path"
                                        fieldLabel="Action path"
                                        name="./actionPath"/>
                                    <actionPathGetProfile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path get profile"
                                        fieldLabel="Action Path Get Profile"
                                        name="./actionPathGetProfile"/>
                                    <actionPathLookupUser
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Action Path Lookup User"
                                        fieldLabel="Action Path Lookup User"
                                        name="./actionPathLookupUser"/>
                                    <offerDisruptorFrag
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter offer disruptor fragment path"
                                        fieldLabel="Offer Disruptor Fragment"
                                        name="./offerDisruptorFragment"/>
									<hcpOfferDisruptorFrag
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter offer disruptor fragment path for HCP offers"
                                        fieldLabel="HCP Offer Disruptor Fragment"
                                        name="./hcpOfferDisruptorFragment"/>
                                    <pwaDownloadDisruptorFrag
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter disruptor fragment path for PWA download"
                                        fieldLabel="PWA Download Disruptor Fragment"
                                        name="./pwaDownloadDisruptorFragment"/>   
                                    <updateLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter update label"
                                        fieldLabel="Update Label"
                                        name="./updateLabel"/>
                                    <signUpNoteLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the footnote for Signup module"
                                        fieldLabel="Sign Up Note Label"
                                        name="./signUpNoteLabel"/>
                                    <noteLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the note label"
                                        fieldLabel="Note Label"
                                        name="./noteLabel"/>
                                    <registerPageUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter the Register Page URL"
                                        fieldLabel="Register Page URL"
                                        name="./registerPageUrl"
                                        rootPath="/content/an/similac/global"/>
                                    <registrationFormName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Registration Form Name"
                                        fieldLabel="Registration Form Name"
                                        name="./registrationFormName"/>
                                    <myOfferPageUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter the My Offer Page Url"
                                        fieldLabel="My Offer Page Url"
                                        name="./myOfferPageUrl"
                                        rootPath="/content/an/similac/global"/>
                                    <loginPageUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter the Login Page Url"
                                        fieldLabel="Login Page Url"
                                        name="./loginPageUrl"
                                        rootPath="/content/an/similac/global"/>
                                    <barCodeGenerate
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the barcode generator api path"
                                        fieldLabel="Barcode Generator API path"
                                        name="./barCodeGenerate"/>
                                    <errorUpdateProfileNonDOUser
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the error message for update profile failure for Non DO users"
                                        fieldLabel="Non DO User Update Profile Error Message"
                                        name="./errorUpdateProfileNonDOUser"/>
                                    <newOfferLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the label for new offers"
                                        fieldLabel="New Offer Label"
                                        name="./newOfferLabel"/>
									<newOfferLabel1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the second label for new offers"
                                        fieldLabel="New Offer Label 2"
                                        name="./newOfferLabel1"/>
                                    <expiringOfferLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the label for offers which will expire soon"
                                        fieldLabel="Expiring Offer Label"
                                        name="./expiringOfferLabel"/>
									<expiringOfferLabel1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the second label for offers which will expire soon"
                                        fieldLabel="Expiring Offer Label 2"
                                        name="./expiringOfferLabel1"/>
									<DOThanksMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the DO Thanks Message"
                                        fieldLabel="DO Thanks Message"
                                        name="./DOThanksMessage"/>
									<goToOfferPageText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter go to offer page text"
                                        fieldLabel="Go To Offer Page Text"
                                        name="./goToOfferPageText"/>
                                </items>
                            </column>
                        </items>
                    </myOffers>
                    <purchaserOnlyOffer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Purchaser Only Offer"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <tile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the title"
                                        fieldLabel="Title"
                                        name="./purchaserOnlyOfferTitle"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the description"
                                        fieldLabel="Description"
                                        name="./purchaserOnlyOfferDec"/>
                                    <imgUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Image url"
                                        fileNameParameter="./purchaserOnlyOfferImgUrl"
                                        fileReferenceParameter="./purchaserOnlyOfferImgUrl"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Image Url"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <buttonLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the button label"
                                        fieldLabel="Button Label"
                                        name="./purchaserOnlyOfferBtnLabel"/>
                                </items>
                            </column>
                        </items>
                    </purchaserOnlyOffer>
                    <scanModule
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Scan Module"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                	<showScanModule
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        checked="{Boolean}true"
                                        name="./showScanModule"
                                        text="Enable Scan Module"
                                        fieldDescription="Choose if the Scan Module has to be shown in the Offers page."
                                        uncheckedValue="{Boolean}false"
                                        value="{Boolean}true"/>
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Scan Module Heading"
                                        fieldLabel="Heading"
                                        name="./scanModuleHeading"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Scan Module Description"
                                        fieldLabel="Description"
                                        name="./scanModuleDesc"/>
                                    <imgUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                        allowUpload="{Boolean}false"
                                        autoStart="{Boolean}false"
                                        class="cq-droptarget"
                                        fieldLabel="Scan Module Image"
                                        fileNameParameter="./scanModuleImage"
                                        fileReferenceParameter="./scanModuleImage"
                                        mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                        multiple="{Boolean}false"
                                        name="./file"
                                        title="Image Url"
                                        uploadUrl="${suffix.path}"
                                        useHTML5="{Boolean}true"/>
                                    <switchToDigitalTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Switch to Digital Title"
                                        fieldLabel="Switch to Digital Title"
                                        name="./switchToDigitalTitle"/>
                                    <switchToDigitalDesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Switch to Digital Description"
                                        fieldLabel="Switch to Digital Description"
                                        name="./switchToDigitalDesc"/>
                                    <goDigitalBtnTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Go Digital Button Text"
                                        fieldLabel="Go Digital Button Text"
                                        name="./goDigitalBtnTxt"/>
                                    <disclaimerTxt1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Disclaimer Text"
                                        fieldLabel="Disclaimer Text"
                                        name="./disclaimerTxt1"/>
                                    <popUpTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Pop Up Text"
                                        fieldLabel="Pop Up Text"
                                        name="./popUpTxt"/>
                                    <popUpYesBtnTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Pop Up Yes Button Text"
                                        fieldLabel="Yes Button Text"
                                        name="./yesBtnTxt"/>
                                    <popUpNoBtnTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Pop Up No Button Text"
                                        fieldLabel="No Button Text"
                                        name="./noBtnTxt"/>
                                    <congratsTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Congratulations Title"
                                        fieldLabel="Congratulations Title"
                                        name="./congratsTitle"/>
                                    <congratsDesc1
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Congratulations Description 1"
                                        fieldLabel="Congratulations Description1"
                                        name="./congratsDesc1"/>
                                    <congratsDesc2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Congratulations Description 2"
                                        fieldLabel="Congratulations Description2"
                                        name="./congratsDesc2"/>
                                    <disclaimerTxt2
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Congratulations Page Disclaimer Text"
                                        fieldLabel="Congratulations Page Disclaimer Text"
                                        name="./disclaimerTxt2"/>
                                    <disclaimerTxt3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Disclaimer Text"
                                        fieldLabel="Disclaimer Text"
                                        name="./disclaimerTxt3"/>
                                    <offerTabFootnote
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Footnote Text in Offers Page"
                                        fieldLabel="Footnote in Offers Page"
                                        name="./offerTabFootnote"/>
                                </items>
                            </column>
                        </items>
                    </scanModule>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
