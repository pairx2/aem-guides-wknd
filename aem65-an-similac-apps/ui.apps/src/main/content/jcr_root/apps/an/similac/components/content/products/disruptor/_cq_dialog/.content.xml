<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Disruptor"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.list.v2.editor]"
    helpPath="/content/documentation/abbott-documenation/abbott-list.html?wcmmode=disabled">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <disruptor
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Disruptor Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <variations
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Variation"
                                                name="./variation">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <product
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Product"
                                                        value="product"/>
                                                    <offers
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Offers"
                                                        value="offers"/>
                                                    <hcp-offer
                                                        jcr:primaryType="nt:unstructured"
                                                        text="HCP Offer"
                                                        value="hcp-offer"/>
                                                </items>
                                            </variations>
                                            <drawerText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Enter the text to be displayed on the blue banner"
                                                fieldLabel="Drawer Text"
                                                name="./drawerText"
                                                removeSingleParagraphContainer="{Boolean}true"
                                                required="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                            </drawerText>
                                            <disruptorTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Enter the disruptor title"
                                                fieldLabel="Disruptor Title"
                                                name="./disruptorTitle"
                                                removeSingleParagraphContainer="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                            <ctaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the CTA Label here"
                                                fieldLabel="CTA Label"
                                                name="./ctaLabel"/>
                                            <ctaURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for the CTA URL"
                                                fieldLabel="CTA URL"
                                                name="./ctaURL"/>
                                            <termsText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Enter the terms &amp; conditions text here"
                                                fieldLabel="Terms &amp; Conditions Text"
                                                name="./termsText"
                                                removeSingleParagraphContainer="{Boolean}true"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <uiSettings
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                                <rtePlugins
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                            </termsText>
                                            <className
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Class Name"
                                                fieldLabel="Class Name"
                                                name="./className"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </disruptor>
                    <images
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Image Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the disruptor image url here"
                                                fieldLabel="Disruptor Image"
                                                fileNameParameter="./imgName"
                                                fileReferenceParameter="./imagePath"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file"
                                                required="{Boolean}true"
                                                title="Disruptor Image"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the mobile disruptor image url here"
                                                fieldLabel="Mobile Disruptor Image"
                                                fileNameParameter="./imgNameMob"
                                                fileReferenceParameter="./imagePathMob"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file"
                                                required="{Boolean}true"
                                                title="Mobile Disruptor Image"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabletImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the tablet disruptor image url here"
                                                fieldLabel="Tablet Disruptor Image"
                                                fileNameParameter="./imgNameTab"
                                                fileReferenceParameter="./imagePathTab"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file"
                                                required="{Boolean}true"
                                                title="Tablet Disruptor Image"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <alttext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter disruptor image alt text here"
                                                fieldLabel="Img Alt text"
                                                name="./altText"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </images>
                    <list
                        jcr:primaryType="nt:unstructured"
                        jcr:title="List Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <listText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="List Text">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./links">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <listText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                            fieldDescription="Enter the list texts here"
                                                            fieldLabel="List Text"
                                                            name="./listText"
                                                            removeSingleParagraphContainer="{Boolean}true"
                                                            useFixedInlineToolbar="{Boolean}true"/>
                                                    </items>
                                                </field>
                                            </listText>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </list>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/apps/an/similac/components/common/dialogs/analytics"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
