<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Product Comparision"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <product1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="First Product"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <productPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Choose Product Path"
                                                fieldLabel="Product Path"
                                                name="./firstProductPath"
                                                required="{Boolean}false"
                                                rootPath="/content"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Product Description"
                                                fieldLabel="Product Description"
                                                name="./description1"
                                                required="{Boolean}false"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the image url here"
                                                fieldLabel="Image URL"
                                                fileNameParameter="./imgName1"
                                                fileReferenceParameter="./imgSrc1"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file1"
                                                title="Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the mobile image url here"
                                                fieldLabel="Mobile Image URL"
                                                fileNameParameter="./imgNameMob1"
                                                fileReferenceParameter="./imagePathMob1"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file1"
                                                title="Mobile Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabletImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the tablet image url here"
                                                fieldLabel="Tablet Image URL"
                                                fileNameParameter="./imgNameTab1"
                                                fileReferenceParameter="./imagePathTab1"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file1"
                                                title="Tablet Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </product1>
                    <product2
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Second Product"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <productPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Choose Product Path"
                                                fieldLabel="Product Path"
                                                name="./secondProductPath"
                                                required="{Boolean}false"
                                                rootPath="/content"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Product Description"
                                                fieldLabel="Product Description"
                                                name="./description2"
                                                required="{Boolean}false"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the image url here"
                                                fieldLabel="Image URL"
                                                fileNameParameter="./imgName2"
                                                fileReferenceParameter="./imgSrc2"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file2"
                                                title="Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the mobile image url here"
                                                fieldLabel="Mobile Image URL"
                                                fileNameParameter="./imgNameMob2"
                                                fileReferenceParameter="./imagePathMob2"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file2"
                                                title="Mobile Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabletImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the tablet image url here"
                                                fieldLabel="Tablet Image URL"
                                                fileNameParameter="./imgNameTab2"
                                                fileReferenceParameter="./imagePathTab2"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file2"
                                                title="Tablet Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </product2>
                    <product3
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Third Product"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <productPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Choose Product Path"
                                                fieldLabel="Product Path"
                                                name="./thirdProductPath"
                                                required="{Boolean}false"
                                                rootPath="/content"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Product Description"
                                                fieldLabel="Product Description"
                                                name="./description3"
                                                required="{Boolean}false"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the image url here"
                                                fieldLabel="Image URL"
                                                fileNameParameter="./imgName3"
                                                fileReferenceParameter="./imgSrc3"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file3"
                                                title="Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the mobile image url here"
                                                fieldLabel="Mobile Image URL"
                                                fileNameParameter="./imgNameMob3"
                                                fileReferenceParameter="./imagePathMob3"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file3"
                                                title="Mobile Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabletImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the tablet image url here"
                                                fieldLabel="Tablet Image URL"
                                                fileNameParameter="./imgNameTab3"
                                                fileReferenceParameter="./imagePathTab3"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file3"
                                                title="Tablet Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </product3>
                    <product4
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Fourth Product"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <productPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Choose Product Path"
                                                fieldLabel="Product Path "
                                                name="./fourthProductPath"
                                                required="{Boolean}false"
                                                rootPath="/content"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldDescription="Enter Product Description"
                                                fieldLabel="Product Description"
                                                name="./description4"
                                                required="{Boolean}false"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the image url here"
                                                fieldLabel="Image URL"
                                                fileNameParameter="./imgName4"
                                                fileReferenceParameter="./imgSrc4"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file4"
                                                title="Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <mobileImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the mobile image url here"
                                                fieldLabel="Mobile Image URL"
                                                fileNameParameter="./imgNameMob4"
                                                fileReferenceParameter="./imagePathMob4"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file4"
                                                title="Mobile Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <tabletImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldDescription="Configure the tablet image url here"
                                                fieldLabel="Tablet Image URL"
                                                fileNameParameter="./imgNameTab4"
                                                fileReferenceParameter="./imagePathTab4"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg]"
                                                multiple="{Boolean}false"
                                                name="./file4"
                                                title="Tablet Image URL"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </product4>
                    <rowConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Row Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <rowcount
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Please enter the number of rows to be displayed"
                                                fieldLabel="Row Count"
                                                name="./rowCount"
                                                value="4"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </rowConfig>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
