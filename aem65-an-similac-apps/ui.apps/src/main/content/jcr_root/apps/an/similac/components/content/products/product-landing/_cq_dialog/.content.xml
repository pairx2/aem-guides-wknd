<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    granite:class="prod-showhide-tabs"
    jcr:primaryType="nt:unstructured"
    jcr:title="Products"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[productComponentEvents]"
    helpPath="/content/documentation/abbott-documenation/abbott-list.html?wcmmode=disabled">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <labels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Label Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <variations
                                                granite:class="variation-dropdown-events"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Variation"
                                                name="./variation">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    variation-dropdown-events-target=".variation-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <prodLanding
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Product Landing"
                                                        value="prodLanding"/>
                                                    <prodSubscription
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Product Subscription"
                                                        value="prodSubscription"/>
                                                </items>
                                            </variations>
                                            <categoryId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Category Id"
                                                fieldLabel="Category Id"
                                                name="./categoryId"/>
                                            <filtersLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the filter label"
                                                fieldLabel="Filter Label"
                                                name="./filtersLabel"/>
                                            <resultsLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the results label"
                                                fieldLabel="Results Label"
                                                name="./resultsLabel"/>
                                            <clearAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the clear all label"
                                                fieldLabel="Clear All Label"
                                                name="./clearAllLabel"/>
                                            <showMoreLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the show more label"
                                                fieldLabel="Show more Label"
                                                name="./showMoreLabel"/>
                                            <showLessLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the show less label"
                                                fieldLabel="Show less Label"
                                                name="./showLessLabel"/>
                                            <addToCartLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the add to cart label"
                                                fieldLabel="Add to Cart Label"
                                                name="./addToCartLabel"/>
                                            <findRetailerLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the find retailer label"
                                                fieldLabel="Find Retailer Label"
                                                name="./findRetailerLabel"/>
                                            <retailerURL
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Browse for retailer url"
                                                fieldLabel="Retailer URL"
                                                name="./retailerURL"
                                                rootPath="/content/an"/>
                                            <outOfStockLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the out of stock label"
                                                fieldLabel="Out of stock Label"
                                                name="./outOfStockLabel"/>
                                            <backOrderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the back order label"
                                                fieldLabel="Back Order Label"
                                                name="./backOrderLabel"/>
                                            <searchLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the search label"
                                                fieldLabel="Search Label"
                                                name="./searchLabel"/>
                                            <searchResultsLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the search results label"
                                                fieldLabel="Search Results Label"
                                                name="./searchResultsLabel"/>
                                            <resetLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the reset label"
                                                fieldLabel="Reset Label"
                                                name="./resetLabel"/>
                                            <noResultLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the no results label"
                                                fieldLabel="No results Label"
                                                name="./noResultLabel"/>
                                            <learnMoreLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the learn more label"
                                                fieldLabel="Learn More Label"
                                                name="./learnMoreLabel"/>
                                            <callToOrderLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the call to order label"
                                                fieldLabel="Call To Order Label"
                                                name="./callToOrderLabel"/>
                                            <regularPriceLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the regular price label"
                                                fieldLabel="Regular Price Label"
                                                name="./regularPriceLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </labels>
                    <sizeInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Product Size and Filter Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sizeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the number of products to be displayed on page"
                                                fieldLabel="Size to be displayed"
                                                name="./showSizeLabel"/>
                                            <defaultSize
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default number of products to be displayed on page"
                                                fieldLabel="Default Size to be displayed"
                                                name="./defaultSize"/>
                                            <options
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Products Count">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./productCount">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <sizeValue
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Add the number of products allowed to be displayed on page"
                                                            fieldLabel="Number of Products"
                                                            name="./optionValue"/>
                                                    </items>
                                                </field>
                                            </options>
                                            <categoryType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Filter Category Type">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./categoryType">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <categoryValue
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the filter category values"
                                                            fieldLabel="Filter Category Value"
                                                            name="./categoryValue"/>
                                                    </items>
                                                </field>
                                            </categoryType>
                                            <excludedPage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Excluded Page Path">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./excludedPage">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <excludedPagePath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Enter the excluded page paths"
                                                            fieldLabel="Excluded Page Path"
                                                            name="./excludedPage"
                                                            rootPath="/content"/>
                                                    </items>
                                                </field>
                                            </excludedPage>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </sizeInfo>
                    <sortInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Product Sort Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sortLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the sort by label"
                                                fieldLabel="Sort By Label"
                                                name="./sortByLabel"/>
                                            <deafultSortOption
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default sort option"
                                                fieldLabel="Default Sort Label"
                                                name="./defaultSortOption"/>
                                            <defaultValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default sort option value"
                                                fieldLabel="Default Sort Option Value"
                                                name="./defaultSortVal"/>
                                            <options
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Sort Options">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./sortOptionVal">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <sortLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the sort label"
                                                            fieldLabel="Sort Label"
                                                            name="./sortLabel"/>
                                                        <sortOption
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the sort option"
                                                            fieldLabel="Sort Value"
                                                            name="./optionValue"/>
                                                    </items>
                                                </field>
                                            </options>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </sortInfo>
                    <subscriptionInfo
                        granite:class="variation-showhide-target"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Subscription Info"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <popUpTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the pop up title"
                                                fieldLabel="Pop Up Title"
                                                name="./popUpTitle"/>
                                            <popUpConfirm
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the pop up confirm text"
                                                fieldLabel="Pop up confirm text"
                                                name="./popUpConfirm"/>
                                            <popUpCancel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the pop up cancel text"
                                                fieldLabel="Pop up Cancel Button"
                                                name="./popUpCancel"/>
                                            <eachPrice
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter text to display next to price"
                                                fieldLabel="Each Price Text"
                                                name="./eachPrice"/>
                                            <priceHelperText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the price helper text"
                                                fieldLabel="Price helper text"
                                                name="./priceHelperText"/>
                                            <requiredMax
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Required Maximum Label"
                                                fieldLabel="Required Maximum Label"
                                                name="./requiredMax"/>
                                            <requiredMin
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Required Minimum Label"
                                                fieldLabel="Required Minimum Label"
                                                name="./requiredMin"/>
                                            <returnLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Return Label"
                                                fieldLabel="Return Label"
                                                name="./returnLabel"/>
                                            <selectLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the select Label"
                                                fieldLabel="Select Label"
                                                name="./selectLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </subscriptionInfo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
