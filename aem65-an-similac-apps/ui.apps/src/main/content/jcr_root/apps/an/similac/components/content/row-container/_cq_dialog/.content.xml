<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Similac Container"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <variations
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Variations"
                                                name="./variations">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <hmo-page
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Product Innovation Landing Page"
                                                        value="hmopage"/>
                                                    <article-page
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Article Page"
                                                        value="articlePage"/>
                                                    <landing-page
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Landing Page"
                                                        value="landingpage"/>
                                                </items>
                                            </variations>
                                            <id
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="HTML ID attribute to apply to the component."
                                                fieldLabel="ID"
                                                name="./id"/>
                                            <padding
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Padding "
                                                name="./padding">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <_x0031_875
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="1.875rem"
                                                        value="1_875"/>
                                                    <_x0032_625
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="2.625rem"
                                                        value="2_625"/>
                                                    <_x0033_375
                                                        jcr:primaryType="nt:unstructured"
                                                        text="3.375rem"
                                                        value="3_375"/>
                                                    <none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="None"
                                                        value="none"/>
                                                </items>
                                            </padding>
                                            <devices
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Devices"
                                                name="./devices">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <all
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="All"
                                                        value="all"/>
                                                    <large
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Desktop"
                                                        value="lg"/>
                                                    <medium
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Tab"
                                                        value="md"/>
                                                    <small
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Mobile"
                                                        value="sm"/>
                                                </items>
                                            </devices>
                                            <orientation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Padding Orientation"
                                                name="./orientation">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top"
                                                        value="pt"/>
                                                    <bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bottom"
                                                        value="pb"/>
                                                    <both
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top &amp; Bottom"
                                                        value="py"/>
                                                </items>
                                            </orientation>
                                            <backgroundColor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Background Color"
                                                name="./bgColor">
                                                <items
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="an/similac/components/common/dialogs/colors"/>
                                            </backgroundColor>
                                            <class
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Specify Custom Class for Components"
                                                fieldLabel="Class"
                                                name="./class"/>
                                            <margin
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Margin"
                                                name="./margin">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <_x0031_875
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="1.875rem"
                                                        value="1_875"/>
                                                    <_x0032_625
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="2.625rem"
                                                        value="2_625"/>
                                                    <_x0033_375
                                                        jcr:primaryType="nt:unstructured"
                                                        text="3.375rem"
                                                        value="3_375"/>
                                                    <none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="None"
                                                        value="none"/>
                                                </items>
                                            </margin>
                                            <marginOrientation
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Margin Orientation"
                                                name="./marginOrientation">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top"
                                                        value="mt"/>
                                                    <bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bottom"
                                                        value="mb"/>
                                                    <both
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Top &amp; Bottom"
                                                        value="my"/>
                                                </items>
                                            </marginOrientation>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
