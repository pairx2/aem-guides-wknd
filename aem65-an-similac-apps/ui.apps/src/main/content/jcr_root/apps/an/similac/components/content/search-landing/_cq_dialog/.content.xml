<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Search"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.list.v2.editor]"
    helpPath="/content/documentation/abbott-documenation/abbott-list.html?wcmmode=disabled">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <labels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Label Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <filtersLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the filter label"
                                                fieldLabel="Filter Label"
                                                name="./filtersLabel"/>
											<actionPath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the action path"
                                                fieldLabel="Action Path"
                                                name="./actionPath"/>
                                            <resultsLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the results label"
                                                fieldLabel="Results Label"
                                                name="./resultsLabel"/>
                                            <clearAllLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the clear all label"
                                                fieldLabel="Clear All Label"
                                                name="./clearAllLabel"/>
                                            <showMoreLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the show more label"
                                                fieldLabel="Show more Label"
                                                name="./showMoreLabel"/>
                                            <showLessLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the show less label"
                                                fieldLabel="Show less Label"
                                                name="./showLessLabel"/>
                                            <searchLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the search label"
                                                fieldLabel="Search Label"
                                                name="./searchLabel"/>
                                            <searchResultsLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the search results label"
                                                fieldLabel="Search Results Label"
                                                name="./searchResultsLabel"/>
                                            <resetLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the reset label"
                                                fieldLabel="Reset Label"
                                                name="./resetLabel"/>
                                            <noResultLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the no results label"
                                                fieldLabel="No results Label"
                                                name="./noResultLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </labels>
                    <sizeInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Size Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sizeLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the number of results to be displayed on page"
                                                fieldLabel="Size to be displayed"
                                                name="./showSizeLabel"/>
                                            <defaultSize
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default number of products to be displayed on page"
                                                fieldLabel="Default Size to be displayed"
                                                name="./defaultSize"/>
                                            <options
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Search Results Count">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./searchResultsCount">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <sizeLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the results label"
                                                            fieldLabel="Results Label"
                                                            name="./optionLabel"/>
                                                        <sizeValue
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the number of results value"
                                                            fieldLabel="Results Value"
                                                            name="./optionValue"/>
                                                    </items>
                                                </field>
                                            </options>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </sizeInfo>
                    <sortInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Sort Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sortLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the sort by label"
                                                fieldLabel="Sort By Label"
                                                name="./sortByLabel"/>
                                            <deafultSortOption
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default sort option"
                                                fieldLabel="Default Sort Label"
                                                name="./defaultSortOption"/>
                                            <defaultValue
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default sort option value"
                                                fieldLabel="Default Sort Option Value"
                                                name="./defaultSortVal"/>
                                            <options
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Sort Options">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./sortOptionVal">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <sortLabel
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the sort label"
                                                            fieldLabel="Sort Label"
                                                            name="./sortLabel"/>
                                                        <sortOption
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the sort option"
                                                            fieldLabel="Sort Value"
                                                            name="./optionValue"/>
                                                    </items>
                                                </field>
                                            </options>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </sortInfo>
                    <filterInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Filter Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        margin="{Boolean}true">
                                        <items jcr:primaryType="nt:unstructured">
											<defaultFilterLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the default filter label"
                                                fieldLabel="Default Filter Label"
                                                name="./defaultFilterLabel"/>
                                            <filterOptions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Filter Details">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./filterOptions">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <attributeCode
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the Attribute Code"
                                                            fieldLabel="Attribute Code"
                                                            name="./attributeCode"/>
                                                        <label
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Enter the filter label"
                                                            fieldLabel="Filter Label"
                                                            name="./filterLabel"/>
                                                        <options
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Filter Results Values">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./filters">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <sizeLabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Enter the results label"
                                                                        fieldLabel="Results Label"
                                                                        name="./filterOptionLabel"/>
                                                                    <sizeValue
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Add the number of products allowed to be displayed on page"
                                                                        fieldLabel="Filter Value"
                                                                        name="./optionValue"/>
                                                                </items>
                                                            </field>
                                                        </options>
                                                    </items>
                                                </field>
                                            </filterOptions>
                                        </items>
                                    </column>
                                </items>
                            </column>
                        </items>
                    </filterInfo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
