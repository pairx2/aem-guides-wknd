<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Title"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.title.v2.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v2"
    trackingFeature="core-components:title:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <colorfield
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                fieldLabel="Title color"
                                                name="./titlecolor"
                                                showDefaultColors="{Boolean}true"
                                                showProperties="{Boolean}false">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <denimblue
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#10679e"/>
                                                    <pinegreen
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#1a8f83"/>
                                                    <vividviolet
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#802f92"/>
                                                    <saffronyellow
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#f3bd3e"/>
                                                </items>
                                            </colorfield>
											<bgColor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                fieldLabel="Background color"
                                                name="./bgColor"
                                                showDefaultColors="{Boolean}true"
                                                showProperties="{Boolean}false">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <denimblue
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#10679e"/>
                                                    <pinegreen
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#1a8f83"/>
                                                    <vividviolet
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#802f92"/>
                                                    <saffronyellow
                                                        jcr:primaryType="nt:unstructured"
                                                        value="#f3bd3e"/>
                                                </items>
                                            </bgColor>
                                            <class
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="Select"
                                                fieldDescription="Select  the corresponding field category"
                                                fieldLabel="Class"
                                                name="./className"
                                                required="{Boolean}false">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <shadowedtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Shadowed Title"
                                                        value="shadowedTitle"/>
													<aaquashadowedtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Aqua Shadowed Title"
                                                        value="aquaShadowedTitle"/>
                                                    <mainShadowedTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Main Shadowed Title"
                                                        value="mainShadowedTitle"/>
													<borderedTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Bordered Title"
                                                        value="borderedTitle"/>
													<productComptitle
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Product Comparison Title"
                                                        value="pdp-tab__product-comp-tablecell-label"/>
													<faqHeader
                                                        jcr:primaryType="nt:unstructured"
                                                        text="FAQ Header"
                                                        value="faq__header"/>
													<none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="None"
                                                        value="none"/>
                                                </items>
                                            </class>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
