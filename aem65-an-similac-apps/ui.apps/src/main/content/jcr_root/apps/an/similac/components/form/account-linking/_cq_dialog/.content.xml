<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Account Linking Parent"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.form.container.v2.editor]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <commonProperties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Common Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
									<formTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form title"
                                        fieldLabel="Form Title"
                                        name="./formTitle"/>
                                    <formName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form name"
                                        fieldLabel="Form Name"
                                        name="./formName"/>
									<accountLinkingTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the account linking title"
                                        fieldLabel="Account Linking Title"
                                        name="./accountLinkingTitle"/>
                                    <actionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path"
                                        fieldLabel="Action path"
                                        name="./actionPath"/>
									<actionPathLinkingAccount
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Conflicting Account API endpoint"
                                        fieldLabel="Conflicting Account API endpoint"
                                        name="./actionPathLinkingAccount"/>
									<actionPathToUpdateProfile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Action Path to Update Profile"
                                        fieldLabel="Action Path to Update Profile"
                                        name="./actionPathToUpdateProfile"/>
									<redirectOnSuccessURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter URL to redirect to on success"
                                        fieldLabel="Redirect On Success URL"
                                        name="./redirectOnSuccessURL"/>
									<redirectOnSuccessURLSubscription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter URL to redirect to on success with Subscription Product in Cart"
                                        fieldLabel="Redirect On Success URL w/Subscription"
                                        name="./redirectOnSuccessURLSubscription"/>
									<checkoutPageURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter the checkout URL"
                                        fieldLabel="Checkout URL"
                                        name="./checkoutPageURL"/>
									<myOffersURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter the My Offers URL"
                                        fieldLabel="My Offers URL"
                                        name="./myOffersURL"/>
                                </items>
                            </column>
                        </items>
                    </commonProperties>
					<formPages
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Pages"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <siteRegistered
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The name of the Site Registered form page"
                                        fieldLabel="Site Form Page name"
                                        name="./site"/>
                                    <site
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The name of the Site form page"
                                        fieldLabel="Social Form Page name"
                                        name="./social"/>
                                    <reRegister
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The name of the Re Register form page"
                                        fieldLabel="Re-Register Form Page name"
                                        name="./reRegister"/>
                                </items>
                            </column>
                        </items>
                    </formPages>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
