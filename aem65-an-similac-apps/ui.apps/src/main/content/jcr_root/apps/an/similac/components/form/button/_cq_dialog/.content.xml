<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Form Button"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.form.button.v2.editor]"
    helpPath="https://www.adobe.com/go/aem_cmp_form_button_v2"
    trackingFeature="core-components:form-button:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <type
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Type"
                                                name="./type">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <button
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Button"
                                                        value="button"/>
                                                    <submit
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Submit"
                                                        value="submit"/>
                                                    <reset
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Reset"
                                                        value="reset"/>
                                                    <cancel
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Cancel"
                                                        value="cancel"/>
                                                    <linkbutton
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Link Button"
                                                        value="linkButton"/>
                                                    <link
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Link"
                                                        value="link"/>
                                                    <sociallogin
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Social Login"
                                                        value="socialLogin"/>
                                                </items>
                                            </type>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Text displayed on the button"
                                                fieldLabel="Title"
                                                name="./jcr:title"/>
                                            <name
                                                granite:class="cmp-form-button__editor-name"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="The name of the button"
                                                fieldLabel="Name"
                                                name="./name">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    error-message="When providing a button value, a name must also be provided"/>
                                            </name>
                                            <value
                                                granite:class="cmp-form-button__editor-value"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the value of button"
                                                fieldLabel="Value"
                                                name="./value"/>
                                            <btnClassName
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the class name"
                                                fieldLabel="Class Name"
                                                name="./btnClassName"/>
                                            <ctalink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="CTA Link"
                                                name="./href"/>
                                            <primary
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check this for primary button"
                                                name="./primary"
                                                text="Check this for Primary button"
                                                value="{Boolean}true"/>
                                            <inlineBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check this for inline button"
                                                name="./inlineBtn"
                                                text="Check this for Inline button"
                                                value="{Boolean}true"/>
                                            <renderOn
                                                granite:class="[renderOn-checkbox-events-button]"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}false"
                                                name="./checkboxRenderOn"
                                                text="Enable to render Premature dropdown field"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    renderOn-checkbox-events-target=".renderon-checkbox-showhide-target"/>
                                            </renderOn>
                                            <renderOnField
                                                granite:class="renderon-checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the render on field"
                                                fieldLabel="Render on Field"
                                                name="./renderOnField"/>
                                            <renderOnValue
                                                granite:class="renderon-checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the render on value"
                                                fieldLabel="Render on Value"
                                                name="./renderOnValue"/>
                                            <renderOnFieldMapped
                                                granite:class="renderon-checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the field to be mapped"
                                                fieldLabel="Mapped Field"
                                                name="./fieldMapped"/>
                                            <magento
                                                granite:class="renderon-checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Magento value"
                                                fieldLabel="Magento"
                                                name="./magento"/>
                                            <otp
                                                granite:class="renderon-checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the OTP"
                                                fieldLabel="OTP"
                                                name="./otp"/>
                                            <dataFunVal
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="data-fun value, added for PWA logout"
                                                fieldLabel="Enter data-fun value"
                                                name="./dataFun"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <eventcategory
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Event Category for  the button"
                                                fieldLabel="Event Category"
                                                name="./eventCategoryButton"/>
                                            <eventaction
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Event Action"
                                                fieldLabel="Event Action"
                                                name="./eventActionButton"/>
                                            <eventlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter the Event Label"
                                                fieldLabel="Event Label"
                                                name="./eventLabelButton"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </analytics>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
