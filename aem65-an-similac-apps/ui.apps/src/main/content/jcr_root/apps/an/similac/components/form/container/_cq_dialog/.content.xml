<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Form Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.form.container.v2.editor,containerCompEvents]"
    helpPath="https://www.adobe.com/go/aem_cmp_form_container_v2"
    trackingFeature="core-components:form-container:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="container-showhide-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <formDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Form Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <variations
                                        granite:class="form-variation-event"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Variation"
                                        name="./variation">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            form-variation-event-target=".contactus-showhide-target"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <registration
                                                jcr:primaryType="nt:unstructured"
                                                text="Registration"
                                                value="registration"/>
                                            <fast-registration
                                                jcr:primaryType="nt:unstructured"
                                                text="Fast Registration"
                                                value="fastRegistration"/>
                                            <forgot-password
                                                jcr:primaryType="nt:unstructured"
                                                text="Forgot Password"
                                                value="forgotPassword"/>
                                            <reset-password
                                                jcr:primaryType="nt:unstructured"
                                                text="Reset Password"
                                                value="resetPassword"/>
                                            <login
                                                jcr:primaryType="nt:unstructured"
                                                text="Login"
                                                value="login"/>
                                            <pwalogin
                                                jcr:primaryType="nt:unstructured"
                                                text="PWA Login"
                                                value="pwalogin"/>
                                            <checkoutlogin
                                                jcr:primaryType="nt:unstructured"
                                                text="Checkout Login"
                                                value="checkoutLogin"/>
                                            <ecommerce-registration
                                                jcr:primaryType="nt:unstructured"
                                                text="Ecommerce Registration"
                                                value="ecommerceRegistration"/>
                                            <header-login
                                                jcr:primaryType="nt:unstructured"
                                                text="Header Login"
                                                value="headerLogin"/>
                                            <contact-us
                                                jcr:primaryType="nt:unstructured"
                                                text="Contact Us"
                                                value="contactUs"/>
                                            <account-linking
                                                jcr:primaryType="nt:unstructured"
                                                text="Account Linking"
                                                value="accountLinking"/>
                                            <do-retailer-login
                                                jcr:primaryType="nt:unstructured"
                                                text="Digital Offer Retailer Login"
                                                value="do-retailer-login"/>
                                            <do-retailer-registration
                                                jcr:primaryType="nt:unstructured"
                                                text="Digital Offer Retailer Registration"
                                                value="do-retailer-registration"/>
                                            <unsubscribe
                                                jcr:primaryType="nt:unstructured"
                                                text="Unsubscribe"
                                                value="unsubscribe"/>
                                            <manage-address
                                                jcr:primaryType="nt:unstructured"
                                                text="Manage Address"
                                                value="manageAddress"/>
                                            <mailing-address
                                                jcr:primaryType="nt:unstructured"
                                                text="Mailing Address"
                                                value="mailingAddress"/>
                                            <complete-profile
                                                jcr:primaryType="nt:unstructured"
                                                text="Complete Profile"
                                                value="completeProfile"/>
                                            <sms-notification
                                                jcr:primaryType="nt:unstructured"
                                                text="SMS Notification"
                                                value="smsNotification"/>
                                            <account-setting
                                                jcr:primaryType="nt:unstructured"
                                                text="Account Settings"
                                                value="accountSettings"/>
                                        </items>
                                    </variations>
                                    <formTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form title"
                                        fieldLabel="Form Title"
                                        name="./formTitle"/>
                                    <formName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form name"
                                        fieldLabel="Form Name"
                                        name="./formName"/>
                                    <formType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form type"
                                        fieldLabel="Form Type"
                                        name="./formType"/>
									<forgotPasswordText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Forgot Password Text"
                                        name="./forgotPasswordText"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </forgotPasswordText>
                                    <actionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path"
                                        fieldLabel="Action path"
                                        name="./actionPath"/>
                                    <forgotPswrdActionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter forgot password action path"
                                        fieldLabel="Forgot Password Action Path"
                                        name="./forgotPasswordActionPath"/>
                                    <disclaimer
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter disclaimer"
                                        fieldLabel="Disclaimer"
                                        name="./disclaimer"/>
                                    <successMsgHeading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter success message heading"
                                        fieldLabel="Success Message Heading"
                                        name="./successMsgHeading"/>
                                    <successMsgText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter success message text"
                                        fieldLabel="Success Message Text"
                                        name="./successMsgText"/>
                                    <saveLocal
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter save local"
                                        fieldLabel="Save Local"
                                        name="./saveLocal"/>
                                    <redirectOnSuccessURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter Redirect on success url"
                                        fieldLabel="Redirect On Success URL"
                                        name="./redirectOnSuccessURL"/>
                                    <redirectOnSuccessURLNeosure
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter Redirect on success url for Neosure"
                                        fieldLabel="Redirect On Success URL Neosure"
                                        name="./redirectOnSuccessURLNeosure"/>
                                    <redirectOnSuccessURLOasis
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter Redirect on success url for Oasis"
                                        fieldLabel="Redirect On Success URL Oasis"
                                        name="./redirectOnSuccessURLOasis"/>
                                    <redirectOnSuccessURLSubscription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter Redirect on success url subscription"
                                        fieldLabel="Redirect On Success URL Subscription"
                                        name="./redirectOnSuccessURLSubscription"/>
                                    <chekoutLoginErrorMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter error message on checkout login failure"
                                        fieldLabel="Checkout Login Error Message"
                                        name="./chekoutLoginErrorMessage"/>
                                    <redirectOnHistoryRedirectUrl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter Redirect on Order History"
                                        fieldLabel="Order History Redirect URL"
                                        name="./historyRedirectUrl"/>
                                    <redirectToPreviousPage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check to redirect to the previous page after submission"
                                        name="./redirectToPreviousPage"
                                        text="Redirect to Previous Page?"
                                        uncheckedValue="{Boolean}false"
                                        value="{Boolean}true"/>
                                    <actionPathOnLoad
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path on load"
                                        fieldLabel="Action path On Load"
                                        name="./actionPathOnLoad"/>
                                    <actionPathToUpdateProfile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path to update profile"
                                        fieldLabel="Action path To Update Profile"
                                        name="./actionPathToUpdateProfile"/>
                                    <actionPathGetProfile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path get profile"
                                        fieldLabel="Action Path Get Profile"
                                        name="./actionPathGetProfile"/>
                                    <errorUpdateProfile
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the error message for update profile failure"
                                        fieldLabel="Update Profile Error Message"
                                        name="./errorUpdateProfile"/>
                                    <errorUpdateProfileNonDO
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the error message for update profile failure for Non DO users"
                                        fieldLabel="Non DO User Update Profile Error Message"
                                        name="./errorUpdateProfileNonDOUser"/>
                                    <socialRegisterMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the social register message"
                                        fieldLabel="Social Register Message"
                                        name="./socialRegisterMessage"/>
                                    <onetimepurchase
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter one time purchase"
                                        fieldLabel="One TIme Purchase Message"
                                        name="./oneTimePurchaseMessage"/>
                                    <subscriptionmessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Subscription Message"
                                        fieldLabel="Subscription Message"
                                        name="./subscriptionMessage"/>
                                    <recaptcha
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Please check if the RECAPTCHA is required"
                                        name="./enableRecaptcha"
                                        text="Enable RECAPTCHA"
                                        uncheckedValue="{Boolean}false"
                                        value="{Boolean}true"/>
									<campaign
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Please check if the Campaign parameter is required in JSON"
                                        name="./enableCampaign"
                                        text="Enable Campaign Parameters"
                                        uncheckedValue="{Boolean}false"
                                        value="{Boolean}true"/>
                                </items>
                            </column>
                        </items>
                    </formDetails>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                    <quickregistration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Quick Registration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <ctalabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter CTA Label"
                                        fieldLabel="CTA Label"
                                        name="./ctaLabel"/>
                                    <ctaLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter CTA Link"
                                        fieldLabel="CTA Link"
                                        name="./ctaLink"/>
                                    <new-window
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Check to open the link in a new window"
                                        name="./newWindow"
                                        text="Open in new window"
                                        value="{Boolean}true"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Description"
                                        name="./description"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </description>
                                </items>
                            </column>
                        </items>
                    </quickregistration>
                    <contactUs
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Contact Us"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Contact Us title"
                                        fieldLabel="Title"
                                        name="./contactUsTitle"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Description"
                                        name="./contactUsDescription"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </description>
                                    <additional_inquiries
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Additional Inquiries Details"
                                        name="./additionalInquiries"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </additional_inquiries>
                                </items>
                            </column>
                        </items>
                    </contactUs>
                    <sociallogin
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Social Login"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <registrationurl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter RegistrationURL"
                                        fieldLabel="Registration URL"
                                        name="./registrationURL"/>
                                    <accountlinkingurl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter AccountLinking URL"
                                        fieldLabel="AccountLinking URL"
                                        name="./accountLinkingURL"/>
                                    <socialloginurl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Social Login URL"
                                        fieldLabel="Social Login URL"
                                        name="./socialLoginSuccessURL"/>
                                    <myOffersURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter My Offers Page URL"
                                        fieldLabel="My Offers URL"
                                        name="./myOffersURL"/>
                                </items>
                            </column>
                        </items>
                    </sociallogin>
                    <manageAddress
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Manager Address"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <initialDataURL
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the data url"
                                        fieldLabel="Initial Data URL"
                                        name="./initialDataURL"/>
                                    <mainHeadLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the main heading label"
                                        fieldLabel="Main Head Label"
                                        name="./mainHeadLabel"/>
                                    <subHeadLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the sub head label"
                                        fieldLabel="Sub Head Label"
                                        name="./subHeadLabel"/>
                                    <contentHead
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the content head"
                                        fieldLabel="Content Head"
                                        name="./contentHead"/>
                                    <changeShippingAddress
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the change shipping address"
                                        fieldLabel="Change Shipping Address Label"
                                        name="./changeShippingAddress"/>
                                    <footerNote
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the footer note"
                                        fieldLabel="Footer Note"
                                        name="./footerNote"/>
                                </items>
                            </column>
                        </items>
                    </manageAddress>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Analytics"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <submissionType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Submission Type"
                                        name="./submissionType"/>
                                    <eventCategory
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Event Category"
                                        name="./eventCategory"/>
                                    <eventAction
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Event Action"
                                        name="./eventAction"/>
                                    <eventLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Event Label"
                                        name="./eventLabel"/>
                                    <eventType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Event type"
                                        fieldLabel="Event Type"
                                        name="./eventType"/>
                                </items>
                            </column>
                        </items>
                    </analytics>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
