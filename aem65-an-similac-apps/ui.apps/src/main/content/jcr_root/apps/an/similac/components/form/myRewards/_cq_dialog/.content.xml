<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="My Rewards"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.form.container.v2.editor,profileContainerCompEvents]"
    helpPath="https://www.adobe.com/go/aem_cmp_form_container_v2"
    trackingFeature="core-components:form-container:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <myRewardData
                        jcr:primaryType="nt:unstructured"
                        jcr:title="My Reward details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <actionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Action Path"
                                        fieldLabel="Action Path"
                                        name="./actionPath"/>
                                    <currentPoints
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter the current points"
                                        fieldLabel="Current Points"
                                        name="./currentPoints"/>
                                    <nextPoints
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter next points"
                                        fieldLabel="Next Points"
                                        name="./nextPoints"/>
									<newUserMessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter new user message"
                                        fieldLabel="New User Message"
                                        name="./newUserMessage"/>	
                                    <rewardsChart
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter rewards chart"
                                        fieldLabel="Rewards chart"
                                        name="./rewardsChart"/>
                                    <howToEarnText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter How to earn link text"
                                        fieldLabel="How to Earn"
                                        name="./howToEarnText"/>
                                    <howToEarnLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter How to earn link URL"
                                        fieldLabel="How to Earn URL"
                                        name="./howToEarnLink"
                                        rootPath="/content"
                                        suffix=".html"/>
                                    <howToEarnIcon
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Enter How to earn icon path"
                                        fieldLabel="How to Earn Icon"
                                        name="./howToEarnIcon"
                                        rootPath="/content"/>
								</items>
                            </column>
                        </items>
                    </myRewardData>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
