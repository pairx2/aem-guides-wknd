<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Profile Container"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[core.wcm.components.form.container.v2.editor,profileContainerCompEvents]"
    helpPath="https://www.adobe.com/go/aem_cmp_form_container_v2"
    trackingFeature="core-components:form-container:v2">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="container-showhide-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <profileFormDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Profile Form Details"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <variations
                                        granite:class="variation-dropdown-events"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Variation"
                                        name="./variation">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            variation-dropdown-events-target=".variation-showhide-target"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <personalInfo
                                                jcr:primaryType="nt:unstructured"
                                                text="Personal Information"
                                                value="personalInfo"/>
                                            <myInfo
                                                jcr:primaryType="nt:unstructured"
                                                text="My Information"
                                                value="myInfo"/>
                                            <changePassword
                                                jcr:primaryType="nt:unstructured"
                                                text="Change Password"
                                                value="changePassword"/>
                                            <babyInfo
                                                jcr:primaryType="nt:unstructured"
                                                text="Baby Information"
                                                value="babyInfo"/>
                                            <smsNotification
                                                jcr:primaryType="nt:unstructured"
                                                text="SMS Notification"
                                                value="smsNotification"/>
                                        </items>
                                    </variations>
                                    <formTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form title"
                                        fieldLabel="Form Title"
                                        name="./formTitle"/>
                                    <formName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter form name"
                                        fieldLabel="Form Name"
                                        name="./formName"/>
                                    <actionPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter action path"
                                        fieldLabel="Action path"
                                        name="./actionPath"/>
                                    <editMyInfo
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Edit My Information"
                                        fieldLabel="Edit My Info"
                                        name="./editMyInfo"/>
                                    <changePwd
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter the Change Password Label"
                                        fieldLabel="Change Password"
                                        name="./changePwd"/>
                                    <requiredLabel
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Please enter the Required Label Text"
                                        fieldLabel="Required Label Text"
                                        name="./requiredLabel"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </requiredLabel>
                                    <strongMomID
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Strong MomID Label"
                                        fieldLabel="Strong MomID Label"
                                        name="./strongMomID"/>
                                    <emailAddress
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter email Address Label"
                                        fieldLabel="Email Address Label"
                                        name="./emailAddress"/>
                                    <getTextAlert
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="getTextAlert"
                                        fieldLabel="getTextAlert"
                                        name="./getTextAlert"/>
                                    <neverMiss
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="neverMiss"
                                        fieldLabel="neverMiss"
                                        name="./neverMiss"/>
                                    <notificationSetting
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="notificationSetting"
                                        fieldLabel="notificationSetting"
                                        name="./notificationSetting"/>
                                    <editPhoneNumberLabelOptOut
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="editPhoneNumberLabelOptOut"
                                        fieldLabel="editPhoneNumberLabelOptOut"
                                        name="./editPhoneNumberLabelOptOut"/>
                                    <editPhoneNumberLabel
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the Edit Phone Number Label"
                                        fieldLabel="Edit Phone Number Label"
                                        name="./editPhoneNumberLabel"/>
                                    <enabledSmsNotificationsTxt
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Add the text for enabled SMS notifications"
                                        fieldLabel="Add the text for enabled SMS notifications"
                                        name="./enabledSmsNotificationsTxt"/>
                                    <enablingSmsNotificationsTxtGreen
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="enablingSmsNotificationsTxtGreen"
                                        fieldLabel="enablingSmsNotificationsTxtGreen"
                                        name="./enablingSmsNotificationsTxtGreen"/>
                                    <enablingSmsNotificationsTxtRed
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="enablingSmsNotificationsTxtRed"
                                        fieldLabel="enablingSmsNotificationsTxtRed"
                                        name="./enablingSmsNotificationsTxtRed"/>
                                    <enablingSmsNotificationsTxt
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Add the text for enabling SMS notifications"
                                        fieldLabel="Add the text for enabling SMS notifications"
                                        name="./enablingSmsNotificationsTxt"/>
                                    <resendSMSText
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="resendSMSText"
                                        fieldLabel="resendSMSText"
                                        name="./resendSMSText"/>
                                    <saveChange
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="saveChange"
                                        fieldLabel="saveChange"
                                        name="./saveChange"/>
                                    <smsNotificaionOptOut
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="smsNotificaionOptOut"
                                        fieldLabel="smsNotificaionOptOut"
                                        name="./smsNotificaionOptOut"/>
                                    <optBackClick
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="optBackClick"
                                        fieldLabel="optBackClick"
                                        name="./optBackClick"/>
                                    <optBackClickTxt
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="optBackClickTxt"
                                        fieldLabel="optBackClickTxt"
                                        name="./optBackClickTxt"/>
                                    <optBackInTxt
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="optBackInTxt"
                                        fieldLabel="optBackInTxt"
                                        name="./optBackInTxt"/>
                                    <modalTitle
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the title for Opt-out modalbox"
                                        fieldLabel="Opt-out Modalbox Title"
                                        name="./modalTitle"/>
                                    <modalDescription1
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the description1 for Opt-out modalbox"
                                        fieldLabel="Opt-out Modalbox Description1"
                                        name="./modalDescription1"/>
                                    <modalDescription2
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the description2 for Opt-out modalbox"
                                        fieldLabel="Opt-out Modalbox Description2"
                                        name="./modalDescription2"/>
                                    <modalConfirmButton
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the confirm Button text for Opt-out modalbox"
                                        fieldLabel="Opt-out Modalbox Confirm Button"
                                        name="./modalConfirmButton"/>
                                    <modalCancelButton
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the cancel buttontext for Opt-out modalbox"
                                        fieldLabel="Opt-out Modalbox Cancel Button"
                                        name="./modalCancelButton"/>
                                    <emailHelperText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Enter the email helper text"
                                        fieldLabel="Email Helper Text"
                                        name="./emailHelperText"
                                        removeSingleParagraphContainer="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/rtePlugins"/>
                                        <uiSettings
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceSuperType="/apps/an/similac/components/common/dialogs/standardrte/uiSettings"/>
                                    </emailHelperText>
                                    <subTitle
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Sub Title"
                                        name="./subTitle"/>
                                    <addBaby
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Add Baby"
                                        name="./addBaby"/>
                                    <editBaby
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Edit Baby"
                                        name="./editBaby"/>
                                    <removeBaby
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Remove Baby"
                                        name="./removeBaby"/>
                                    <label
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Label"
                                        name="./label"/>
                                    <placeholder
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="emailAddress"
                                        fieldLabel="Placeholder"
                                        name="./placeholder"/>
                                    <title
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the title for remove modalbox"
                                        fieldLabel="Remove Modalbox Title"
                                        name="./title"/>
                                    <description
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the description for remove modalbox"
                                        fieldLabel="Remove Modalbox Description"
                                        name="./description"/>
                                    <submitButton
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the submit Button text for remove modalbox"
                                        fieldLabel="Remove Modalbox Submit Button"
                                        name="./submitButton"/>
                                    <cancelButton
                                        granite:class="variation-showhide-target"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter the cancel buttontext for remove modalbox"
                                        fieldLabel="Remove Modalbox Cancel Button"
                                        name="./cancelButton"/>
                                </items>
                            </column>
                        </items>
                    </profileFormDetails>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        sling:hideResource="{Boolean}true"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
