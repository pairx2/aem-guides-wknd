<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
    jcr:mixinTypes="[rep:AccessControllable]"
    jcr:primaryType="cq:Page">
    <rep:policy/>
    <wcm jcr:primaryType="nt:unstructured">
        <foundation jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <responsivegrid jcr:primaryType="nt:unstructured">
                    <policy_corp_generic_page
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ARDX - Generic Page Policy"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[group:ARDX - Container,group:ARDX - Content,group:ARDX - Form,group:ARDX - Navigation,group:ARDX - Search]">
                        <jcr:content
                            jcr:mixinTypes="[cq:ReplicationStatus]"
                            jcr:primaryType="nt:unstructured"/>
                    </policy_corp_generic_page>
                </responsivegrid>
            </components>
        </foundation>
    </wcm>
	<ardx jcr:primaryType="nt:unstructured">
        <division jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <structure jcr:primaryType="nt:unstructured">
                    <genericpage jcr:primaryType="nt:unstructured">
                         <policy_ardx_abbott_favicon_page_template
                            jcr:primaryType="nt:unstructured"
                            jcr:title="ARDx Abbott Microsites Favicon Policy"
                            sling:resourceType="wcm/core/components/policy/policy"
                            appResourcesClientlib="abbott.favicon.abbott">
                            <jcr:content
                                jcr:mixinTypes="[cq:ReplicationStatus]"
                                jcr:primaryType="nt:unstructured"/>
                        </policy_ardx_abbott_favicon_page_template>                                              
                    </genericpage>
                </structure>
            </components>
        </division>
       <globals jcr:primaryType="nt:unstructured">
            <components jcr:primaryType="nt:unstructured">
                <content jcr:primaryType="nt:unstructured">
                </content>
            </components>
        </globals>
    </ardx>
</jcr:root>
