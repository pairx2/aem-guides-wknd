<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/ardx/microsites/settings/wcm/templates/two-col-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="ardx/division/components/structure/genericpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <herobanner 
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/content/herobanner"/>
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/container/container">
                <columncontrol
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="ardx/globals/components/content/columncontrol"
                    colCount="2"
                    columnDivider="true"
                    containerDivRequired="false"
                    fiveColumns="2,2,2,2,4"
                    fourColumns="3,3,3,3"
                    sixColumns="2,2,2,2,2,2"
                    threeColumns="4,4,4"
                    twoColumns="2,10">
                    <tab_item_no_1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <container
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/container/container">
                            <breadcrumb
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="ardx/globals/components/content/breadcrumb"/>
                        </container>
                    </tab_item_no_1>
                    <tab_item_no_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <navigation
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/megamenu/megamenuversions/navigation"
                            collectAllPages="true"
                            disableShadowing="false"
                            megaMenuStyle="ardx/globals/components/content/megamenu/megamenuversions/version3"
                            structureStart="0"/>
                    </tab_item_no_0>
                </columncontrol>
            </container>
        </root>
    </jcr:content>
</jcr:root>
