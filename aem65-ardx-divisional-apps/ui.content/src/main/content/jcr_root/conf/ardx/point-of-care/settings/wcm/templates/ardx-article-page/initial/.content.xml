<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/ardx/point-of-care/settings/wcm/templates/ardx-article-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="ardx/division/components/structure/genericpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <container_1048282004
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/container/container"
                decorative="false"
                hideImgOnMobile="false"
                id="breadcrum-container">			
				<columncontrol
					jcr:primaryType="nt:unstructured"
					sling:resourceType="ardx/globals/components/content/columncontrol"
					colCount="2"
					containerDivRequired="false"
					twoColumns="10,2">
					<tab_item_no_0
						jcr:primaryType="nt:unstructured"
						sling:resourceType="wcm/foundation/components/parsys">
						<breadcrumb
							cq:styleIds="[1675434017470,1675433802069]"
							jcr:primaryType="nt:unstructured"
							sling:resourceType="ardx/globals/components/content/breadcrumb"
							disableShadowing="false"
							hideCurrent="false"
							id="breadcrumb-section"
							showHidden="false"
							startLevel="4"/>
					</tab_item_no_0>
					<tab_item_no_1
						jcr:primaryType="nt:unstructured"
						sling:resourceType="wcm/foundation/components/parsys">
						<link
							cq:styleIds="[1675434450762,1601294943958]"
							jcr:primaryType="nt:unstructured"
							sling:resourceType="ardx/globals/components/content/link"
							action="_self"
							external="false"
							icon="abt-icon abt-icon-share-alt"
							id="page-share"
							redirectConfirm="true"/>
						<embed
							jcr:primaryType="nt:unstructured"
							sling:resourceType="ardx/globals/components/content/embed"
							id="addThis-share"
							iframe="&lt;script type=&quot;text/javascript&quot; src=&quot;//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-63d8b1dba25b82da&quot;>&lt;/script>"
							type="iframe"/>
					</tab_item_no_1>
				</columncontrol>
			</container_1048282004>
            <herobanner
                cq:styleIds="[1605930726984]"
                jcr:primaryType="nt:unstructured"
                fileReference="/content/dam/ardx/globalpointofcare/common/Viewpoints-Interior-Header-large.jpg"				
                sling:resourceType="ardx/globals/components/content/herobanner"/>
            <container
                cq:styleIds="[1607946430061]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/container/container">
                <columncontrol
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="ardx/globals/components/content/columncontrol"
                    colCount="2"
                    containerDivRequired="false"
					id="article-detail-section"					
                    twoColumns="8,4">
                    <tab_item_no_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/article/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h2"/>
                        <image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/article/image"/>
                        <articledetail
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/article/articledetail"/>
                        <horizontalrule
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/horizontalrule"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/text"/>
                        <articletags
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/articletags"/>
                    </tab_item_no_0>
                    <tab_item_no_1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <customtextlist
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/customtextlist"
                            headerTitle="Select a topic"
                            listType="icon"
                            textIsRich="[true,true,true,true,true,true,true]">
                            <customLists jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    external="false"
                                    icon="abt-icon abt-icon-lightbulb-o"
                                    link="#insights"
                                    text="Insights"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    external="false"
                                    icon="abt-icon abt-icon-cogs"
                                    link="#innovations"
                                    text="Innovations"/>
                                <item2
                                    jcr:primaryType="nt:unstructured"
                                    external="false"
                                    icon="abt-icon abt-icon-bolt"
                                    link="#impacts"
                                    text="Impacts"/>
                            </customLists>
                        </customtextlist>
                        <title
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Related Articles"
                            sling:resourceType="ardx/globals/components/content/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h3"/>
                        <searchresultswithfil
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/search/searchresultswithfilter"
                            defaultSortingOrder="best-match"
                            enableStaticContent="false"                            
                            resultSet="Total Match"
                            searchResultsPerPage="3"
                            searchType="articlesearch">
                            <responsivegrid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid">
                                <searchresultitemv2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="ardx/globals/components/search/searchresultitemV2"
                                    defaultSearchButtonLabel="\{articlesubtitle}"
                                    defaultSearchButtonLink="\{printableuri}"
                                    designType="listView"
                                    isVideoPopupEnable="false"
                                    productCompareLabel="true"
                                    searchButtonLabel="\{articlesubtitle}"
                                    searchButtonLink="\{sysprintableuri}"/>
                            </responsivegrid>
                            <searchFilters jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="jcrlanguage"
                                    fieldValue="CURRENT_PAGE_LANGUAGE"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="tags"
                                    fieldValue="CURRENT_PAGE_TAGS"/>
                                <item2
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="excludeUrl"
                                    fieldValue="CURRENT_PAGE_URL"/>
                                <item3
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="includePath"
                                    fieldValue="CURRENT_PAGE_PARENT_URL"/>										
                            </searchFilters>
                        </searchresultswithfil>
                    </tab_item_no_1>
                </columncontrol>
            </container>
        </root>
    </jcr:content>
</jcr:root>