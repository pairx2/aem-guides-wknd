<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/ardx/point-of-care/settings/wcm/templates/ardx-product-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="ardx/division/components/structure/genericpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/container/container"
                decorative="false"
                hideImgOnMobile="false"
                id="overview">
                <columncontrol_446203098
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="ardx/globals/components/content/columncontrol"
                    colCount="2"
                    containerDivRequired="false"
                    twoColumns="10,2">
                    <tab_item_no_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <breadcrumb
							cq:styleIds="[1675434023598]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/breadcrumb"
                            disableShadowing="true"
                            hideCurrent="true"
                            showHidden="false"
							id="breadcrumb-section"
                            startLevel="4"/>
                    </tab_item_no_0>
                    <tab_item_no_1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <link
							cq:styleIds="[1675434455659,1601294943958]"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/link"
                            action="_self"
                            external="false"
                            icon="abt-icon abt-icon-share-alt"
                            id="page-share"
                            redirectConfirm="true"/>
                        <embed
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/content/embed"
                            id="addThis-share"
                            iframe="&lt;script type=&quot;text/javascript&quot; src=&quot;//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-63d8b1dba25b82da&quot;>&lt;/script>"
                            type="iframe"/>
                    </tab_item_no_1>
                </columncontrol_446203098>
                <columncontrol
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="ardx/globals/components/content/columncontrol"
                    colCount="2"
                    containerDivRequired="false"
                    twoColumns="7,5">
                    <tab_item_no_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <title
                            jcr:primaryType="nt:unstructured"
							id="product-title"
                            sling:resourceType="ardx/globals/components/content/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h1"/>
                        <text
                            jcr:primaryType="nt:unstructured"
							id="product-description"
                            sling:resourceType="ardx/division/components/content/text"/>
                        <productsupportbutton
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/division/components/content/productsupportbutton"
                            supportButton="true"
                            technicalButton="true">
                        </productsupportbutton>
                    </tab_item_no_0>
                    <tab_item_no_1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/parsys">
                        <image
                            jcr:primaryType="nt:unstructured"
							id="product-image"
                            sling:resourceType="ardx/globals/components/content/image"/>
                    </tab_item_no_1>
                </columncontrol>
            </container>
            <stickymenu
                jcr:primaryType="nt:unstructured"
                sling:resourceType="ardx/globals/components/content/stickymenu"
                action="_self"
                linkStackType="siteselect"
                redirectConfirm="true"
                stackExternal="false">
                <stickyitems
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys">
                    <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="ardx/globals/components/container/container"
                        decorative="false"
                        hideImgOnMobile="false"
                        id="benefits">
                        <title
                            cq:styleIds="[1603352511660]"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Benefits"
                            sling:resourceType="ardx/globals/components/content/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h5"/>
                    </container>
                    <container_copy
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="ardx/globals/components/container/container"
                        decorative="false"
                        hideImgOnMobile="false"
                        id="helpfuldocuments">
                        <title
                            cq:styleIds="[1603352511660]"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Helpful Documents"
                            sling:resourceType="ardx/globals/components/content/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h5"/>
                        <accordion
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/container/accordion"
                            id="product-documents"
                            singleExpansion="false">
                            <item_1675169497471
                                cq:panelTitle="PRODUCT DOCUMENTS"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="ardx/division/components/content/text"/>
                        </accordion>
                        <accordion_1847064238
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/container/accordion"
                            id="product-specifications"
                            singleExpansion="false">
                            <item_1675169591499
                                cq:panelTitle="SPECIFICATIONS"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="ardx/division/components/content/text"/>
                        </accordion_1847064238>
                        <accordion_1824977042
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/container/accordion"
                            id="product-features"
                            singleExpansion="false">
                            <item_1675169652998
                                cq:panelTitle="FEATURES"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="ardx/division/components/content/text"/>
                        </accordion_1824977042>
                        <accordion_1046668338
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/container/accordion"
                            id="product-codes"
                            singleExpansion="false">
                            <item_1675169711663
                                cq:panelTitle="PRODUCT CODES"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="ardx/division/components/content/text"/>
                        </accordion_1046668338>
                    </container_copy>
                    <container_1965732442
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="ardx/globals/components/container/container"
                        decorative="false"
                        hideImgOnMobile="false"
                        id="relatedproducts">
                        <title
                            cq:styleIds="[1603352511660]"
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Related Products"
                            sling:resourceType="ardx/globals/components/content/title"
                            enableTooltip="false"
                            textIsRich="true"
                            type="h5"/>
                        <searchresultswithfil
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="ardx/globals/components/search/searchresultswithfilter"
                            defaultSortingOrder="best-match"
                            enableStaticContent="false"
							resultSet="Total Match"
							searchResultsPerPage="8"
                            searchType="sitesearch">
                            <responsivegrid
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/foundation/components/responsivegrid">
                                <searchresultitemv2
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="ardx/globals/components/search/searchresultitemV2"									
                                    defaultSearchButtonLabel="\{title}"
                                    defaultSearchButtonLink="\{printableuri}"
                                    designType="cardView"
                                    isVideoPopupEnable="false"
                                    productCompareLabel="true"
									imagePath="\{productimage}"
                                    searchButtonLabel="\{producttitle}"
                                    searchButtonLink="\{sysprintableuri}"/>
                            </responsivegrid>							
                            <searchFilters jcr:primaryType="nt:unstructured">
                                <item0
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="jcrlanguage"
                                    fieldValue="CURRENT_PAGE_LANGUAGE"/>
                                <item1
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="contentcategory"
                                    fieldValue="product"/>									
                                <item2
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="tags"
                                    fieldValue="CURRENT_PAGE_BRAND_TAGS"/>
                                <item3
                                    jcr:primaryType="nt:unstructured"
                                    fieldName="excludeUrl"
                                    fieldValue="CURRENT_PAGE_URL"/>										
                            </searchFilters>
                        </searchresultswithfil>
                    </container_1965732442>
                </stickyitems>
                <sites jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        external="false"
                        link="#overview"
                        text="Overview"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        external="false"
                        link="#benefits"
                        text="Benefits"/>
                    <item2
                        jcr:primaryType="nt:unstructured"
                        external="false"
                        link="#helpfuldocuments"
                        text="Helpful Documents"/>
                    <item3
                        jcr:primaryType="nt:unstructured"
                        external="false"
                        link="#relatedproducts"
                        text="Related Products"/>
                </sites>
            </stickymenu>
			<formcontainer
				jcr:primaryType="nt:unstructured"
				sling:resourceType="ardx/globals/components/form/formcontainer"
				cancel="false"
				failureMessage="FailureMessage"
				formEventTracking="false"
				formMode="formcontainer"
				formName="Signup"
				formType="/quality/api/public/products"
				id="form-product-documents"
				recaptcha="false"
				requestType="documentsearch"
				reset="false"
				submit="false"
				successMessage="SuccessMessage">
				<formcontainer-par
					jcr:primaryType="nt:unstructured"
					sling:resourceType="wcm/foundation/components/parsys">
					<hidden
						jcr:primaryType="nt:unstructured"
						sling:resourceType="abbott-platform/components/form/hidden/v1/hidden"
						dataRequest="body"
						name="action"
						source="static"
						value="documentsearch"/>
				</formcontainer-par>
			</formcontainer>			
        </root>
    </jcr:content>
</jcr:root>