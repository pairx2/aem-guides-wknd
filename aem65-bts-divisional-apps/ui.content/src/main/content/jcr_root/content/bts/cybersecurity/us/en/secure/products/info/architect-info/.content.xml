                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="bts/globals/components/content/text"
                                text="&lt;table cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;>&#xd;&#xa;&lt;tbody>&lt;tr>&lt;td valign=&quot;top&quot;>&lt;p>&lt;strong>&amp;nbsp;CLINICAL CHEMISTRY and/or IMMUNOASSAY SYSTEM&lt;/strong>&lt;/p>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xd;&#xa;"
                                textIsRich="true"/>
                            <image
                                jcr:created="{Date}2021-02-26T10:40:49.741Z"
                                jcr:createdBy="sanket.sinha@abbott.com"
                                jcr:lastModified="{Date}2021-02-26T10:40:58.381Z"
                                jcr:lastModifiedBy="sanket.sinha@abbott.com"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="bts/globals/components/content/image"
                                fileReference="/content/dam/bts/cybersecurity/us/en/images/products/Architect.jpg"/>
                            <text_1649972332
                                jcr:created="{Date}2021-02-22T03:38:00.936Z"
                                jcr:createdBy="ravikumar.sangubotla@abbott.com"
                                jcr:lastModified="{Date}2021-02-26T11:02:49.511Z"
                                jcr:lastModifiedBy="sanket.sinha@abbott.com"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="bts/globals/components/content/text"
                                text="&lt;p>&amp;nbsp;&lt;/p>&#xd;&#xa;&lt;table cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;>&#xd;&#xa;&lt;tbody>&lt;tr>&lt;td valign=&quot;top&quot;>&lt;p>&lt;strong>Product Category: &lt;/strong>&lt;em>Diagnostics&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&lt;strong>Product Description: &lt;/strong>&lt;em>A family of diagnostic instruments that perform chemiluminescent immunoassay and photometric clinical chemistry testing.&lt;/em>&lt;/p>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xd;&#xa;&lt;p>&lt;strong>Security Highlights:&lt;/strong>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>The system employs whitelisting to maintain the integrity of the system.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>The system operates in a kiosk mode which prevents customer access to the File System/OS.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>Upon starting up the computer, the system is automatically booted up into the ARCHITECT Software.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>The OS level password cannot be modified by the user. Auto-Run is disabled on devices.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>There are 4 supported user access levels: General, System Administrator, CSC (customer service center), and FSE (field service engineer). System functionality is restricted based on User Level.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>For systems that require additional security measures, the premium version of Architect software can be configured to utilize individual User Names and Passwords, and can be configured to automatically lock the system after an inactivity period to restrict access&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&lt;strong>&amp;nbsp;&lt;/strong>&lt;/p>&#xd;&#xa;&lt;p>&lt;strong>Product Type&lt;/strong>: &lt;em>Floor-Standing Instrument&lt;/em>&lt;/p>&#xd;&#xa;"
                                textIsRich="true"/>
