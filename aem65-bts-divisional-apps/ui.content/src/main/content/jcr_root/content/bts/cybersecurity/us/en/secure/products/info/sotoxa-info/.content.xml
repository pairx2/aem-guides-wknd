                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="bts/globals/components/content/text"
                                text="&lt;table cellspacing=&quot;0&quot; cellpadding=&quot;0&quot;>&#xd;&#xa;&lt;tbody>&lt;tr>&lt;td valign=&quot;top&quot;>&lt;p>&lt;strong>Product Category: &lt;/strong>&lt;em>Diagnostics&lt;/em>&lt;strong>&lt;/strong>&lt;/p>&#xd;&#xa;&lt;p>&lt;strong>Product Description: &lt;/strong>&lt;em>The SoToxa™ Oral Fluid Mobile Test System is designed for rapid drug screening and detection in oral fluid. With test results in minutes, this handheld analyzer is lightweight, compact, and easy to use—convenient for drug screening at the roadside in any type of vehicle or in the workplace.&lt;/em>&lt;/p>&#xd;&#xa;&lt;/td>&#xd;&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xd;&#xa;&lt;p>&lt;strong>Security Highlights:&lt;/strong>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>Passwords and the entered Operator IDs are encrypted in the mode of operation in which they are used&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;o&amp;nbsp;&amp;nbsp; &lt;em>The OS level password cannot be modified by the user. Auto-Run is disabled on devices.&lt;/em>&lt;/p>&#xd;&#xa;&lt;p>&lt;strong>Product Type&lt;/strong>: &lt;em>Handheld Instrument&lt;/em>&lt;/p>&#xd;&#xa;"
