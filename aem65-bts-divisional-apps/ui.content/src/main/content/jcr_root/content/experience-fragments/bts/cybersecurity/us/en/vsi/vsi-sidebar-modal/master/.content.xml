            <text
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T14:33:00.424+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiVulID"
                shortEnabled="false"
                text="&lt;h4>Vulnerability Identifier&lt;/h4>&#xd;&#xa;&lt;p>CVE number or other unique identifier for the vulnerability.&lt;/p>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T14:53:42.535+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiVulType"
                shortEnabled="false"
                text="&lt;h4>Vulnerability Type&lt;/h4>&#xd;&#xa;&lt;p>Is the vulnerability specific to an Abbott product (Abbott Vulnerability) or related to commonly used software an Abbott product utilizes (Third-Party Vulnerability).&lt;/p>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T14:54:34.726+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiInitRiskScore"
                shortEnabled="false"
                text="&lt;h4>CVSS v3 Base Score&lt;/h4>&#xd;&#xa;&lt;p>CVSS base score, if available. Abbott uses version 3.0 of the Common Vulnerability Scoring System framework.&lt;/p>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy_copy
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T14:55:22.754+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiVulSource"
                shortEnabled="false"
                text="&lt;h4>Source&lt;/h4>&#xd;&#xa;&lt;p>How Abbott product cybersecurity vulnerabilities are discovered.&lt;br>&#xd;&#xa;Sources include:&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>&lt;strong>Vulnerability Scans&lt;/strong> - Vulnerability scans performed on a product&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Penetration Tests&lt;/strong> - Penetration tests performed on a product&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>NVD&lt;/strong> - Vulnerabilities discovered in the NVD database for components that are resident in a product&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Threat Intel&lt;/strong> - Vulnerabilities discovered from various external sources such as H-ISAC, industry groups or other media sources&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Coordinated Disclosure&lt;/strong> - Vulnerabilities discovered by 3rd party researchers using Abbott's Coordinated Disclosure process&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy_copy_
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-09-22T06:11:08.116Z"
                jcr:lastModifiedBy="ravina.mahajan@abbott.com"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiVulStatus"
                shortEnabled="false"
                text="&lt;h4>Status&lt;/h4>&#xd;&#xa;&lt;p>Current status of the vulnerability (see “Response” for additional information):&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>&lt;strong>Affected&lt;/strong>&amp;nbsp;-&amp;nbsp;Actions are recommended to remediate or address the vulnerability.&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Not Affected&lt;/strong>&amp;nbsp;-&amp;nbsp;No remediation is required.&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Fixed -&amp;nbsp;&lt;/strong>The vulnerability was fixed in the subsequent product version.&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy_copy__1867863859
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T15:02:59.285+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiAssessDecision"
                shortEnabled="false"
                text="&lt;h4>Vulnerability Assessment Decision&lt;/h4>&#xd;&#xa;&lt;p>Abbott's assessment of the risk the vulnerability poses to a product after determining whether the vulnerability impacts clinical performance. Decisions include classifying the risk as:&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>&lt;strong>Controlled&lt;/strong> - When a vulnerability exists but there is sufficiently low (acceptable) residual risk of patient harm&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Uncontrolled&lt;/strong> - When a vulnerability exists that creates unacceptable residual risk of patient harm due to inadequate compensating controls and risk mitigations&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Critical and Controlled&lt;/strong> - When a vulnerability exists that would allow an individual or system without authorization or access rights or proper credentials to gain administrative-like access to a Product or Service&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>Critical and Uncontrolled&lt;/strong> - A critical vulnerability that is also defined as Uncontrolled&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy_copy__406748489
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-10-05T13:13:16.963Z"
                jcr:lastModifiedBy="ravina.mahajan@abbott.com"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiVulResponse"
                shortEnabled="false"
                text="&lt;h4>Response&lt;/h4>&#xd;&#xa;&lt;p>The response provides a brief justification description to support the vulnerability status. One of the following is displayed for each status:&lt;/p>&#xd;&#xa;&lt;ul>&#xd;&#xa;&lt;li>&lt;strong>&amp;quot;Not Affected&amp;quot; status&lt;/strong>&amp;nbsp;-&amp;nbsp;component not present, vulnerable code not present, vulnerable code not in execute path, vulnerable code cannot be controlled by adversary, or inline mitigations already exist.&lt;/li>&#xd;&#xa;&lt;li>&lt;strong>&amp;quot;Affected&amp;quot;/&amp;quot;Fixed&amp;quot; status&lt;/strong> - component removed, vendor provided patch applied, vulnerable code removed, vulnerable code removed from execute path, inline mitigation created, or no action taken (see Response Details).&lt;/li>&#xd;&#xa;&lt;/ul>&#xd;&#xa;"
                textIsRich="true"/>
            <text_copy_copy_copy__1738434240
                cq:styleIds="[1692547400467]"
                jcr:lastModified="{Date}2023-07-26T15:04:57.522+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="bts/globals/components/content/text"
                id="vsiResDetails"
                shortEnabled="false"
                text="&lt;h4>Response Details&lt;/h4>&#xd;&#xa;&lt;p>Additional information provided to support the Vulnerability Assessment Decision and Response&lt;/p>&#xd;&#xa;"
                textIsRich="true"/>
