{
	"identifier": "ADC_NAV_US",
	"httpMethod": "POST",
	"url": "$[env:COMMERCE_ENDPOINT_ADC_NAV_US]",
	"connectionTimeout": 5000,
	"socketTimeout": 5000,
	"maxHttpConnections": 20,
	"requestPoolTimeout": 2000,
	"acceptSelfSignedCertificates": false,
	"allowHttpProtocol": true,
	"httpHeaders": [
		"$[secret:COMMERCE_AUTH_HEADER;default= ]",
		"$[secret:ADC_NAV_US_MAGENTO_ORIGIN_SECRET;default= ]",
		"$[secret:NAV_ESL_APPLICATION_ACCESS_KEY;default= ]",
		"$[env:NAV_ESL_AUTHOR_APPLICATION_ID;default= ]"
	]
}
