{
	"identifier": "ARDX_COLIBRI_US",
	"httpMethod": "POST",
	"url": "$[env:COMMERCE_ENDPOINT_ARDX_COLIBRI_US]",
	"connectionTimeout": 5000,
	"socketTimeout": 5000,
	"maxHttpConnections": 20,
	"requestPoolTimeout": 2000,
	"acceptSelfSignedCertificates": false,
	"allowHttpProtocol": true,
	"httpHeaders": [
		"$[secret:COMMERCE_AUTH_HEADER;default= ]",
		"$[secret:ARDX_COLIBRI_MAGENTO_ORIGIN_SECRET;default= ]",
		"$[secret:ARDX_COLIBRI_ESL_APPLICATION_ACCESS_KEY;default= ]",
		"$[env:ARDX_COLIBRI_ESL_AUTHOR_APPLICATION_ID;default= ]"
	]
}
