<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Newsroom Banner component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Banner Links"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <images
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldDescription="Click on the Add button to add banner image"
                                        fieldLabel="Banner Images">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            name="./bannerDetails">
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <bannertype
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldDescription="Please choose the type of Banner"
                                                            fieldLabel="Banner Type"
                                                            name="./bannerType">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <videoStory
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Video Story"
                                                                    value="videoStory"/>
                                                                <imageStory
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Image Story"
                                                                    value="imageStory"/>
                                                            </items>
                                                        </bannertype>
                                                        <imagepath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Banner Image Path"
															rootPath="/content/dam/corp/abbott"
                                                            name="./imagePath"/>
                                                        <alttext
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Alt Text"
                                                            name="./altText"/>
                                                        <text
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                            class="rte"
                                                            fieldLabel="Banner Text"
                                                            name="./text"
                                                            removeSingleParagraphContainer="{Boolean}true"
                                                            useFixedInlineToolbar="{Boolean}true">
                                                            <rtePlugins
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="common/dialogs/standardrte/rtePlugins"/>
                                                            <uiSettings
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="common/dialogs/standardrte/uiSettings"/>
                                                        </text>
                                                        <colorType
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldDescription="This authored color will be visible for the end user."
                                                            fieldLabel="Banner Text Color"
                                                            name="./colorType"
                                                            selected="{Boolean}true"
                                                            value="colorPalette_Black">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <primaryblue
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Primary Blue"
                                                                    value="colorPalette_Abbott-Primary-Blue"/>
                                                                <black
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Black"
                                                                    value="colorPalette_Black"/>
                                                                <darkblue
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Dark Blue"
                                                                    value="colorPalette_Abbott-Dark-Blue"/>
                                                                <mediumblue
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Medium Blue"
                                                                    value="colorPalette_Medium-blue"/>
                                                                <blue
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Light Blue"
                                                                    value="colorPalette_Abbott-light-blue"/>
                                                                <mint
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Mint"
                                                                    value="colorPalette_Abbott-Mint"/>
                                                                <purple
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Purple"
                                                                    value="colorPalette_Abbott-purple"/>
                                                                <magenta
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Magenta"
                                                                    value="colorPalette_Abbott-Magenta"/>
                                                                <red
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Red"
                                                                    value="colorPalette_Abbott-red"/>
                                                                <orange
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Orange"
                                                                    value="colorPalette_Abbott-Orange"/>
                                                                <gold
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Gold"
                                                                    value="colorPalette_Abbott-gold"/>
                                                                <yellow
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Yellow"
                                                                    value="colorPalette_Yellow"/>
                                                                <mediumgreen
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Medium Green"
                                                                    value="colorPalette_Medium-Green"/>
                                                                <green
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Light Green"
                                                                    value="colorPalette_Light-Green"/>
                                                                <gray
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Charcoal"
                                                                    value="colorPalette_Abbott-charcole"/>
                                                                <darkgray
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Dark Gray"
                                                                    value="colorPalette_DarkGray"/>
                                                                <mediumgray
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Medium Gray"
                                                                    value="colorPalette_Medium-gray"/>
                                                                <lightgray
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Abbott Light Gray"
                                                                    value="colorPalette_LightGray"/>
                                                                <white
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="White"
                                                                    value="colorPalette_White"/>
                                                            </items>
                                                        </colorType>
                                                        <textposition
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Text Position"
                                                            name="./textPosition"
                                                            value="top">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <top
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Top"
                                                                    value="disp-top"/>
                                                                <bottom
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Bottom"
                                                                    value="disp-bottom"/>
                                                                <middle
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Middle"
                                                                    value="disp-middle"/>
                                                            </items>
                                                        </textposition>
                                                        <link
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Banner Image Hyperlink"
															rootPath="/content/corp/abbott"
                                                            name="./link"/>
                                                        <newtab
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            fieldDescription="Open in new window"
                                                            name="./newTab"
                                                            text="Open in new window"
                                                            uncheckedValue="_self"
                                                            value="_blank"/>
                                                        <ceros
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            defaultValue="false"
                                                            name="./cerosNeeded"
                                                            text="Enable checkbox for ceros integration"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true"/>
                                                        <cerosscript
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                            fieldLabel="Add Ceros Script here"
                                                            name="./cerosScript"/>
                                                        <typeofvideo
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Type of video"
                                                            name="./videoType">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <limelightvideo
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Limelight Video"
                                                                    value="limelightvideo"/>
                                                                <youtube
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Youtube Video"
                                                                    value="youtube"/>
                                                            </items>
                                                        </typeofvideo>
                                                        <playerid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Player ID"
                                                            name="./playerId"/>
                                                        <mediaid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Media ID"
                                                            name="./mediaId"/>
                                                        <videoscript
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                            fieldLabel="Youtube Video Script"
                                                            name="./videoScript"/>
                                                        <autoplay
                                                            granite:class="cq-dialog-checkbox-showhide"
                                                            jcr:mixinTypes="[]"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            defaultValue="false"
                                                            fieldDescription="Enable Check Box To Display  Data Dynamically"
                                                            name="./autoPlay"
                                                            text="Auto Play for lime light"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true"/>
                                                        <popup
                                                            granite:class="cq-dialog-checkbox-showhide"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            defaultValue="false"
                                                            fieldDescription="Enable Check Box To Display  Data Dynamically"
                                                            name="./hubModelPopup"
                                                            text="Enable modal popup"
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </images>
                                </items>
                            </column>
                        </items>
                    </tab1>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
