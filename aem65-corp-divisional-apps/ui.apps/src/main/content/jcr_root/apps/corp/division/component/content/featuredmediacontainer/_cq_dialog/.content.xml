<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Featured Media Container"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <viewselection
                        jcr:primaryType="nt:unstructured"
                        jcr:title="View Selection"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titlerequired
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Please specify if Title  is required"
                                                name="./titleRequired"
                                                text="Is Title Required?"
                                                uncheckedValue="false"
                                                value="true"/>
                                            <altText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter the Alternative Text for Image"
                                                    fieldLabel="Alternative Text for Image"
                                                    name="./altText"/>
                                            <ctalink
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="CTA link needed"
                                                fieldLabel="CTA link needed"
                                                name="./ctaLinkRequired"
                                                text="CTA link needed">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".curvetype-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <no
                                                        jcr:primaryType="nt:unstructured"
                                                        text="No"
                                                        value="no"/>
                                                    <yes
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Yes"
                                                        value="yes"/>
                                                </items>
                                            </ctalink>
                                            <ctalabelcontainer
                                                granite:class="hide curvetype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="yes"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ctaLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="CTA  Label"
                                                        fieldLabel="CTA Label"
                                                        name="./ctaLabel"/>
                                                    <ctalink
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="CTA Link"
                                                        fieldLabel="CTA Link"
                                                        name="./ctaLink"/>
                                                    <buttoncolor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldDescription="Color Theme"
                                                        fieldLabel="Color Theme"
                                                        name="./backgroundColor">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <white
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott White"
                                                                value="button-featured-media-variation--color-white"/>
                                                            <green
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott  Green"
                                                                value="button-featured-media-variation--color-green"/>
                                                            <magenta
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Magenta"
                                                                value="button-featured-media-variation--color-magenta"/>
                                                            <darkgray
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Gray"
                                                                value="button-featured-media-variation--color-dark-gray"/>
                                                            <yellow
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Yellow"
                                                                value="button-featured-media-variation--color-yellow"/>
                                                            <red
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Red"
                                                                value="button-featured-media-variation--color-red"/>
                                                            <lightgray
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Light Gray"
                                                                value="button-featured-media-variation--color-light-gray"/>
                                                            <charcoal
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Charcoal"
                                                                value="button-featured-media-variation--color-charcoal"/>
                                                            <mediumgreen
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Medium Green"
                                                                value="button-featured-media-variation--color-medium-green"/>
                                                            <purple
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott  Purple"
                                                                value="button-featured-media-variation--color-purple"/>
                                                            <gold
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Gold"
                                                                value="button-featured-media-variation--color-gold"/>
                                                            <darkblue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Dark Blue"
                                                                value="tileswithbackground-mob-bg-color--dark-blue"/>
                                                            <primaryblue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Primary Blue"
                                                                value="button-featured-media-variation--color-primary-blue"/>
                                                            <mint
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Mint"
                                                                value="button-featured-media-variation--color-mint"/>
                                                            <lightblue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott  Light Blue"
                                                                value="button-featured-media-variation--color-light-blue"/>
                                                            <mediumblue
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Abbott Medium Blue"
                                                                value="button-featured-media-variation--color-medium-blue"/>
                                                        </items>
                                                    </buttoncolor>
                                                    <ctalinknewtab
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        fieldDescription="Please select the checkbox to Open CTA Link in New Tab"
                                                        name="./newTab"
                                                        text="Open CTA Link in new tab?"
                                                        uncheckedValue="_self"
                                                        value="_blank"/>
                                                </items>
                                            </ctalabelcontainer>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </viewselection>
                    <backgroundImage
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Background Image"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <file
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="{Boolean}false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Image"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./fileReference"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./desktopImage"
                                                title="Upload Image Asset"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </backgroundImage>
                    <pressreleaseevents
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Press Release/Upcoming Event Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <bgcolorlastbox
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Need background color for last box"
                                                name="./calendarbgcolor"
                                                text="Need background color for last box"
                                                uncheckedValue="false"
                                                value="true"/>
                                            <eventdetails
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Press Release Event Details">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./eventDetails">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <title
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            emptyText="Title"
                                                            fieldLabel="Press Release/Upcoming Event Title"
                                                            name="./title"/>
                                                        <eventdate
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                            displayedFormat="MMM DD YYYY"
                                                            fieldLabel="Press Release/Upcoming Event Date"
                                                            name="./eventDate"
                                                            type="date"
                                                            typeHint="date"
                                                            valueFormat="MMM DD YYYY"/>
                                                        <links
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Investor Links">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./links">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <investorlabel
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Investor Label"
                                                                        name="./investorLabel"/>
                                                                    <investorlink
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldLabel="Investor Link"
                                                                        name="./investorLink"/>
                                                                    <ctalinknewtab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        fieldDescription="Please select the checkbox to Open CTA Link in New Tab"
                                                                        name="./newTab"
                                                                        text="Open CTA Link in new tab?"
                                                                        uncheckedValue="_self"
                                                                        value="_blank"/>
                                                                </items>
                                                            </field>
                                                        </links>
                                                    </items>
                                                </field>
                                            </eventdetails>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </pressreleaseevents>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
