<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="CTA Section"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <icon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                name="./icon"
                                                path="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"/>
                                            <iconColor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select the color of the icon to be displayed"
                                                fieldLabel="Alert Icon Color"
                                                name="./iconColor"
                                                required="true"
                                                value="light-blue">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <darkBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Dark Blue"
                                                        value="dark-blue"/>
                                                    <lightBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Blue"
                                                        value="light-blue"/>
                                                    <mediumBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Blue"
                                                        value="medium-blue"/>
                                                    <primaryBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Primary Blue"
                                                        value="primary-blue"/>
                                                    <charcoal
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Abbott Charcoal"
                                                        value="charcoal"/>
                                                    <gold
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Gold"
                                                        value="gold"/>
                                                    <lightGreen
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Green"
                                                        value="light-green"/>
                                                    <mediumGreen
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Green"
                                                        value="medium-green"/>
                                                    <darkGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Dark Gray"
                                                        value="dark-gray"/>
                                                    <mediumGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Gray"
                                                        value="medium-gray"/>
                                                    <lightGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Gray"
                                                        value="light-gray"/>
                                                    <magenta
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Magenta"
                                                        value="magenta"/>
                                                    <mint
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Mint"
                                                        value="mint"/>
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Orange"
                                                        value="orange"/>
                                                    <purple
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Purple"
                                                        value="purple"/>
                                                    <red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Red"
                                                        value="red"/>
                                                    <yellow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Yellow"
                                                        value="yellow"/>
                                                    <black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
                                                    <white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
                                                </items>
                                            </iconColor>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="cv/division/components/content/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Please enter Title"
                                                fieldLabel="Title"
                                                name="./title"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                            <subTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="cv/division/components/content/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Please enter Sub-title"
                                                fieldLabel="Sub Title"
                                                name="./subTitle"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="cv/division/components/content/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Please enter Description"
                                                fieldLabel="CTA Section Description"
                                                name="./description"
                                                useFixedInlineToolbar="{Boolean}true"/>
                                            <buttonCount
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Please enter number of button [0,2] required in the CTA Section"
                                                fieldLabel="CTA Button Count"
                                                max="{Long}3"
                                                min="{Long}0"
                                                name="./buttonCount"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
