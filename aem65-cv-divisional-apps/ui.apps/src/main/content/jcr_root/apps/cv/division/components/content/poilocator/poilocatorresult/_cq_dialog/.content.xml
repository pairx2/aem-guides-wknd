<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="POI Locator Result (CV)"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <poiresultItem jcr:primaryType="nt:unstructured">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured">
                                <items jcr:primaryType="nt:unstructured">
                                    <displayType
                                        jcr:primaryType="nt:unstructured"
                                        sling:orderBefore="searchResultText"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Select how the data should display"
                                        fieldLabel="Display Type"
                                        name="./displayType">
                                        <items jcr:primaryType="nt:unstructured">
                                            <oneColumn
                                                jcr:primaryType="nt:unstructured"
                                                text="One Column"
                                                value="oneColumn"/>
                                            <twoColumn
                                                jcr:primaryType="nt:unstructured"
                                                text="Two Column"
                                                value="twoColumn"/>
                                        </items>
                                    </displayType>
									<kmselection
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Display distance in kilometers"
                                        name="./kmSelection"
                                        text="Display Distance In Kilometers"
                                        uncheckedValue="false"
                                        value="kilometers"/>
                                </items>
                            </columns>
                        </items>
                    </poiresultItem>
                    <pagination
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Pagination"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <listLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="List Label"
                                                fieldLabel="List Label"
                                                name="./listLabel"/>
                                            <gridLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Grid Label"
                                                fieldLabel="Grid Label"
                                                name="./gridLabel"/>
                                            <printLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Print Label"
                                                fieldLabel="Print Label"
                                                name="./printLabel"/>
                                        </items>
                                    </well>
                                </items>
                            </columns>
                        </items>
                    </pagination>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
