<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Back to Top"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <button-type
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select button type"
                                                fieldLabel="Back to Top Button Type"
                                                name="./buttonType">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".backToTop-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Text only"
                                                        value="textOnly"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Icon Only"
                                                        value="iconOnly"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Text with Icon"
                                                        value="textWithIcon"/>
                                                </items>
                                            </button-type>
                                            <iconOnly-option
                                                granite:class="hide backToTop-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="iconOnly"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <icon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:orderBefore="id"
                                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                                fieldDescription="Please select Button icon"
                                                                fieldLabel="Button icon"
                                                                name="./icon"
                                                                path="/apps/abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </iconOnly-option>
                                            <text-only
                                                granite:class="hide backToTop-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="textOnly"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <onlyTextBtnTitle
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter button title"
                                                                fieldLabel="Button title"
                                                                name="./onlyTextBtnTitle"/>
                                                            <onlyicontextonlybtnmob
                                                                granite:class="cq-dialog-checkbox-showhide"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                fieldDescription="If checked, icon only for mobile view"
                                                                text="show only icon for mobile view"
                                                                name="./onlyIconTextOnlyBtnMob"
                                                                uncheckedValue="{Boolean}false"
                                                                value="{Boolean}true">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    cq-dialog-checkbox-showhide-target=".icon-picker-showhide-target"/>
                                                            </onlyicontextonlybtnmob>
                                                            <iconpickerContainer
                                                                granite:class="hide icon-picker-showhide-target"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <textOnlyIcon
                                                                        granite:class="abbott-icon-picker"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Please select button icon"
                                                                        fieldLabel="Button icon"
                                                                        name="./textOnlyIcon"/>
                                                                </items>
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    showhidetargetvalue="true"/>
                                                            </iconpickerContainer>
                                                        </items>
                                                    </well>
                                                </items>
                                            </text-only>
                                            <textWithIcon-option
                                                granite:class="hide backToTop-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="textWithIcon"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <title
                                                                granite:class="add-back-title"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter button title"
                                                                fieldLabel="Button title"
                                                                name="./title"/>

                                                            <onlyicon
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                fieldDescription="If checked, icon only for mobile view"
                                                                name="./isIconOnly"
                                                                text="show only icon for mobile view"
                                                                uncheckedValue="{Boolean}false"
                                                                value="{Boolean}true"/>

                                                            <iconWithText
                                                                granite:class="abbott-icon-picker"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please select button icon"
                                                                fieldLabel="Button icon"
                                                                name="./iconWithText"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </textWithIcon-option>
                                            <screen-size
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select screen&#xa;size"
                                                fieldLabel="Screen Size"
                                                name="./screenSize">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="2"
                                                        value="2"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="3"
                                                        value="3"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="4"
                                                        value="4"/>
                                                    <option4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="5"
                                                        value="5"/>
                                                    <option5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="6"
                                                        value="6"/>
                                                </items>
                                            </screen-size>
                                            <position
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select the button's position"
                                                fieldLabel="Button Position"
                                                name="./position">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Inherit from theme"
                                                        value="inheritedTheme"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Left"
                                                        value="left"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Right"
                                                        value="right"/>
                                                </items>
                                            </position>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
