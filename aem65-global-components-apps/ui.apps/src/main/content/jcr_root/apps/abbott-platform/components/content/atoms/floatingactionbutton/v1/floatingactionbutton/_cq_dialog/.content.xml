<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Floating Action Button"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <buttonproperties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Button Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <buttontype
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Button Type"
                                                name="./buttonType"
                                                required="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <url
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Go to URL"
                                                        value="url"/>
                                                    <anchor
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Go to Anchor"
                                                        value="anchor"/>
                                                    <video
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Play Limelight Video"
                                                        value="video"/>
                                                    <ytVideo
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Play YouTube Video"
                                                        value="ytvideo"/>
                                                    <clickToCall
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Click to Call"
                                                        value="clicktocall"/>
                                                    <asset
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Download Asset"
                                                        value="asset"/>
                                                    <popupmodal
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Open pop-up modal"
                                                        value="popupmodal"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".buttontype-showhide-target"/>
                                            </buttontype>
                                            <buttonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="The text to display on the button."
                                                fieldLabel="Button Text"
                                                name="./buttonText"/>
                                            <targetAction
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <path
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldDescription="For internal links, use selection dialog to locate Target URL page. For external links, enter full URL, starting with HTTPS "
                                                                fieldLabel="Target URL"
                                                                name="./urlLink"
                                                                rootPath="/content"/>
                                                            <checkbox
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                name="./targetNewWindow"
                                                                text="Open Target URL in New Window?"
                                                                uncheckedValue="{Boolean}false"
                                                                value="true"/>
                                                            <popup
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                fieldDescription="Displays redirect disclaimer message, when created in Experience Fragments and configured in Properties"
                                                                name="./redirectConfirm"
                                                                text="Confirmation Redirect popup?"
                                                                uncheckedValue="{Boolean}false"
                                                                value="true"/>
                                                        </items>
                                                    </well>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="url"/>
                                            </targetAction>
                                            <anchorName
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <anchorName
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Anchor Name"
                                                        name="./anchorName"
                                                        rootPath="/content"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="anchor"/>
                                            </anchorName>
                                            <asset
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <path
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Link to a content page, external URL or page anchor."
                                                        fieldLabel="Asset"
                                                        name="./assetLink"
                                                        rootPath="/content/dam"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="asset"/>
                                            </asset>
                                            <videoplayid
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <videoid
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter Limelight Video Player ID "
                                                        fieldLabel="Limelight Video Player ID"
                                                        name="./playerId"/>
                                                    <mediaid
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter Limelight Media ID"
                                                        fieldLabel="Limelight Media ID"
                                                        name="./mediaId"/>
                                                    <orgid
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="EnterLimelight Org ID"
                                                        fieldLabel="Limelight Org ID"
                                                        name="./orgId"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="video"/>
                                            </videoplayid>
                                            <ytvideoplay
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <videoId
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter video id for the video to be played using YouTube"
                                                        fieldLabel="YouTube Video ID"
                                                        name="./videoId"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="ytvideo"/>
                                            </ytvideoplay>
                                            <clickcal
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <phoneNumber
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Enter telephone number, using numbers only (no special characters)"
                                                        fieldLabel="Telephone Number"
                                                        name="./phoneNumber"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="clicktocall"/>
                                            </clickcal>
                                            <popupModal
                                                granite:class="buttontype-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <modalicon
                                                        granite:class="abbott-icon-picker"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Select modal icon to be displayed"
                                                        fieldLabel="Modal Icon"
                                                        name="./modalIcon"/>
                                                    <modaltitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldDescription="Modal title or descriptive text"
                                                        fieldLabel="Modal Title"
                                                        name="./modalTitle"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                                            <format
                                                                jcr:primaryType="nt:unstructured"
                                                                features="bold,italic"/>
                                                            <justify
                                                                jcr:primaryType="nt:unstructured"
                                                                features="-"/>
                                                            <links
                                                                jcr:primaryType="nt:unstructured"
                                                                features="modifylink,unlink"/>
                                                            <lists
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                            <misctools jcr:primaryType="nt:unstructured">
                                                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                                    <chars jcr:primaryType="nt:unstructured">
                                                                        <default_copyright
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;copy;"
                                                                            name="copyright"/>
                                                                        <default_euro
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;euro;"
                                                                            name="euro"/>
                                                                        <default_registered
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;reg;"
                                                                            name="registered"/>
                                                                        <default_trademark
                                                                            jcr:primaryType="nt:unstructured"
                                                                            entity="&amp;trade;"
                                                                            name="trademark"/>
                                                                    </chars>
                                                                </specialCharsConfig>
                                                            </misctools>
                                                            <paraformat
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*">
                                                                <formats jcr:primaryType="nt:unstructured">
                                                                    <default_p
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Paragraph"
                                                                        tag="p"/>
                                                                    <default_h1
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 1"
                                                                        tag="h1"/>
                                                                    <default_h2
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 2"
                                                                        tag="h2"/>
                                                                    <default_h3
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 3"
                                                                        tag="h3"/>
                                                                    <default_h4
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 4"
                                                                        tag="h4"/>
                                                                    <default_h5
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 5"
                                                                        tag="h5"/>
                                                                    <default_h6
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Heading 6"
                                                                        tag="h6"/>
                                                                    <default_blockquote
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Quote"
                                                                        tag="blockquote"/>
                                                                    <default_pre
                                                                        jcr:primaryType="nt:unstructured"
                                                                        description="Preformatted"
                                                                        tag="pre"/>
                                                                </formats>
                                                            </paraformat>
                                                            <table
                                                                jcr:primaryType="nt:unstructured"
                                                                features="-">
                                                                <hiddenHeaderConfig
                                                                    jcr:primaryType="nt:unstructured"
                                                                    hiddenHeaderClassName="cq-wcm-foundation-aria-visuallyhidden"
                                                                    hiddenHeaderEditingCSS="cq-RichText-hiddenHeader--editing"/>
                                                            </table>
                                                            <tracklinks
                                                                jcr:primaryType="nt:unstructured"
                                                                features="*"/>
                                                        </rtePlugins>
                                                        <uiSettings jcr:primaryType="nt:unstructured">
                                                            <cui jcr:primaryType="nt:unstructured">
                                                                <inline
                                                                    jcr:primaryType="nt:unstructured"
                                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
                                                                    <popovers jcr:primaryType="nt:unstructured">
                                                                        <justify
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                            ref="justify"/>
                                                                        <lists
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                                            ref="lists"/>
                                                                        <paraformat
                                                                            jcr:primaryType="nt:unstructured"
                                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                                            ref="paraformat"/>
                                                                    </popovers>
                                                                </inline>
                                                                <tableEditOptions
                                                                    jcr:primaryType="nt:unstructured"
                                                                    toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]"/>
                                                            </cui>
                                                        </uiSettings>
                                                    </modaltitle>
                                                    <modalurl
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/experience-fragments/editor/components/xffield"
                                                        emptyText="Add the path for Pop up's experience fragment"
                                                        fieldDescription="Select Experience Fragment path for modal content"
                                                        fieldLabel="Modal URL"
                                                        id="popupModal"
                                                        name="./modalUrl"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="popupmodal"/>
                                            </popupModal>
                                            <buttonstyle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Maximum one Full-width and one Right-aligned configuration per page"
                                                fieldLabel="Button Style"
                                                name="./buttonStyle"
                                                required="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fullwidth
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Full-width"
                                                        value="btn-full-width"/>
                                                    <rightaligned
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Right-aligned"
                                                        value="right-aligned"/>
                                                </items>
                                            </buttonstyle>
                                            <buttonicon
                                                granite:class="abbott-icon-picker"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Button icon is optional. Set icon position in Style Menu."
                                                fieldLabel="Button Icon"
                                                name="./buttonIcon"/>
                                            <arialabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Value of an aria-label attribute for the button. Useful if the visual button text is not very descriptive."
                                                fieldLabel="Aria Label"
                                                name="./ariaLabel"/>
                                            <hideondesktop
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./hideOnDesktop"
                                                text="Hide on Desktop"
                                                value="true"/>
                                            <hidebuttontext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check to make icon only button"
                                                name="./hideButtonText"
                                                text="Hide Button Text"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </buttonproperties>
                    <buttonstyles
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Button Styles"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <buttonColor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Button Color Theme"
                                                name="./buttonColor"
                                                value="btn-style-default">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <rounded
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Rounded"
                                                        value=""/>
                                                    <danger
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Danger"
                                                        value="a-floatingactionbutton__floating-button__danger"/>
                                                    <primaryTheme
                                                        jcr:primaryType="nt:unstructured"
                                                        text="PRIMARY"
                                                        value="a-floatingactionbutton__floating-button__primary-button"/>
                                                    <primaryv1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Primary v1"
                                                        value="a-floatingactionbutton__floating-button__primary-v1"/>
                                                    <primaryv2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Primary v2"
                                                        value="a-floatingactionbutton__floating-button__primary-v2"/>
                                                    <secondary
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Secondary"
                                                        value="a-floatingactionbutton__floating-button__secondary-button"/>
                                                    <secondaryv1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Secondary v1"
                                                        value="a-floatingactionbutton__floating-button__secondary-v1"/>
                                                    <teritary
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Tertiary"
                                                        value="a-floatingactionbutton__floating-button__tertiary-button"/>
                                                </items>
                                            </buttonColor>
                                            <buttonsize
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Button Size"
                                                name="./buttonSize"
                                                value="medium">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <small
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Small"
                                                        value="small"/>
                                                    <medium
                                                        jcr:primaryType="nt:unstructured"
                                                        text="MEDIUM"
                                                        value="medium"/>
                                                    <large
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Large"
                                                        value="large"/>
                                                </items>
                                            </buttonsize>
                                            <iconPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Icon position"
                                                name="./iconPosition"
                                                value="btn-style-default">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <none
                                                        jcr:primaryType="nt:unstructured"
                                                        text="NONE"
                                                        value="none"/>
                                                    <left
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Left"
                                                        value="left"/>
                                                    <right
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Right"
                                                        value="center"/>
                                                </items>
                                            </iconPosition>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </buttonstyles>
                   <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
