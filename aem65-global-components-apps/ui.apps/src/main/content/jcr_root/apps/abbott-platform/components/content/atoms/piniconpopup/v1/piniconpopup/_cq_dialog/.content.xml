<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Pin Icon Popup"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <piniconpopupitem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="POI Locator Result Items"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pinPopupDistanceLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Label Text for distance value"
                                                fieldLabel="Pop Up Distance Label Text"
                                                name="./pinPopupDistanceLabelText"/>
                                            <pinPopupAddressLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter Label Text for Address Field"
                                                fieldLabel="Pop Up Address Label Text"
                                                name="./pinPopupAddressLabelText"/>
                                            <pinPopupPhoneLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter text for phone label text"
                                                fieldLabel="Pop Up Phone Label Text"
                                                name="./pinPopupPhoneLabelText"/>
                                            <pinPopupGetDirectionsLinkText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter get direction link text"
                                                fieldLabel="Get Directions Link Text"
                                                name="./pinPopupGetDirectionsLinkText"/>
                                            <pinPopupDescriptionLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter description label text"
                                                fieldLabel="Description Label Text"
                                                name="./pinPopupDescriptionLabelText"/>
                                            <pinPopupOperatingHoursLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter operating hours label text"
                                                fieldLabel="Pop Up Operating Hours Label Text"
                                                name="./pinPopupOperatingHoursLabelText"/>
                                            <pinPopupEmailLabelText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Enter email label text"
                                                fieldLabel="Pop Up Email Label Text"
                                                name="./pinPopupEmailLabelText"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </piniconpopupitem>
                    <extrafields
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Additional Fields"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <overridePinIconPopItem
                                        granite:class="cq-dialog-checkbox-showhide"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Override the piniconpopitem properties"
                                        name="./overridePinIconPopItem"
                                        text="Override the piniconpopitem properties"
                                        uncheckedValue="false"
                                        value="true">
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            cq-dialog-checkbox-showhide-target=".show-hide-checkbox"/>
                                    </overridePinIconPopItem>
                                    <overridePinIconPopUpList
                                        granite:class="hide show-hide-checkbox"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./overridePinIconPopUpList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <pinIconPopUpKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="author the key from the ESL Response"
                                                    fieldLabel="PinIconPopUp Key"
                                                    name="./pinIconPopUpKey"/>
                                                <pinIconPopUpLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="PinIconPopUp Label"
                                                    fieldDescription="PinIconPopUp author the label"
                                                                                                        name="./pinIconPopUpLabel"/>
                                            </items>
                                        </field>
                                        <granite:data
                                            jcr:primaryType="nt:unstructured"
                                            showhidetargetvalue="true"/>
                                    </overridePinIconPopUpList>
                                </items>
                            </columns>
                        </items>
                    </extrafields>
                    <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
