<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Tabs">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <id
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="HTML ID attribute to apply to the component."
                                                fieldLabel="ID"
                                                name="./id"
                                                validation="html-unique-id-validator"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <tabview
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab View"
                        sling:orderBefore="tabitems"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <tabView
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select tab view"
                                                fieldLabel="Tab View"
                                                name="./tabView">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <horizontal
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Horizontal"
                                                        value="horizontal"/>
                                                    <vertical
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Vertical"
                                                        value="vertical"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".enable-contentsection-showhide-target"/>
                                            </tabView>
                                            <contentSectionTitle
                                                granite:class="hide enable-contentsection-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Content Section Title&#xa;"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                margin="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <columns
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        margin="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <title
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                        fieldDescription="Specify content section title."
                                                                        fieldLabel="Title"
                                                                        name="./contentSectionTitle"
                                                                        removeSingleParagraphContainer="{Boolean}true"
                                                                        useFixedInlineToolbar="{Boolean}true">
                                                                        <rtePlugins
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/rtePlugins"/>
                                                                        <uiSettings
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/uiSettings"/>
                                                                        <htmlRules
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceSuperType="/apps/abbott-platform/components/core/text/v1/text/htmlRules"/>
                                                                    </title>
                                                                </items>
                                                            </column>
                                                        </items>
                                                    </columns>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="vertical"/>
                                            </contentSectionTitle>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </tabview>
                    <icons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icons&#xa;"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <icons
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Tabs Icons">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                                    margin="{Boolean}true"
                                                    name="./icons"
                                                    variant="large">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <active
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Active Icon"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <activeicon
                                                                    granite:class="abbott-icon-picker"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Icon"
                                                                    name="./activeIcon"/>
                                                                <activecolor
                                                                    granite:hide="${!cqDesign.iconColorEnabled}"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                                    fieldLabel="Color"
                                                                    name="./activeColor"
                                                                    showDefaultColors="{Boolean}true"
                                                                    showProperties="{Boolean}false"
                                                                    showSwatches="{Boolean}true"/>
                                                            </items>
                                                        </active>
                                                        <inactive
                                                            jcr:primaryType="nt:unstructured"
                                                            jcr:title="Inactive Icon"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <inactiveicon
                                                                    granite:class="abbott-icon-picker"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Icon"
                                                                    name="./inactiveIcon"/>
                                                                <inactivecolor
                                                                    granite:hide="${!cqDesign.iconColorEnabled}"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                                    fieldLabel="Color"
                                                                    name="./inactiveColor"
                                                                    showDefaultColors="{Boolean}true"
                                                                    showProperties="{Boolean}false"
                                                                    showSwatches="{Boolean}true"/>
                                                            </items>
                                                        </inactive>
                                                    </items>
                                                </field>
                                            </icons>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </icons>
                    <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
