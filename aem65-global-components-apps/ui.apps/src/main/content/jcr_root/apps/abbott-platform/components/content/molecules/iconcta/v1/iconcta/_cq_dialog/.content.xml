<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    granite:class="coral-multifield"
    jcr:primaryType="nt:unstructured"
    jcr:title="Iconcta&#xa;"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="iconcta.clientlibs"
    validation="coral-multifield">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <iconctastyle
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icon CTA Style"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <ctatitle
                                                granite:hide="${cqDesign.titleHidden}"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceSuperType="abbott-platform/components/content/atoms/text/v1/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="CTA Title"
                                                        name="./title"
                                                        useFixedInlineToolbar="{Boolean}true"/>
                                                </items>
                                            </ctatitle>
											<cardalignment
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="By unchecking this box, cards will display in rows"
                                                name="./cardAlignment"
                                                text="Display cards as columns"
                                                uncheckedValue="{Boolean}false"
												checked="{Boolean}true"
                                                value="{Boolean}true"/>
											<rulecolor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Rule Color"
                                                name="./ruleColor"
                                                text="Rule color">
                                                <items jcr:primaryType="nt:unstructured">
													<white
                                                        jcr:primaryType="nt:unstructured"
                                                        text="White"
                                                        value="white"/>
													<black
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Black"
                                                        value="black"/>
													<primaryBlue
                                                        jcr:primaryType="nt:unstructured"
														selected="{Boolean}true"
                                                        text="ABBOTT PRIMARY BLUE"
                                                        value="primary-blue"/>
													<darkBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Dark Blue"
                                                        value="dark-blue"/>
													<mediumBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Blue"
                                                        value="medium-blue"/>
													<lightBlue
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Blue"
                                                        value="light-blue"/>
													<mint
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Mint"
                                                        value="mint"/>
													<purple
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Purple"
                                                        value="purple"/>
                                                    <red
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Red"
                                                        value="red"/>
													<orange
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Orange"
                                                        value="orange"/>
													<magenta
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Magenta"
                                                        value="magenta"/>
													<gold
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Gold"
                                                        value="gold"/>
													<yellow
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Yellow"
                                                        value="yellow"/>  
													<mediumGreen
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Green"
                                                        value="medium-green"/>
													<lightGreen
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Green"
                                                        value="light-green"/>
													<charcoal
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Charcoal"
                                                        value="charcoal"/>
													<darkGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Dark Gray"
                                                        value="dark-gray"/>
                                                    <mediumGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Medium Gray"
                                                        value="medium-gray"/>
													<lightGray
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Abbott Light Gray"
                                                        value="light-gray"/>
                                                </items>
                                            </rulecolor>
                                            <topmargin
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
												fieldDescription="Add Top margin of 48px for the component"
                                                name="./topMargin"
                                                text="Top Margin"
                                                uncheckedValue="{Boolean}false"
                                                value="true"/>
                                            <bottommargin
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
												fieldDescription="Add Bottom margin of 48px for the component"
                                                name="./bottomMargin"
                                                text="Bottom Margin"
                                                uncheckedValue="{Boolean}false"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </iconctastyle>
                    <iconctacontent
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icon CTA Content"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <cardconfiguration
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
								fieldDescription="Click on the + button to add Card configs.Maximum 3 cards."
                                fieldLabel="Card Configuration">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./multifield">
                                    <items jcr:primaryType="nt:unstructured">
                                        <chapter
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                            <items jcr:primaryType="nt:unstructured">
                                                <imagepath
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldDescription="Optimal image size 100x100"
                                                    fieldLabel="Image path"
                                                    id="geturl"
                                                    name="./urlImage"
                                                    rootPath="/content/dam"/>
                                                <checkbox
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    name="./isActive"
                                                    text="Display image in original aspect ratio"
                                                    uncheckedValue="{Boolean}false"
                                                    value="{Boolean}true"/>
                                                <decorative
                                                    granite:class="cq-dialog-checkbox-showhide-multifield"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    fieldDescription="Check if the image should be ignored by assistive technology and therefore does not require an alternative text. This applies to decorative images only."
                                                    name="./isDecorative"
                                                    text="Image is decorative"
                                                    uncheckedValue="false"
                                                    value="{Boolean}true">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-dialog-checkbox-showhide-target=".showhidetarget"/>
                                                </decorative>
                                                <alt
                                                    granite:class="showhidetarget"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <altText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                            fieldLabel="Alternative Text"
                                                            name="./altText"/>
                                                    </items>
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetValue=""/>
                                                </alt>
                                                <cardtitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceSuperType="abbott-platform/components/content/atoms/text/v1/text/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text"
                                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                    fieldLabel="Card Title"
                                                    name="./cardTitle"
                                                    useFixedInlineToolbar="{Boolean}true"/>
                                                <button-type
                                                    granite:class="cq-dialog-dropdown-showhide-multifield"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Button Type"
                                                    name="./buttonType">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        cq-dialog-dropdown-showhide-target=".button-type-showhide-target"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <nobutton
                                                            jcr:primaryType="nt:unstructured"
                                                            text="No Button"
                                                            value="nobutton"/>
														<openurl
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Open URL"
                                                            value="url"/>
														<anchorname
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Go to Anchor"
                                                            value="anchor"/>
														<downloadasset
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Download Asset"
                                                            value="asset"/>
                                                        <phonenumber
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Click to Call"
                                                            value="clicktocall"/>
                                                        <brightcovevideo
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Play Brightcove Video"
                                                            value="brightCove"/>
                                                        <limelightvideo
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Play Limelight Video"
                                                            value="limeLight"/>
                                                        <videourl
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Play Youtube Video"
                                                            value="videoURL"/>
                                                        <saveresult
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Save Results"
                                                                value="saveResults"/>
                                                        <email
                                                                jcr:primaryType="nt:unstructured"
                                                                text="Email"
                                                                value="email"/>
                                                    </items>
                                                </button-type>
                                                <url-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="url"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldDescription="For internal links, use selection dialog to locate Target URL page. For external links, enter full URL, starting with HTTPS "
                                                                    fieldLabel="Target URL"
                                                                    name="./url"
                                                                    rootPath="/content"/>
                                                                <checkbox
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                    name="./targetnewWindow"
                                                                    text="Open Target URL in New Window?"
                                                                    uncheckedValue="{Boolean}false"
                                                                    value="true"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </url-option>
                                                <video-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="videoURL"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <videoId
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Please provide URL for the video. Youtube requires structure: https://www.youtube.com/embed/(id_of_video)YouTube"
                                                                    fieldLabel="Video URL"
                                                                    name="./videoURL"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </video-option>
                                                <anchorname-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="anchor"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <anchorName
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Anchor Name"
                                                                    name="./anchorName"
                                                                    rootPath="/content"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </anchorname-option>
                                                <downloadasset-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="asset"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldDescription="Link to a content page, external URL or page anchor."
                                                                    fieldLabel="Asset"
                                                                    name="./downloadAsset"
                                                                    rootPath="/content/dam"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </downloadasset-option>
                                                <email-config
                                                        granite:class="button-type-showhide-target"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            showhidetargetvalue="email"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <emailSubject
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Please Enter Email Subject"
                                                                    fieldLabel="Email Subject"
                                                                    name="./emailSubject"/>
                                                                <emailTemplatePath
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldDescription="Please select Email Template Path"
                                                                    fieldLabel="Email Template Path"
                                                                    rootPath="/content/email-templates"
                                                                    name="./emailTemplatePath"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </email-config>
                                                <phonenumber-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="clicktocall"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <phoneNumber
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter phone number in the following format: 8885551212"
                                                                    fieldLabel="Telephone Number"
                                                                    name="./phoneNumber"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </phonenumber-option>
                                                <limelight-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="limeLight"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <well
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/well">
                                                            <items jcr:primaryType="nt:unstructured">
																<mediaid
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Enter Limelight Media ID"
                                                                    fieldLabel="Limelight Media ID"
                                                                    name="./medID"/>
                                                                <videoid
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Please enter player ID of Limelight"
                                                                    fieldLabel="Limelight Player ID"
                                                                    name="./vidID"/>
                                                                <orgid
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Please enter org ID of Limelight"
                                                                    fieldLabel="Limelight Org ID"
                                                                    name="./orID"/>
                                                            </items>
                                                        </well>
                                                    </items>
                                                </limelight-option>
                                                <brightcove-option
                                                    granite:class="button-type-showhide-target"
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <granite:data
                                                        jcr:primaryType="nt:unstructured"
                                                        showhidetargetvalue="brightCove"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <videoid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please enter video ID of Brightcove video"
                                                            fieldLabel="BrightCove Video Player ID"
                                                            name="./videoID"/>
                                                        <playerid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please enter player ID of Brightcove account"
                                                            fieldLabel="Bright Cove Player ID"
                                                            name="./playerID"/>
                                                        <accountid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please enter account ID of Brightcove account"
                                                            fieldLabel="BrightCove account ID"
                                                            name="./accountID"/>
                                                    </items>
                                                </brightcove-option>
                                                <text
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Button Text"
                                                    id="text"
                                                    name="./buttonText"/>
                                                <buttoncolortheme
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Button Color Theme"
                                                    name="./buttoncolorTheme"
                                                    text="Button Color theme">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <danger
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Danger"
                                                            value="button-bg--danger"/>
                                                        <primary
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="PRIMARY"
                                                            value="button-bg--primary"/>
                                                        <primaryv1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Primary v1"
                                                            value="button-bg--primary-v1"/>
                                                        <primaryv2
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Primary v2"
                                                            value="button-bg--primary-v2"/>
                                                        <secondary
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Secondary"
                                                            value="button-bg--secondary"/>
                                                        <secondaryv1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Secondary v1"
                                                            value="button-bg--secondary-v1"/>
                                                        <tertiary
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Tertiary"
                                                            value="button-bg--tertiary"/>
                                                    </items>
                                                </buttoncolortheme>
                                            </items>
                                        </chapter>
                                    </items>
                                </field>
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    max-item="{Long}3"
                                    min-item="{Long}1"/>
                            </cardconfiguration>
                        </items>
                    </iconctacontent>
                    <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>