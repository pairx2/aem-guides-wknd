<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Mega Menu"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Main"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <primary
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Primary Nav Options"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <enablePrimaryLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldDescription="Enable link options for Primary Navigation"
                                        name="./enablePrimaryLink"
                                        text="Enable Link Options for Primary Link"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <enablePrimarySeeMore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enablePrimarySeeMore"
                                        text="Enable See More"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <disableSecondary
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./disableSecondary"
                                        text="Disable Secondary"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </primary>
                            <secondary
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Secondary Nav Options"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <enableSecondarySubtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableSecondarySubtitle"
                                        text="Enable Subtitle"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <enableSecondaryImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableSecondaryImage"
                                        text="Enable Image"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <enableSecondarySeeMore
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableSecondarySeeMore"
                                        text="Enable See More"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <disableTertiary
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./disableTertairy"
                                        text="Disable Tertiary"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </secondary>
                            <tertiary
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Tertiary Nav Options"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <enableTertiarySubtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableTertiarySubtitle"
                                        text="Enable Subtitle"
                                        uncheckedValue="false"
                                        value="true"/>
                                    <enableTertiaryImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableTertiaryImage"
                                        text="Enable Image"
                                        uncheckedValue="false"
                                        value="true"/>
                                </items>
                            </tertiary>
                        </items>
                    </properties>
                    <styletab
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/include"
                        path="/mnt/overlay/cq/gui/components/authoring/dialog/style/tab_design/styletab"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
