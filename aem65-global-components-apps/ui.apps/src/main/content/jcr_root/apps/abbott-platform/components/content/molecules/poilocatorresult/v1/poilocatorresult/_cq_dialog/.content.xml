<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="POI Locator Result"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <poiresultItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="POI Locator Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <mapAlignment
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Map Alignment"
                                        name="./mapAlignment">
                                        <items jcr:primaryType="nt:unstructured">
                                            <vertical
                                                jcr:primaryType="nt:unstructured"
                                                text="Vertical"
                                                value="vertical"/>
                                            <horizontal
                                                jcr:primaryType="nt:unstructured"
                                                selected="{Boolean}true"
                                                text="Horizontal"
                                                value="horizontal"/>
                                        </items>
                                    </mapAlignment>
                                    <searchResultText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Search Result Text"
                                        fieldLabel="Search Result Text"
                                        name="./searchResultText"/>
                                    <noResultFoundText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter text for no result found data"
                                        fieldLabel="No Result Found Text"
                                        name="./noResultFoundText"/>
                                    <distanceLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Distance Label Text"
                                        fieldLabel="Distance Label Text"
                                        name="./distanceLabelText"/>
                                    <addressLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter Address Label Text"
                                        fieldLabel="Address Label Text"
                                        name="./addressLabelText"/>
                                    <telephoneLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter telephone label text"
                                        fieldLabel="TelePhone Number Label Text"
                                        name="./telephoneLabelText"/>
                                    <emailAddressLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter email address label text"
                                        fieldLabel="Email Address Label Text"
                                        name="./emailAddressLabelText"/>
                                    <getWebsiteIcon
                                        granite:class="abbott-icon-picker"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter icon image for get Website text"
                                        fieldLabel="Get Website Icon"
                                        name="./getWebsiteIcon"/>
                                    <websiteUrlLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter website Url label text"
                                        fieldLabel="Website Url Label Text"
                                        name="./websiteUrlLabelText"/>
                                    <getDirectionIcon
                                        granite:class="abbott-icon-picker"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Please enter icon image for get direction text"
                                        fieldLabel="Get Direction Icon"
                                        name="./getDirectionIcon"/>
                                    <getDirectionsLinkLabelText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Enter get direction link label text"
                                        fieldLabel="Get Directions Link Label Text"
                                        name="./getDirectionsLinkLabelText"/>
                                </items>
                            </columns>
                        </items>
                    </poiresultItem>
                    <extrafields
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Additional Fields"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                   <poiLocatorResult
                                        jcr:primaryType="nt:unstructured"
                                        fieldLabel="POI Locator Dynamic Properties"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./poiLocatorResultList">
                                            <items jcr:primaryType="nt:unstructured">
                                                <resultKey
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="author the key from the ESL Response"
                                                    fieldLabel="Result Key"
                                                    name="./resultKey"/>
                                                <resultLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Result Label"
                                                    fieldDescription="author the Result label"
                                                    name="./resultLabel"/>
                                            </items>
                                        </field>
                                    </poiLocatorResult>
                                </items>
                            </columns>
                        </items>
                    </extrafields>
                    <pagination
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Pagination"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <showPagination
                                                granite:class="cq-dialog-checkbox-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./showPagination"
                                                text="enable the checkbox to show the pagination and Result"
                                                uncheckedValue="false"
                                                value="{Boolean}true">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-checkbox-showhide-target=".show-hide-checkbox"/>
                                            </showPagination>
                                            <paginationProperties
                                                granite:class="hide show-hide-checkbox"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pageResult
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                        defaultValue="6"
                                                        fieldDescription="Default value per page result is 6"
                                                        fieldLabel="Number of Result per Page"
                                                        max="{Long}20"
                                                        min="{Long}1"
                                                        name="./pageResult"/>
                                                    <resultLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Page Result Label"
                                                        name="./resultLabel"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="true"/>
                                            </paginationProperties>
                                        </items>
                                    </well>
                                </items>
                            </columns>
                        </items>
                    </pagination>
                     <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
