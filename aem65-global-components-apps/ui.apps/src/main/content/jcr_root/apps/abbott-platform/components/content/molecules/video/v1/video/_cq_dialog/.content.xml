<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Video"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="core.wcm.components.image.v2.editor">
    <content
        granite:class="cmp-image__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <video-type
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select video&#xa;type"
                                                fieldLabel="Video Type"
                                                name="./videoType">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".video-type-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Brightcove"
                                                        value="brightCove"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Limelight"
                                                        value="limeLight"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="DAM Video"
                                                        value="dam"/>
                                                    <option4
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Video Link"
                                                        value="videoURL"/>
                                                    <option5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Wistia Video"
                                                        value="wistia"/>
                                                </items>
                                            </video-type>
                                            <brightcove-option
                                                granite:class="hide video-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="brightCove"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <account-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter account&#xa;ID of Brightcove&#xa;account"
                                                                fieldLabel="Account ID"
                                                                name="./accountID"
                                                                required="{Boolean}true"/>
                                                            <video-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter video ID&#xa;of Brightcove video"
                                                                fieldLabel="Video ID"
                                                                name="./videoID"
                                                                required="{Boolean}true"/>
                                                            <player-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter player&#xa;ID of Brightcove&#xa;account"
                                                                fieldLabel="Player ID"
                                                                name="./playerID"
                                                                required="{Boolean}true"/>
                                                            <brightcove-file
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                fieldLabel="Fallback Image"
                                                                fileNameParameter="./fileName2"
                                                                fileReferenceParameter="./fallBackImage"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./file2"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </brightcove-option>
                                            <limelight-option
                                                granite:class="hide video-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="limeLight"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <org-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter organisation&#xa;ID of Limelight&#xa;account"
                                                                fieldLabel="Organisation ID"
                                                                name="./orgID"/>
                                                            <video-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter Media ID&#xa;of Limelight video"
                                                                fieldLabel="Media ID"
                                                                name="./mediaID"
                                                                required="{Boolean}true"/>
                                                            <limelight-player-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter player&#xa;ID of Limelight"
                                                                fieldLabel="Limelight Player ID"
                                                                name="./limelightPlayerID"
                                                                required="{Boolean}true"/>
                                                            <limelight-file
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                fieldLabel="Fallback Image"
                                                                fileNameParameter="./fileName3"
                                                                fileReferenceParameter="./limeLightFallBackImage"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./file3"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </limelight-option>
                                            <dam-option
                                                granite:class="hide video-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="dam"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <dam-video
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldDescription="Please provide DAM&#xa;path for the video"
                                                                fieldLabel="DAM Video"
                                                                name="./damVideo"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam"/>
                                                            <dam-file
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                fieldLabel="Fallback Image"
                                                                fileNameParameter="./fileName4"
                                                                fileReferenceParameter="./damFallBackImage"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./file4"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </dam-option>
                                            <video-option
                                                granite:class="hide video-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="videoURL"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <video-iframe-title
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                class="hide video-type-hide-show"
                                                                fieldDescription="Please enter iframe&#xa;title"
                                                                fieldLabel="Video Iframe Title"
                                                                name="./videoIframeTitle"
                                                                showhidetargetvalue="brightCove"/>
                                                            <video-url
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                emptyText="https://www.youtube.com/embed/{id_of_video}"
                                                                fieldDescription="Please provide URL&#xa;for the video. YouTube requires structure: https://www.youtube.com/embed/{id_of_video}"
                                                                fieldLabel="Video URL"
                                                                name="./videoURL"
                                                                required="{Boolean}true"/>
                                                            <video-iframe-lang
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter iframe&#xa;language"
                                                                fieldLabel="Video Iframe Language"
                                                                name="./videoIframeLang"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </video-option>
                                            <wistia-option
                                                granite:class="hide video-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="wistia"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <video-id
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter the video id of the video that will display in this component."
                                                                fieldLabel="Wistia Video ID"
                                                                name="./wistiaVideoID"
                                                                required="{Boolean}true"/>
                                                            <video-document-number
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please provide the video document approval number for the video."
                                                                fieldLabel="Video Document Number"
                                                                name="./videoDocumentNumber"/>
                                                            <imagePath
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldDescription="This is the path to the image in the repository."
                                                                fieldLabel="Path to the image in the DAM."
                                                                name="./imagePath"
                                                                rootPath="/content/dam"
                                                                value="true"/>
                                                            <imagealttext
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="This text will display if the image selected earlier does not render on the html page."
                                                                fieldLabel="Alt Text"
                                                                name="./imageAltText"/>
                                                            <embed-option
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                fieldDescription="Please check the checkbox to play video in page"
                                                                name="./embedOption"
                                                                text="Play video in page"
                                                                value="true"/>
                                                            <wistia-file
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                fieldLabel="Fallback Image"
                                                                fileNameParameter="./fileName5"
                                                                fileReferenceParameter="./wistiaFallBackImage"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./file5"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </wistia-option>
                                            <video-caption
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please enter Video&#xa;Caption"
                                                fieldLabel="Video Caption"
                                                name="./videoCaption"/>
                                            <video-size
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select video&#xa;size"
                                                fieldLabel="Video Size"
                                                name="./videoSize">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <option1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Thumbnail"
                                                        value="a-video--thumbnail"/>
                                                    <option2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Small"
                                                        value="m-video--small"/>
                                                    <option3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Medium"
                                                        value="m-video--medium"/>
                                                    <option4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Large"
                                                        value="a-video--large"/>
                                                    <option5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Extra-Large"
                                                        value="a-video--extra-large"/>
                                                    <option6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Full-width"
                                                        value="m-video--large"/>
                                                </items>
                                            </video-size>
                                            <transcript
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Please enabled the video transcript checkbox"
                                                name="./videoTranScriptText"
                                                text="Video transcript"
                                                value="true"/>
                                            <player-type
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select player type"
                                                fieldLabel="Player Type"
                                                name="./playerType">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".player-type-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <embed
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Embed"
                                                        value="embed"/>
                                                    <modal
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Modal Pop up"
                                                        value="modal"/>
                                                </items>
                                            </player-type>
                                            <modalpopup-option
                                                granite:class="hide player-type-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="modal"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <file
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                                class="cq-droptarget"
                                                                fieldLabel="Image asset"
                                                                fileNameParameter="./fileName"
                                                                fileReferenceParameter="./fileReference"
                                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                                name="./file"/>
                                                            <decorative
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                checked="{Boolean}true"
                                                                fieldDescription="Check if the image should be ignored by assistive technology and therefore does not require an alternative text. This applies to decorative images only."
                                                                name="./isDecorative"
                                                                text="Image is decorative"
                                                                uncheckedValue="false"
                                                                value="{Boolean}true"/>
                                                            <alternativeGroup
                                                                granite:class="cmp-image__editor-alt"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <alt
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Textual alternative of the meaning or function of the image, for visually impaired readers."
                                                                        fieldLabel="Alternative Text"
                                                                        name="./alt"/>
                                                                    <altValueFromDAM
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        checked="${not empty cqDesign.altValueFromDAM ? cqDesign.altValueFromDAM : true}"
                                                                        fieldDescription="When checked, populate the image's alt attribute with the value of the dc:description metadata in DAM."
                                                                        name="./altValueFromDAM"
                                                                        text="Get alternative text from DAM"
                                                                        uncheckedValue="false"
                                                                        value="{Boolean}true"/>
                                                                </items>
                                                            </alternativeGroup>
                                                        </items>
                                                    </well>
                                                </items>
                                            </modalpopup-option>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <controls
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Video Controls"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <videoControlOn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}true"
                                                fieldDescription="Either autoplay or video controls on should be enabled to play the video."
                                                name="./videoControlOn"
                                                text="Video Control ON"
                                                uncheckedValue="{Boolean}true"
                                                value="false"/>
                                            <audioControlOn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}true"
                                                fieldDescription="Please enable the audio control on checkbox to play with audio."
                                                name="./audioControlOn"
                                                text="Audio Control ON"
                                                uncheckedValue="{Boolean}true"
                                                value="false"/>
                                            <loopOn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Please check to play video in loop"
                                                name="./loopOn"
                                                text="Play Video In Loop"
                                                uncheckedValue="{Boolean}false"
                                                value="true"/>
                                            <autoplay
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Please specify if&#xa;video should be&#xa;played by default in desktop view."
                                                name="./autoPlay"
                                                text="Enable Autoplay In Desktop View"
                                                value="true"/>
                                            <autoplayMobile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="If checked, video will be automatically played in mobile view"
                                                name="./autoplayMobileView"
                                                text="Enable Autoplay In Mobile View"
                                                uncheckedValue="{Boolean}false"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </controls>
                     <configItem
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ID/Class Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/include"
                                                path="/apps/abbott-platform/components/core/customconfig/v1/customconfig/cq:dialog/content/items/tabs/items/configItem/items/columns/items/column"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </configItem>
                    <styles
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Styles"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <captionPlacement
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select the values from the dropdown for the caption placement."
                                                fieldLabel="Caption Placement"
                                                name="./captionPlacement">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <bottom
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Below Video"
                                                        value="Bottom"/>
                                                    <top
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Above Video"
                                                        value="top"/>
                                                </items>
                                            </captionPlacement>
                                            <videoAlignment
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select the alignment of the video from the dropdown."
                                                fieldLabel="Video Alignment"
                                                name="./videoAlignment">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <left
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Left"
                                                        value="m-video--start"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Center"
                                                        value="m-video--center"/>
                                                    <right
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Right"
                                                        value="m-video--end"/>
                                                </items>
                                            </videoAlignment>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </styles>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>