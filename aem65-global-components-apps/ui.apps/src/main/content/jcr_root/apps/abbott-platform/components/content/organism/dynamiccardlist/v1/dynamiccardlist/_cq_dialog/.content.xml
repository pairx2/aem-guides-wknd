<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Dynamic Card List">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[items]">
                    <component
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <filePath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                emptyText="Link"
                                                fieldLabel="Path of the component"
                                                name="./filePath"
                                                rootPath="/content"/>
                                            <componentType
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Select Compnent Type"
                                                id="component type"
                                                name="./componentType">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <similarTags
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Related Products And Related Resources"
                                                        value="similartags"/>
                                                    <trending
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Trending Resources"
                                                        value="trending"/>
                                                    <pickup
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Pick Up where you left off"
                                                        value="pickUp"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".type-showhide-target"/>
                                            </componentType>
                                            <imageDefault
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Default Image"
                                                name="./imageDefault"
                                                rootPath="/content/dam"/>
                                            <logoImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                emptyText="Logo Image"
                                                fieldLabel="Logo Image"
                                                name="./logoImage"
                                                rootPath="/content/dam"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </component>
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Carousel Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideChildren="[text,iconpicker,action,urlContainer,modalContainer]">
                                            <variableWidth
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Variable Width between crds"
                                                name="./variableWidth"
                                                text="Variable Width"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <centerMode
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Center Mode"
                                                name="./centerMode"
                                                text="Center Mode"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            <arrows
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="id"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Side Arrows"
                                                name="./arrows"
                                                text="Arrows"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
