<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Fields"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-form-text__editDialog"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">                                          
                                            <fieldType
                                                jcr:primaryType="nt:unstructured"
												granite:class="cmp-form-text__types cq-dialog-tab-showhide"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select input type"
                                                fieldLabel="Constraint"
                                                name="./type"/>
                                            <rows
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Please enter number of Text lines"
                                                fieldLabel="Text Lines"
                                                min="{Long}1"
                                                name="./rows"
                                                value="2"/>
                                        
                                            <enableTooltip
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="If checked, tooltip is enabled"
                                                name="./enableTooltip"
                                                text="Enable Tooltip"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true"/>
                                            
                                             <id
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="HTML ID attribute to apply to the component."
                                                fieldLabel="ID"
                                                name="./id"/> 

                                       </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                    <about
                        jcr:primaryType="nt:unstructured"
                        jcr:title="About"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <helpMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="A hint to the user for what can be added in this field"
                                                fieldLabel="Help Message"
                                                name="./helpMessage"/>
                                            <placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="A placeholder for the users for what can be entered in the field "
                                                fieldLabel="Placeholder Message"
                                                name="./placeholder"/>
                                            <usePlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./usePlaceholder"
                                                text="Display help message as placeholders"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </about>
                    <icons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icons"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <field-icon
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Please select type of Icon"
                                                fieldLabel="Icon Image"
                                                id="fieldIcon"
                                                name="./fieldIcon"
                                                text="Input Field Icon">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".fields-option-showhide-target"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <no-icon
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="true"
                                                        text="No Icon"
                                                        value="no-icon"/>
                                                    <double-icon
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Double Icon"
                                                        value="double-icon"/>
                                                    <left-icon
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Left Icon"
                                                        value="left-icon"/>
                                                    <right-icon
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Right Icon"
                                                        value="right-icon"/>
                                                </items>
                                            </field-icon>
                                            <icon-right
                                                granite:class="hide fields-option-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="right-icon"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                        granite:class="coral-Heading coral-Heading--4"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                        level="{Long}4"
                                                        text="Right Icon"/>
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <iconRight
                                                                granite:class="abbott-icon-picker"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter path for right icon image"
                                                                name="./iconRight"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </icon-right>
                                            <icon-left
                                                granite:class="hide fields-option-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="left-icon"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                        granite:class="coral-Heading coral-Heading--4"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                        level="{Long}4"
                                                        text="Left Icon"/>
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <iconLeft
                                                                granite:class="abbott-icon-picker"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Please enter path for left icon image"
                                                                name="./iconLeft"/>
                                                        </items>
                                                    </well>
                                                </items>
                                            </icon-left>
                                            <double-icon
                                                granite:class="hide fields-option-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="double-icon"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <doubleright
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="About"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        margin="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <heading
                                                                granite:class="coral-Heading coral-Heading--4"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                                level="{Long}4"
                                                                text="Right Icon"/>
                                                            <well
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <doubleIconRight
                                                                        granite:class="abbott-icon-picker"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Please enter path for right icon image"
                                                                        name="./doubleIconRight"/>
                                                                </items>
                                                            </well>
                                                        </items>
                                                    </doubleright>
                                                    <doubleleft
                                                        jcr:primaryType="nt:unstructured"
                                                        jcr:title="About"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        margin="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <heading
                                                                granite:class="coral-Heading coral-Heading--4"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                                level="{Long}4"
                                                                text="Left Icon"/>
                                                            <well
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <iconLeft
                                                                        granite:class="abbott-icon-picker"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Please enter path for left icon image"
                                                                        name="./doubleIconLeft"/>
                                                                </items>
                                                            </well>
                                                        </items>
                                                    </doubleleft>
                                                </items>
                                            </double-icon>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </icons>
                    <constraints
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Constraints"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <validationerrormsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Error Message when validation Fails"
                                                fieldLabel="Constraint Message"
                                                name="./validationErrorMessage"/>
                                            <constraintMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:hideResource="{Boolean}true"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Message displayed as tooltip when submitting the form if the value does not validate the Type chosen"
                                                fieldLabel="Constraint Message new"
                                                name="./constraintMessage"/>
                                            <regexRequired
                                                granite:class="cq-dialog-checkbox-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Select if a regular expression pattern is required for the field"
                                                fieldLabel="Regular Expression Pattern Required?"
                                                name="./regexRequired"
                                                text="Regex Required"
                                                value="true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <show
                                                        jcr:primaryType="nt:unstructured"
                                                        text="true"
                                                        value="true"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-checkbox-showhide-target=".all-regex-fields"/>
                                            </regexRequired>
                                            <regex-fields
                                                granite:class="hide all-regex-fields"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <regexPattern
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Regular expression pattern if required for the input field"
                                                        fieldLabel="Regular Expression Pattern"
                                                        name="./regexPattern"/>
                                                    <regexErrorMessage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Error message displayed if the input field data does not match regex pattern "
                                                        fieldLabel="Regex Error Message"
                                                        name="./regexErrorMessage"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="true"/>
                                            </regex-fields>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </constraints>
                    <passwordProperties
                        granite:class="fields-password-policy"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Password Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                        	<confirmPassword
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Select this checkbox if this field needs confirm password validation"
                                                fieldLabel="Is Confirm Password field?"
                                                name="./confirmPassword"
                                                text="Is Confirm Password field?"
                                                value="true"/>
										    <passwordPolicyRequired
                                                granite:class="cq-dialog-checkbox-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Select this checkbox to attach password policy to password field."
                                                fieldLabel="Regular Expression Pattern Required?"
                                                name="./passwordPolicyRequired"
                                                text="Need Password Policy?"
                                                value="true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <show
                                                        jcr:primaryType="nt:unstructured"
                                                        text="true"
                                                        value="true"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-checkbox-showhide-target=".all-password-policy-fields"/>
                                            </passwordPolicyRequired>
											<password-policy
                                                granite:class="hide all-password-policy-fields"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<heading
															granite:class="coral-Heading coral-Heading--4"
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/heading"
															level="{Long}4"
															text="Password Policy"/>
														<well
															jcr:primaryType="nt:unstructured"
															sling:resourceType="granite/ui/components/coral/foundation/well">
															<items jcr:primaryType="nt:unstructured">
															   <policyType
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/select"
																	fieldLabel="Policy Type"
																	name="./policyType">
																	<items jcr:primaryType="nt:unstructured">
																		<complex
																			jcr:primaryType="nt:unstructured"
																			text="Complex"
																			value="complex"/>
																		<moderate
																			jcr:primaryType="nt:unstructured"
																			text="Moderate"
																			value="moderate"/>
																	</items>
																</policyType>
																<tooltip-icon
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/container">
																	<items jcr:primaryType="nt:unstructured">
																		<heading
																			granite:class="coral-Heading coral-Heading--4"
																			jcr:primaryType="nt:unstructured"
																			sling:resourceType="granite/ui/components/coral/foundation/heading"
																			level="{Long}4"
																			text="Tooltip Icon"/>
																		<well
																			jcr:primaryType="nt:unstructured"
																			sling:resourceType="granite/ui/components/coral/foundation/well">
																			<items jcr:primaryType="nt:unstructured">
																				<tooltipIcon
																					granite:class="abbott-icon-picker"
																					jcr:primaryType="nt:unstructured"
																					sling:resourceSuperType="abbott-platform/components/core/iconpicker/v1/iconpicker/cq:dialog/content/items/icon"
																					sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																					fieldDescription="Please Choose tooltip icon image"
																					name="./tooltipIcon"/>
																			</items>
																		</well>
																	</items>
																</tooltip-icon>															
																<tooltipLabel
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip label"
																	fieldLabel="Tooltip Label"
																	name="./tooltipLabel"/>
																<tooltipDescription
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Description"
																	fieldLabel="Tooltip Description"
																	name="./tooltipDescription"/>
																<minCharCount
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
																	fieldDescription="Password policy suggestion tooltip minimum required characters to set the password"
																	emptyText="e.g 8"
																	max="{Long}10"
																	min="{Long}1"
																	fieldLabel="Minimum Character Length"
																	name="./minCharCount"/>																
																<minCharLabel
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Minimum Character Required label"
																	fieldLabel="Minimum Character Label"
																	name="./minCharLabel"/>
																<minCharDescription
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Minimum Character description"
																	fieldLabel="Minimum Character Description"
																	name="./minCharDescription"/>
																<alphabetLabel
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Alphabet label"
																	fieldLabel="Alphabet Label"
																	name="./alphabetLabel"/>
																<alphabetDescription
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Albphabet description"
																	fieldLabel="Alphabet Description"
																	name="./alphabetDescription"/>
																<numericLabel
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Numeric label"
																	fieldLabel="Numeric Label"
																	name="./numericLabel"/>
																<numericDescription
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Numeric description"
																	fieldLabel="Numeric Description"
																	name="./numericDescription"/>
																<specialCharLabel
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Special Charcater label"
																	fieldLabel="Special Character Label"
																	name="./specialCharLabel"/>
																<specialCharDescription
																	jcr:primaryType="nt:unstructured"
																	sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
																	fieldDescription="Password policy suggestion tooltip Character description"
																	fieldLabel="Special Character Description"
																	name="./specialCharDescription"/>
                                                                <specialCharRegex
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Password policy Special Character regex"
                                                                        fieldLabel="Special Character Regex"
                                                                        name="./specialCharRegex"/>
															</items>
														</well>
													</items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="true"/>
                                            </password-policy>                                            											
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                        <granite:data
                            jcr:primaryType="nt:unstructured"
                            dep-value="password"/>
                    </passwordProperties>
                      <configItem
                            jcr:primaryType="nt:unstructured"
                            jcr:title="ID/Class Config"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <configTab
