<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Email Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration,cq.siteadmin.admin.properties]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v2"
    mode="edit"
    trackingFeature="core-components:page:v2">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:hideChildren="[pageProperties,seoProperties]">
                    <emailProperties
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Email Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <content
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Content Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <section
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <align
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                fieldDescription="Select Default option for content alignment on left for LTR Scripts and right for RTL Scripts"
                                                                fieldLabel="Choose the content alignment"
                                                                name="./align">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <option1
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Default"
                                                                        value="0"/>
                                                                    <option2
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Center"
                                                                        value="auto"/>
                                                                </items>
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    cq-msm-lockable="align"/>
                                                            </align>
                                                        </items>
                                                    </section>
                                                </items>
                                            </content>
                                            <asset
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Asset Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <section
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <assetPrefixDomain
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="Enter Asset Prefix Domain"
                                                                fieldLabel="Enter the Asset Prefix Domain"
                                                                name="./assetPrefixDomain">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    cq-msm-lockable="assetPrefixDomain"/>
                                                            </assetPrefixDomain>
                                                        </items>
                                                    </section>
                                                </items>
                                            </asset>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </emailProperties>
                    <siteProperties
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Site Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items
                                            jcr:primaryType="nt:unstructured"
                                            sling:hideChildren="[analytics,recaptchaProperties,experienceFragment,coveo,searchResults]">
                                            <general
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="General Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <section
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:hideChildren="[errorPages,clientlibsName]"/>
                                                    </section>
                                                </items>
                                            </general>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </siteProperties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
