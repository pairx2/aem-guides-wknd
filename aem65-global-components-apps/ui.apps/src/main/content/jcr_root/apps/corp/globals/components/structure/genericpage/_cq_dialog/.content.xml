<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Page"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.components.page.v2.editor,cq.wcm.msm.properties,granite.contexthub.configuration,cq.siteadmin.admin.properties]"
    mode="edit"
    trackingFeature="core-components:page:v1">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                granite:class="cq-siteadmin-admin-properties-tabs"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                size="L">
                <items jcr:primaryType="nt:unstructured">
                    <pageProperties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <general
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="General Properties"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <column
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <section
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <headingcolor
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        fieldLabel="Select Heading Color"
                                                                        name="./articlecolor">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <lightBlue
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Light Blue"
                                                                                value="color-light-blue"/>
                                                                            <mediumBlue
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Medium Blue"
                                                                                value="color-medium-blue"/>
                                                                            <mint
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Mint"
                                                                                value="color-mint"/>
                                                                            <mediumGreen
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Medium Green"
                                                                                value="color-medium-green"/>
                                                                            <gold
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Gold"
                                                                                value="color-gold"/>
                                                                            <red
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Red"
                                                                                value="color-red"/>
                                                                            <magenta
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Magenta"
                                                                                value="color-magenta"/>
                                                                            <purple
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Purple"
                                                                                value="color-purple"/>
                                                                            <gray
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Gray"
                                                                                value="color-gray"/>
                                                                            <mediumRed
                                                                                jcr:primaryType="nt:unstructured"
                                                                                text="Abbott Medium Red"
                                                                                value="color-medium-red"/>
                                                                        </items>
                                                                    </headingcolor>
                                                                </items>
                                                            </section>
                                                        </items>
                                                    </column>
                                                </items>
                                            </general>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </pageProperties>
                    <mostreadarticles
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Most Read Articles Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                        multiple="{Boolean}true"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <mostread
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Most Read Configuration"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <section
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <title
                                                                granite:class="checkParentNodesConfig"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Most Read Article Title"
                                                                name="./mostReadTitle">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    cq-msm-lockable="mostReadArticle"/>
                                                            </title>
                                                            <hovertitle
                                                                granite:class="checkParentNodesConfig"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Article Hover Title"
                                                                name="./mostReadHoverTitle">
                                                                <granite:data
                                                                    jcr:primaryType="nt:unstructured"
                                                                    cq-msm-lockable="hoverTitle"/>
                                                            </hovertitle>
                                                            <articlespath
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}true"
                                                                fieldDescription="Please author top 5 most read article paths"
                                                                fieldLabel="Most Read Articles Path">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./mostReadPaths">
                                                                    <layout
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                                        margin="{Boolean}false"/>
                                                                    <items jcr:primaryType="nt:unstructured">
                                                                        <path
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                            fieldLabel="Article Path"
                                                                            name="./articlePath"
                                                                            rootPath="/content/corp/abbott" />
                                                                    </items>
                                                                    <granite:data
                                                                        jcr:primaryType="nt:unstructured"
                                                                        cq-msm-lockable="mostReadPaths"/>
                                                                </field>
                                                            </articlespath>
                                                        </items>
                                                    </section>
                                                </items>
                                                <parentConfig
                                                    jcr:primaryType="nt:unstructured"
                                                    active="{Boolean}true"/>
                                            </mostread>
                                        </items>
                                    </accordion>
                                </items>
                            </column>
                        </items>
                    </mostreadarticles>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
