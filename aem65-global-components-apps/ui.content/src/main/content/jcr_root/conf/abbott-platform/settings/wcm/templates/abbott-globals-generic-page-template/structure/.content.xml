<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:template="/conf/abbott-platform/settings/wcm/templates/abbott-globals-generic-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="abbott-platform/components/structure/genericpage/v1/genericpage">
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
        <breadcrumb
            jcr:created="{Date}2016-09-20T23:26:30.627+02:00"
            jcr:createdBy="admin"
            jcr:lastModified="{Date}2016-09-20T23:26:47.347+02:00"
            jcr:lastModifiedBy="admin"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="abbott-platform/components/content/atoms/breadcrumb/v1/breadcrumb"
            absParent="5"
            disableShadowing="{Boolean}false"
            hideCurrent="false"
            relParent="1"
            startLevel="3"/>
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"
            editable="{Boolean}true"/>
    </jcr:content>
</jcr:root>
