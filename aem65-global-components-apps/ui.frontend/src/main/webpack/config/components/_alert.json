{
  "_alert": {
    "selector": ".m-alert",
    "title": "Alert",
    "groups": {
      "default-appearance": {
        "default-state":[
          {
            "label": "Title Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-title-font-size",
                "value": ""
              }
            }
          },

          {
            "label": "Title Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-title-line-height",
                "value": ""
              }
            }
          },

          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-title-color",
                "value": "--primary-color"
              }
            }
          },

          {
            "label": "Title Padding",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom":{
                  "key": "--alert-title-padding-bottom",
                  "value": "0.31rem"
                }
              }
            }
          },

          {
            "label": "Message Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-para-font-size",
                "value": ""
              }
            }
          },

          {
            "label": "Message Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-para-line-height",
                "value": ""
              }
            }
          },

          {
            "label": "Message Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-para-color",
                "value": "--gray-color"
              }
            }
          },

          {
            "label": "Alert Border Radius",
            "type": "TRBL",
            "config": {
              "isBorderRadius": true,
              "default": {
                "key": "--alert-border-radius",
                "value": "0.31rem"
              }
            }
          },

          {
            "label": "Alert Padding",
            "type": "TRBL",
            "config": {
              "default": {
                "key": "--alert-padding",
                "value": "0.75rem 1rem"
              }
            }
          },

          {
            "label": "Close Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-close-icon-color",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Close Icon Width",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-close-icon-width",
                "value": "15px"
              }
            }
          },

          {
            "label": "Close Icon Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--alert-close-icon-height",
                "value": "15px"
              }
            }
          },

          {
            "label": "Alert Icon Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "right": {
                  "key": "--alert-icon-margin-right",
                  "value": "1rem"
                }
              }
            }
          }
        ]
      },
      "info": {
        "default-state":[
          {
            "label": "Alert Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-info-icon-color",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-information-deemphasized-color",
                "value": "--info-10-color"
              }
            }
          }
        ]
      },
      "infoDefault": {
        "default-state":[
          {
            "label": "Alert Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-info-icon-color",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-information-default-color",
                "value": "--info-10-color"
              }
            }
          }
        ]
      },
      "warning": {
        "default-state":[
          {
            "label": "Alert Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-warning-icon-color",
                "value": "--warning-color"
              }
            }
          },
          {
            "label": "Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-warning-color",
                "value": "--warning-10-color"
              }
            }
          }
        ]
      },
      "success": {
        "default-state":[
          {
            "label": "Alert Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-success-icon-color",
                "value": "--success-color"
              }
            }
          },

          {
            "label": "Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-success-color",
                "value": "--success-10-color"
              }
            }
          }
        ]
      },
      "danger": {
        "default-state":[
          {
            "label": "Alert Icon Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-danger-icon-color",
                "value": "--danger-color"
              }
            }
          },

          {
            "label": "Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--alert-danger-color",
                "value": "--danger-10-color"
              }
            }
          }
        ]
      }
    }
  }
}
