{
  "_cta-section": {
    "selector": ".o-cta-section",
    "title": "CTA Section",
    "groups": {
      "default-appearance": {
        "default-state": [
          {
            "label": "Paragraph Text Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--cta-section-para-font-size",
                "value": ""
              }
            }
          },

          {
            "label": "Paragraph Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--cta-section-para-line-height",
                "value": ""
              }
            }
          },

          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--titlecolor-default",
                "value": "--black-color"
              }
            }
          },


          {
            "label": "Title Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom":{
                  "key": "--cta-section-title-margin-bottom",
                  "value": ""
                }
              }
            }
          },

          {
            "label": "Subtitle Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--subtitle-default",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Description Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--description-default",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Button Margin",
            "type": "TRBL",
            "config": {
              "isResponsive": true,
              "default": {
                  "key": "--cta-section-button-margin-r",
                  "value": ""
              },
              "mobile": {
                "key": "--cta-section-button-margin-mob",
                  "value": ""
              }
            }
          },

          {
            "label": "Button Wrap Padding",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "top":{
                  "key": "--cta-section-button-wrap-padding-t",
                  "value": ""
                }
              }
            }
          }
        ],

        "primary": [
          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--titlecolor-primary",
                "value": "--primary-color"
              }
            }
          },
          {
            "label": "Subtitle Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--subtitle-primary",
                "value": "--primary-color"
              }
            }
          },

          {
            "label": "Description Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--description-primary",
                "value": "--primary-color"
              }
            }
          }
        ],

        "reversed": [
          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--titlecolor-reversed",
                "value": "--white-color"
              }
            }
          },
          {
            "label": "Subtitle Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--subtitle-reversed",
                "value": "--white-color"
              }
            }
          },

          {
            "label": "Description Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--description-reversed",
                "value": "--white-color"
              }
            }
          }
        ],

        "alternate": [
          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--titlecolor-alternate",
                "value": "--gray-color"
              }
            }
          },
          {
            "label": "Subtitle Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--subtitle-alternate",
                "value": "--grahy-color"
              }
            }
          },

          {
            "label": "Description Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--cta-section--description-alternate",
                "value": "--gray-color"
              }
            }
          }
        ]
      }
    }
  }
}
