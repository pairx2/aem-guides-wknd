{
  "_custom-list": {
    "selector": ".m-custom-list",
    "title": "Custom List",
    "groups": {
      "default-appearance": {
        "default-state": [
          {
            "label": "Paragraph Text Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-para-font-size",
                "value": ""
              }
            }
          },
          {
            "label": "Paragraph Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-para-line-height",
                "value": ""
              }
            }
          },
          {
            "label": "Paragraph Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom": {
                  "key": "--custom-list-para-margin",
                  "value": ""
                }
              }
            }
          },
          {
            "label": "Heading Bullet Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-font-size",
                "value": ""
              }
            }
          },
          {
            "label": "Heading Bullet Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-line-height",
                "value": ""
              }
            }
          },
          {
            "label": "Heading Bullet Position",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "isResponsive": true,
              "default": {
                "top": {
                  "key": "--custom-list-bullet-position-top",
                  "value": ""
                }
              },
              "mobile": {
                "top": {
                  "key": "--custom-list-small-bullet-position-top",
                  "value": ""
                }
              }
            }
          },
          {
            "label": "Heading Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom": {
                  "key": "--custom-list-heading-bottom-margin",
                  "value": ""
                }
              }
            }
          },
          {
            "label": "Spacing (Left/Right)",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-padding-x",
                "value": ""
              }
            }
          },
          {
            "label": "Spacing (Top/Bottom)",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-padding-y",
                "value": ""
              }
            }
          },
          {
            "label": "Spacing Large (Left)",
            "type": "unit",
            "config": {
              "default": {
                "key": "--custom-list-padding-xl",
                "value": ""
              }
            }
          }
        ]
      },
      "dark": {
        "default-state": [
          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--custom-list-title-dark",
