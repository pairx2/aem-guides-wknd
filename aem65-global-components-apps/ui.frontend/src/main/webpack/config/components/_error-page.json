{
  "_error-page": {
    "selector": ".m-error-page",
    "title": "Error Page",
    "variables": [
      {
        "label": "Min Height",
        "type": "unit",
        "config": {
          "default": {
            "key": "--error-page-min-height",
            "value": "300px"
          }
        }
      },

      {
        "label": "Top",
        "type": "TRBL",
        "config": {
          "isMultiVariable": true,
          "isResponsive": true,
          "default": {
            "top": {
              "key": "--error-page-lg-top",
              "value": "80px"
            }
          },
          "tablet": {
            "top": {
              "key": "--error-page-md-top",
              "value": "50px"
            }
          },
          "mobile": {
            "top": {
              "key": "--error-page-sm-top",
              "value": "10px"
            }
          }
        }
      }
    ]
  }
}
