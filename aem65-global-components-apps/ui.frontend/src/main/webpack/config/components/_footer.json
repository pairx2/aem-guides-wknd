{
  "_footer": {
    "selector": ".o-footer",
    "title": "Footer",
    "variables": [
      {
        "label": "Background Fill Color",
        "type": "color",
        "config": {
          "default": {
            "key": "--footer-bg-color",
            "value": "--black-color"
          }
        }
      },

      {
        "label": "Footer Text Color",
        "type": "color",
        "config": {
          "default": {
            "key": "--footer-color",
            "value": "--white-color"
          }
        }
      },

      {
        "label": "Footer Padding",
        "type": "TRBL",
        "config": {
          "default": {
            "key": "--footer-padding",
            "value": ""
          }
        }
      },

      {
        "label": "Footer Links Container Padding",
        "type": "TRBL",
        "config": {
          "isMultiVariable": true,
          "default": {
            "bottom": {
              "key": "--footer-top-section-padding-bottom",
              "value": ""
            }
          }
        }
      },

      {
        "label": "Footer Link Block Padding",
        "type": "TRBL",
        "config": {
          "isMultiVariable": true,
          "isResponsive":true,
          "tablet": {
            "bottom": {
              "key": "--footer-link-wrapper-padding-bottom",
              "value": ""
            }
          },
          "mobile": {
            "bottom": {
              "key": "--footer-link-wrapper-padding-bottom1",
              "value": "3px"
            }
          }
        }
      },

      {
        "label": "Footer Link Margin",
        "type": "TRBL",
        "config": {
          "isMultiVariable": true,
          "default": {
            "bottom": {
              "key": "--footer-link-wrapper-margin-bottom",
              "value": ""
            }
          }
        }
      },

      {
        "label": "Footer Icons/Image Section Padding",
        "type": "TRBL",
        "config": {
          "default": {
            "key": "--footer-bootom-section-padding",
            "value": ""
          }
        }
      },

      {
        "label": "Footer Link Font Family",
        "type": "font",
        "config": {
          "default": {
            "key": "--footer-link-text-font-family",
            "value": ""
          }
        }
      },

      {
        "label": "Footer Link Font Size",
        "type": "unit",
        "config": {
          "default": {
            "key": "--footer-link-text-font-size",
            "value": ""
          }
        }
      },

      {
        "label": "Footer Link Line Height",
        "type": "unit",
        "config": {
          "default": {
            "key": "--footer-link-text-line-height",
            "value": ""
          }
        }
      },

      {
        "label": "Footer Link Color",
        "type": "color",
        "config": {
          "default": {
            "key": "--footer-link-text-color",
            "value": "--white-color"
          }
        }
      },

      {
        "label": "Copyrights Font Family",
        "type": "font",
        "config": {
          "default": {
            "key": "--footer-copyright-font-family",
            "value": ""
          }
        }
      },

      {
        "label": "Copyrights Font Size",
        "type": "unit",
        "config": {
          "default": {
            "key": "--footer-copyright-font-size",
            "value": ""
          }
        }
      },

      {
        "label": "Copyrights Line Height",
        "type": "unit",
        "config": {
          "default": {
            "key": "--footer-copyright-line-height",
            "value": ""
          }
        }
      },

      {
        "label": "Copyrights Color",
        "type": "color",
        "config": {
          "default": {
            "key": "--footer-copyright-color",
            "value": "--gray-darker-color"
          }
        }
      }





    ]
  }
}
