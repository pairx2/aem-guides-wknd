{
  "_information-section": {
    "selector": ".o-info-section",
    "title": "Information Section",
    "variables": [
      {
        "label": "Height",
        "type": "unit",
        "config": {
          "default": {
            "key": "--info-section-height",
            "value": "581px"
          }
        }
      },
      {
        "label": "Padding",
        "type": "TRBL",
        "config": {
          "isResponsive": true,
          "default": {
            "key": "--info-section-padding",
            "value": ""
          },
          "mobile": {
            "key": "--info-section-padding-mob",
            "value": ""
          }
        }
      },
      {
        "label": "Heading Padding",
        "type": "TRBL",
        "config": {
          "isMultiVariable": true,
          "isResponsive": true,
          "default": {
            "bottom": {
              "key": "--info-section-heading-padding-bottom",
              "value": ""
            }
          }
        }
      },
      {
        "label": "Background Fill Color",
        "type": "color",
        "config": {
          "default": {
            "key": "--info-section-light",
            "value": "--white-color"
          }
        }
      },
      {
        "label": "Border",
        "type": "border",
        "config": {
          "default": {
            "key": "--info-section-border",
            "value": "2px groove --white-color"
          }
        }
      },
      {
        "label": "Shadow",
        "type": "shadow",
        "config": {
          "default": {
            "key": "--info-section-shadow",
            "value": "--shadow-small"
          }
        }
      }
    ]
  }
}
