{
  "_link-stack": {
    "selector": ".m-link-stack",
    "title": "Link Stack",
    "groups": {
      "country-select": {
        "default-state": [
          {
            "label": "Icon Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--country-select-icon-font-size",
                "value": "1.2rem"
              }
            }
          },

          {
            "label": "Icon Width",
            "type": "unit",
            "config": {
              "default": {
                "key": "--country-select-icon-width",
                "value": "1.5rem"
              }
            }
          },

          {
            "label": "Icon Padding",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "isResponsive": true,
              "default": {
                "top": {
                  "key": "--country-select-icon-top",
                  "value": "0.9rem"
                },
                "left":{
                  "key": "--country-select-icon-left",
                  "value": "0.675rem"
                }

              },
              "tablet": {
                "top":{
                  "key": "--country-select-icon-tablet-top",
                  "value": "1.4rem"
                }
              }
            }
          },

          {
            "label": "Search Field Padding",
            "type": "TRBL",
            "config": {
              "default": {
                "key": "--country-select-input-search-padding",
                "value": "0.9375rem 0.4rem 0.8125rem 2.5rem"
              }
            }
          },

          {
            "label": "Search Field Max Width (Desktop)",
            "type": "unit",
            "config": {
              "default": {
                "key": "--country-select-input-search-desktop-max-width",
                "value": "189px"
              }
            }
          },

          {
            "label": "Search Language Padding",
            "type": "TRBL",
            "config": {
              "default": {
                "key": "--country-select-language-margin",
                "value": "0 0.3rem"
              }
            }
          },

          {
            "label": "Link Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-link-color",
                "value": "--black-color"
              }
            }
          },

          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-link-color-lg",
                "value": "--primary-color"
              }
            }
          }
        ],
        "hover": [
          {
            "label": "Link Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-link-color-hover",
                "value": "--primary-color"
              }
            }
          },

          {
            "label": "Link Background Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-link-bg-color-hover",
                "value": "--gray-light-color"
              }
            }
          }
        ]
      },
      "link-stack": {
        "default-state": [
          {
            "label": "Border Width",
            "type": "unit",
            "config": {
              "default": {
                "key": "--link-stack-border-width",
                "value": "4px"
              }
            }
          },

          {
            "label": "Icon Width",
            "type": "unit",
            "config": {
              "default": {
                "key": "--country-select-icon-width",
                "value": "1.5rem"
              }
            }
          },

          {
            "label": "Icon Font Size",
            "type": "unit",
            "config": {
              "isResponsive": true,
              "default": {
                "key": "--link-stack-icon-font-size",
                "value": "1.125rem"
              },
              "mobile": {
                "key": "--link-stack-icon-font-size-sm",
                "value": "1rem"
              }
            }
          },

          {
            "label": "Title Text Size",
            "type": "unit",
            "config": {
              "default": {
                "key": "--link-stack-h6-font-size",
                "value": "0.875rem"
              }
            }
          },

          {
            "label": "Title Line Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--link-stack-h6-line-height",
                "value": "1.25rem"
              }
            }
          },

          {
            "label": "Title Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-h6-color",
                "value": "--secondary-color"
              }
            }
          },

          {
            "label": "Link Color",
            "type": "color",
            "config": {
              "default": {
                "key": "--link-stack-color",
                "value": "--gray-lighter-color"
              }
            }
          },

          {
            "label": "Content Padding",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "isResponsive": true,
              "tablet": {
                "top": {
                  "key": "--link-stack-tablet-spacing-top",
                  "value": "0.5rem"
                }
              }
            }
          },

          {
            "label": "Search Field Padding",
            "type": "TRBL",
            "config": {
              "default": {
                "key": "--country-select-input-search-padding",
                "value": "0.9375rem 0.4rem 0.8125rem 2.5rem"
              }
            }
          },

          {
            "label": "Search Field Max Width (Desktop)",
            "type": "unit",
            "config": {
              "default": {
                "key": "--country-select-input-search-desktop-max-width",
                "value": "189px"
              }
            }
          },

          {
            "label": "Search Language Padding",
            "type": "TRBL",
            "config": {
              "default": {
                "key": "--country-select-language-margin",
                "value": "0 0.3rem"
              }
            }
          }
        ]
      }
    }
  }
}
