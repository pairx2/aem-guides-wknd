{
  "_section-panel": {
    "selector": ".o-section-panel",
    "title": "Section panel",
    "groups": {
      "default-appearance": {
        "default-state": [
          {
            "label": "Padding",
            "type": "TRBL",
            "config": {
              "isResponsive": true,
              "default": {
                "key": "--section-panel-padding",
                "value": "30px"
              },
              "mobile": {
                "key": "--section-panel-padding-mobile",
                "value": "15px"
              }
            }
          },
          {
            "label": "Max-Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--section-panel-max-height",
                "value": "746px"
              }
            }
          },
          {
            "label": "Heading Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom": {
                  "key": "--section-panel-heading-margin-bottom",
                  "value": "45px"
                }
              }
            }
          },
          {
            "label": "Paragraph Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom": {
                  "key": "--section-panel-para-margin-bottom",
                  "value": "24px"
                }
              }
            }
          },
          {
            "label": "Tiles Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "top": {
                  "key": "--section-panel-tiles-margin-top",
                  "value": "24px"
                },
                "bottom": {
                  "key": "--section-panel-title-margin-bottom",
                  "value": "24px"
                }
              }
            }
          },
          {
            "label": "Logo-Height",
            "type": "unit",
            "config": {
              "default": {
                "key": "--section-panel-logo-height",
                "value": "100px"
              }
            }
          },
          {
            "label": "Logo Margin",
            "type": "TRBL",
            "config": {
              "isMultiVariable": true,
              "default": {
                "bottom": {
                  "key": "--section-panel-logo-margin-bottom",
                  "value": "24px"
                }
              }
            }
          }
        ]
      },
      "dark": {
        "default-state": [
          {
            "label": "Background Fill Color (Dark)",
            "type": "color",
            "config": {
              "default": {
                "key": "--section-panel-dark",
                "value": "--gray-light-color"
              }
            }
          },
          {
            "label": "Border Color (Dark)",
            "type": "color",
            "config": {
              "default": {
                "key": "--section-panel-dark-border-color",
                "value": "--black-color"
              }
            }
          },
          {
            "label": "Text Color (Dark)",
            "type": "color",
            "config": {
              "default": {
                "key": "--section-panel-dark-color",
                "value": "--black-color"
              }
            }
          }
        ]
      },
      "light": {
        "default-state": [
          {
            "label": "Background Fill Color (Light)",
            "type": "color",
            "config": {
              "default": {
                "key": "--section-panel-light-color",
                "value": "--white-color"
              }
            }
          }
        ]
      }
    }
  }
}
