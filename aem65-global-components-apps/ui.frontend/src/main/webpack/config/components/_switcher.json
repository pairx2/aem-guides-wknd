{
    "_switcher": {
      "selector": ".a-switcher",
      "title": "Switcher",
      "groups": {
        "default-appearance": {
          "default-state": [
            {
              "label": "Label Text Style",
              "type": "unit",
              "config": {
                "default": {
                  "key": "--switcher-font-size",
                  "value": ""
                }
              }
            },

            {
              "label": "Line Height",
              "type": "unit",
              "config": {
                "default": {
                  "key": "--switcher-line-height",
                  "value": ""
                }
              }
            },

            {
                "label": "Switch Fill Color",
                "type": "color",
                "config": {
                  "default": {
                    "key": "--switcher-button-bg-color",
                    "value": ""
                  }
                }
            }
          ],

          "on":[
            {
                "label": "Track Fill Color",
                "type": "color",
                "config": {
                  "default": {
                    "key": "--switcher-on-bg-color",
                    "value": "--primary-color"
                  }
                }
            }

          ],

          "off":[
            {
                "label": "Track Fill Color",
                "type": "color",
                "config": {
                  "default": {
                    "key": "--switcher-off-bg-color",
                    "value": "--gray-darker-color"
                  }
                }
            }

          ]
        }
      }
    }
  }
  